     100:  00:9713                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:9713  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:9716  B7                  		or	a				; If priority has bit 7 on...
     103:  00:9717  F8                  		ret	m				; ...return
     104:  00:9718                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:9718  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:971B  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:971D  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:971F  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:9722  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:9723  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:9725  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:9727                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:9727  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:972A  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:972B  23                  		inc	hl				; Increment pointer
     116:  00:972C                      		; --- Check if there's new tone on stream ---
     117:  00:972C  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:972E  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:9730                      		; --- Extract new tone from stream ---
     120:  00:9730  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:9731  23                  		inc	hl				; Increment pointer
     122:  00:9732  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:9733  23                  		inc	hl				; Increment pointer
     124:  00:9734  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:9738                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:9738  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:973A  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:973C                      		; --- Extract new noise from stream ---
     129:  00:973C  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:973D  23                  		inc	hl				; Increment pointer
     131:  00:973E  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:9740  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:9742  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:9745                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:9745  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:9748                      		; --- Extract volume ---
     137:  00:9748  79                  		ld	a,c				; a:=Control byte
     138:  00:9749  E6 0F               		and	$0F				; lower nibble
     139:  00:974B                      
     140:  00:974B                      		IF ( AYFXRELATIVE == 1 )
     141:  00:974B                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:974B                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:974B                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:974B                    ~ 			ld	d,0				; d:=0
     145:  00:974B                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:974B                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:974B                    ~ 			or	a				; If relative volume is zero...
     148:  00:974B                    ~ 		ENDIF
     149:  00:974B                      
     150:  00:974B  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:974E  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:974F                      		; -------------------------------------
     153:  00:974F                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:974F                      		; -------------------------------------
     155:  00:974F                      		; --- Set noise channel ---
     156:  00:974F  CB 79               		bit	7,c				; If noise is off...
     157:  00:9751  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
