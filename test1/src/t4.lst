     187:  00:8EAF  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:8EB2  CB 49               		bit	1,c				; If tone is off...
     189:  00:8EB4  C0                  		ret	nz				; ...return
     190:  00:8EB5  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:8EB8  C9                  		ret					; Return
     192:  00:8EB9                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:8EB9  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:8EBB  0F                  		rrca					; Rotate right OR mask
     195:  00:8EBC                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:8EBC  CD C9 8E            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:8EBF  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:8EC2  CB 41               		bit	0,c				; If tone is off...
     199:  00:8EC4  C0                  		ret	nz				; ...return
     200:  00:8EC5  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:8EC8  C9                  		ret					; Return
     202:  00:8EC9                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:8EC9  4F                  		ld	c,a				; c:=OR mask
