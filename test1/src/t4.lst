     100:  00:9A53                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:9A53  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:9A56  B7                  		or	a				; If priority has bit 7 on...
     103:  00:9A57  F8                  		ret	m				; ...return
     104:  00:9A58                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:9A58  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:9A5B  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:9A5D  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:9A5F  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:9A62  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:9A63  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:9A65  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:9A67                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:9A67  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:9A6A  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:9A6B  23                  		inc	hl				; Increment pointer
     116:  00:9A6C                      		; --- Check if there's new tone on stream ---
     117:  00:9A6C  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:9A6E  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:9A70                      		; --- Extract new tone from stream ---
     120:  00:9A70  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:9A71  23                  		inc	hl				; Increment pointer
     122:  00:9A72  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:9A73  23                  		inc	hl				; Increment pointer
     124:  00:9A74  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:9A78                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:9A78  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:9A7A  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:9A7C                      		; --- Extract new noise from stream ---
     129:  00:9A7C  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:9A7D  23                  		inc	hl				; Increment pointer
     131:  00:9A7E  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:9A80  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:9A82  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:9A85                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:9A85  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:9A88                      		; --- Extract volume ---
     137:  00:9A88  79                  		ld	a,c				; a:=Control byte
     138:  00:9A89  E6 0F               		and	$0F				; lower nibble
     139:  00:9A8B                      
     140:  00:9A8B                      		IF ( AYFXRELATIVE == 1 )
     141:  00:9A8B                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:9A8B                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:9A8B                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:9A8B                    ~ 			ld	d,0				; d:=0
     145:  00:9A8B                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:9A8B                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:9A8B                    ~ 			or	a				; If relative volume is zero...
     148:  00:9A8B                    ~ 		ENDIF
     149:  00:9A8B                      
     150:  00:9A8B  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:9A8E  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:9A8F                      		; -------------------------------------
     153:  00:9A8F                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:9A8F                      		; -------------------------------------
     155:  00:9A8F                      		; --- Set noise channel ---
     156:  00:9A8F  CB 79               		bit	7,c				; If noise is off...
     157:  00:9A91  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:9A93  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:9A96  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:9A99                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:9A99  79                  		ld	a,c				; a:=Control byte
     162:  00:9A9A  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:9A9C  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:9A9E  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:9A9F                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:9A9F  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:9AA0  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:9AA1  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:9AA3                      		; --- Dump to correct channel ---
     170:  00:9AA3  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:9AA6  46                  		ld	b,[hl]				; Channel counter
     172:  00:9AA7                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:9AA7  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:9AA9                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:9AA9  CD D8 9A            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:9AAC  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:9AAF  CB 51               		bit	2,c				; If tone is off...
     178:  00:9AB1  C0                  		ret	nz				; ...return
     179:  00:9AB2  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:9AB5  C9                  		ret					; Return
     181:  00:9AB6                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:9AB6  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:9AB8  0F                  		rrca					; Rotate right OR mask
     184:  00:9AB9  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:9ABB                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:9ABB  CD D8 9A            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:9ABE  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:9AC1  CB 49               		bit	1,c				; If tone is off...
     189:  00:9AC3  C0                  		ret	nz				; ...return
     190:  00:9AC4  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:9AC7  C9                  		ret					; Return
     192:  00:9AC8                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:9AC8  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:9ACA  0F                  		rrca					; Rotate right OR mask
     195:  00:9ACB                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:9ACB  CD D8 9A            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:9ACE  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:9AD1  CB 41               		bit	0,c				; If tone is off...
     199:  00:9AD3  C0                  		ret	nz				; ...return
     200:  00:9AD4  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:9AD7  C9                  		ret					; Return
     202:  00:9AD8                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:9AD8  4F                  		ld	c,a				; c:=OR mask
     204:  00:9AD9  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:9ADC  A2                  		and	d				; AND mask
     206:  00:9ADD  B1                  		or	c				; OR mask
     207:  00:9ADE  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
     208:  00:9AE1  3A 49 F2            		ld	a,[ayFX_VOLUME]			; a:=ayFX volume value
     209:  00:9AE4  2A 46 F2            		ld	hl,[ayFX_TONE]			; ayFX tone value
     210:  00:9AE7  C9                  		ret					; Return
