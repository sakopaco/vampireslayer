     173.  00:51A8  87                       add    a
     174.  00:51A9  87                       add    a
     175.  00:51AA  87                       add    a           ;shift 3th to 6th bit
     176.  00:51AB  B0                       or    b           	;merge with previous
     177.  00:51AC  47                       ld    b,a
     178.  00:51AD                      
     179.  00:51AD  CD BA 51                 call  read_joy_0
     180.  00:51B0  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:51B2  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:51B3  47                       ld    b,a      	;save for later
     183.  00:51B4  F1                       pop    af      	; let's check on P
     184.  00:51B5                      
     185.  00:51B5  E6 20                    and    00100000b
     186.  00:51B7  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:51B8  B0                       or    b
