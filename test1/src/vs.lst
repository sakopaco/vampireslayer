      37:  00:8269  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:826C  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:826D  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:826E  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:8270                      		; The bank has less than 256 samples
