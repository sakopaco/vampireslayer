     176:  00:814B  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:814E  CB 51               		bit	2,c				; If tone is off...
     178:  00:8150  C0                  		ret	nz				; ...return
     179:  00:8151  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:8154  C9                  		ret					; Return
     181:  00:8155                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:8155  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:8157  0F                  		rrca					; Rotate right OR mask
     184:  00:8158  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:815A                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:815A  CD 77 81            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:815D  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:8160  CB 49               		bit	1,c				; If tone is off...
     189:  00:8162  C0                  		ret	nz				; ...return
     190:  00:8163  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:8166  C9                  		ret					; Return
     192:  00:8167                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:8167  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:8169  0F                  		rrca					; Rotate right OR mask
     195:  00:816A                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:816A  CD 77 81            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:816D  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:8170  CB 41               		bit	0,c				; If tone is off...
     199:  00:8172  C0                  		ret	nz				; ...return
     200:  00:8173  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:8176  C9                  		ret					; Return
     202:  00:8177                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:8177  4F                  		ld	c,a				; c:=OR mask
     204:  00:8178  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
