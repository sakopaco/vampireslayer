      82:  00:8F45  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:8F46  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:8F47  56                  		ld	d,[hl]				; de:=increment
      85:  00:8F48  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:8F49  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:8F4C  AF                  		xor	a				; a:=0 (no errors)
      88:  00:8F4D  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:8F4E  D1                  		pop	de				; Retrieve de from stack
      90:  00:8F4F  C1                  		pop	bc				; Retrieve bc from stack
