      29:  00:7E59                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7E59                      		; --- INPUT: A -> sound to be played ---
      31:  00:7E59                      		; ---        C -> sound priority     ---
      32:  00:7E59  C5                  		push	bc				; Store bc in stack
      33:  00:7E5A  D5                  		push	de				; Store de in stack
      34:  00:7E5B  E5                  		push	hl				; Store hl in stack
      35:  00:7E5C                      		; --- Check if the index is in the bank ---
      36:  00:7E5C  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7E5D  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7E60  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7E61  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7E62  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7E64                      		; The bank has less than 256 samples
