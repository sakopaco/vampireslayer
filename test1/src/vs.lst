     150.  00:52CA                      
     151.  00:52CA  E6 01                    and    #01    		;save L
     152.  00:52CC  CB 27                    sla    a
     153.  00:52CE  CB 27                    sla    a    		;now 00000L00
     154.  00:52D0  4F                       ld    c,a    		;save for later
     155.  00:52D1                      
     156.  00:52D1  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:52D2  CB 3F                    srl    a
     158.  00:52D4  E6 03                    and    00000011b
     159.  00:52D6                      						;we've got 000000DU on a
     160.  00:52D6  B1                       or    c    		;now we've got 00000LDU
     161.  00:52D7  4F                       ld    c,a    		;save for later
     162.  00:52D8                      
     163.  00:52D8  78                       ld    a,b
     164.  00:52D9  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:52DB  B1                       or    c
     166.  00:52DC                           ;finally    000TRLDU
     167.  00:52DC                      
     168.  00:52DC                           ;and    00000100   ;preserve just
     169.  00:52DC  47                       ld    b,a    		;save for later
     170.  00:52DD  CD 17 53                 call  check_4th_keyboard_row      ;check M & P
