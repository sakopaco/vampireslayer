     153.  00:8547                      ;;update_controllers_status
     154.  00:8547                      ;;=====================================================	
     155.  00:8547                      ;depends on the next rountine
     156.  00:8547                      ;  returns
     157.  00:8547                      ;  a - 0P2TRLDU
     158.  00:8547                      
     159.  00:8547                      update_controllers_status:
     160.  00:8547  CD 92 85                 call  check_8th_keyboard_row
     161.  00:854A                      [4]  rrc    a
     161.  00:854A  CB 0F CB 0F CB 0F CB 0F 
     162.  00:8552  47                       ld    b,a    		;save for later  xxxTRDUL
     163.  00:8553                      
     164.  00:8553  E6 01                    and    #01    		;save L
     165.  00:8555  CB 27                    sla    a
     166.  00:8557  CB 27                    sla    a    		;now 00000L00
     167.  00:8559  4F                       ld    c,a    		;save for later
     168.  00:855A                      
     169.  00:855A  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     170.  00:855B  CB 3F                    srl    a
     171.  00:855D  E6 03                    and    00000011b
     172.  00:855F                      						;we've got 000000DU on a
     173.  00:855F  B1                       or    c    		;now we've got 00000LDU
     174.  00:8560  4F                       ld    c,a    		;save for later
     175.  00:8561                      
     176.  00:8561  78                       ld    a,b
     177.  00:8562  E6 18                    and    00011000b 	;we've got 000TR000
     178.  00:8564  B1                       or    c
     179.  00:8565                           ;finally    000TRLDU
     180.  00:8565                      
     181.  00:8565                           ;and    00000100   ;preserve just
     182.  00:8565  47                       ld    b,a    		;save for later
     183.  00:8566  CD A0 85                 call  check_4th_keyboard_row      ;check M & P
     184.  00:8569  F5                       push  af
     185.  00:856A  E6 04                    and    00000100b	;filter just M
     186.  00:856C  87                       add    a
     187.  00:856D  87                       add    a
     188.  00:856E  87                       add    a           ;shift 3th to 6th bit
     189.  00:856F  B0                       or    b           	;merge with previous
     190.  00:8570  47                       ld    b,a
     191.  00:8571                      
     192.  00:8571  CD 7E 85                 call  read_joy_0
     193.  00:8574  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     194.  00:8576  B0                       or    b      		;added 000TRLDU from keyboard
     195.  00:8577  47                       ld    b,a      	;save for later
     196.  00:8578  F1                       pop    af      	; let's check on P
     197.  00:8579                      
     198.  00:8579  E6 20                    and    00100000b
     199.  00:857B  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     200.  00:857C  B0                       or    b
     201.  00:857D                      
     202.  00:857D  C9                       ret
     203.  00:857E                      
     204.  00:857E                      
