      17:  00:95FD                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:95FD                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:95FD  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:9600  3E 01               		ld	a,1				; a:=0
      21:  00:9602  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:9605  3C                  		inc	a				; Starting channel (=1)
      23:  00:9606  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:9609                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:9609  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:960B  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:960E  C9                  		ret					; Return
