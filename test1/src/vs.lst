     140.  00:5125                      ;;update_controllers_status
     141.  00:5125                      ;;=====================================================	
     142.  00:5125                      ;depends on the next rountine
     143.  00:5125                      ;  returns
     144.  00:5125                      ;  a - 0P2TRLDU
