     172.  00:52E1  E6 04                    and    00000100b	;filter just M
     173.  00:52E3  87                       add    a
     174.  00:52E4  87                       add    a
     175.  00:52E5  87                       add    a           ;shift 3th to 6th bit
     176.  00:52E6  B0                       or    b           	;merge with previous
     177.  00:52E7  47                       ld    b,a
     178.  00:52E8                      
     179.  00:52E8  CD F5 52                 call  read_joy_0
     180.  00:52EB  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:52ED  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:52EE  47                       ld    b,a      	;save for later
     183.  00:52EF  F1                       pop    af      	; let's check on P
     184.  00:52F0                      
     185.  00:52F0  E6 20                    and    00100000b
     186.  00:52F2  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:52F3  B0                       or    b
