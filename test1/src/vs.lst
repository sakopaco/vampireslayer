      56:  00:7C40  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7C43                      
      58:  00:7C43                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7C43                    ~ 			; --- Volume adjust using PT3 volume table ---
