      23:  00:7DC3  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7DC6                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7DC6  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7DC8  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7DCB  C9                  		ret					; Return
