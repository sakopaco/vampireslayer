      29:  00:9232                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:9232                      		; --- INPUT: A -> sound to be played ---
      31:  00:9232                      		; ---        C -> sound priority     ---
      32:  00:9232  C5                  		push	bc				; Store bc in stack
      33:  00:9233  D5                  		push	de				; Store de in stack
      34:  00:9234  E5                  		push	hl				; Store hl in stack
      35:  00:9235                      		; --- Check if the index is in the bank ---
      36:  00:9235  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:9236  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:9239  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:923A  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:923B  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:923D                      		; The bank has less than 256 samples
