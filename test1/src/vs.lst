      17:  00:75B4                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:75B4                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:75B4  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
