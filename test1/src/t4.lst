     169.  00:9061                      
     170.  00:9061                           ;and    00000100   ;preserve just
     171.  00:9061  47                       ld    b,a    		;save for later
     172.  00:9062  CD 9C 90                 call  check_4th_keyboard_row      ;check M & P
