      17:  00:908F                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:908F                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:908F  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
