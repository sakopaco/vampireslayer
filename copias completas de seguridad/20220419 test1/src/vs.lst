     140.  00:4F86                      ;;update_controllers_status
     141.  00:4F86                      ;;=====================================================	
     142.  00:4F86                      ;depends on the next rountine
     143.  00:4F86                      ;  returns
     144.  00:4F86                      ;  a - 0P2TRLDU
     145.  00:4F86                      
     146.  00:4F86                      update_controllers_status:
     147.  00:4F86  CD D1 4F                 call  check_8th_keyboard_row
     148.  00:4F89                      [4]  rrc    a
     148.  00:4F89  CB 0F CB 0F CB 0F CB 0F 
     149.  00:4F91  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:4F92                      
     151.  00:4F92  E6 01                    and    #01    		;save L
     152.  00:4F94  CB 27                    sla    a
     153.  00:4F96  CB 27                    sla    a    		;now 00000L00
     154.  00:4F98  4F                       ld    c,a    		;save for later
     155.  00:4F99                      
     156.  00:4F99  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:4F9A  CB 3F                    srl    a
     158.  00:4F9C  E6 03                    and    00000011b
     159.  00:4F9E                      						;we've got 000000DU on a
     160.  00:4F9E  B1                       or    c    		;now we've got 00000LDU
     161.  00:4F9F  4F                       ld    c,a    		;save for later
     162.  00:4FA0                      
     163.  00:4FA0  78                       ld    a,b
     164.  00:4FA1  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:4FA3  B1                       or    c
     166.  00:4FA4                           ;finally    000TRLDU
     167.  00:4FA4                      
     168.  00:4FA4                           ;and    00000100   ;preserve just
     169.  00:4FA4  47                       ld    b,a    		;save for later
     170.  00:4FA5  CD DF 4F                 call  check_4th_keyboard_row      ;check M & P
     171.  00:4FA8  F5                       push  af
     172.  00:4FA9  E6 04                    and    00000100b	;filter just M
     173.  00:4FAB  87                       add    a
     174.  00:4FAC  87                       add    a
     175.  00:4FAD  87                       add    a           ;shift 3th to 6th bit
     176.  00:4FAE  B0                       or    b           	;merge with previous
     177.  00:4FAF  47                       ld    b,a
     178.  00:4FB0                      
     179.  00:4FB0  CD BD 4F                 call  read_joy_0
     180.  00:4FB3  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:4FB5  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:4FB6  47                       ld    b,a      	;save for later
     183.  00:4FB7  F1                       pop    af      	; let's check on P
     184.  00:4FB8                      
     185.  00:4FB8  E6 20                    and    00100000b
     186.  00:4FBA  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:4FBB  B0                       or    b
     188.  00:4FBC                      
     189.  00:4FBC  C9                       ret
