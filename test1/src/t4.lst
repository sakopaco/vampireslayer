     212.  00:8585  E6 BF                      and    10111111b        ;joy0
     213.  00:8587                      
     214.  00:8587  D3 A1                      out    [PSG_W],a        ;set joy0
     215.  00:8589                      
     216.  00:8589  3E 0E                      ld    a,14
     217.  00:858B  D3 A0                      out    [PSG_A],a
     218.  00:858D  DB A2                      in    a,[PSG_R]
