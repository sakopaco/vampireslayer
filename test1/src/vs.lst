      83:  00:7E84  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:7E85  56                  		ld	d,[hl]				; de:=increment
      85:  00:7E86  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
