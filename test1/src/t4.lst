     100:  00:8F51                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:8F51  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:8F54  B7                  		or	a				; If priority has bit 7 on...
     103:  00:8F55  F8                  		ret	m				; ...return
     104:  00:8F56                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:8F56  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:8F59  E6 01               		and	1				; If bit0=0 (fixed channel)...
