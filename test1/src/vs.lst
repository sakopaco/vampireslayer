     149.  00:5093  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:5094                      
     151.  00:5094  E6 01                    and    #01    		;save L
     152.  00:5096  CB 27                    sla    a
     153.  00:5098  CB 27                    sla    a    		;now 00000L00
     154.  00:509A  4F                       ld    c,a    		;save for later
     155.  00:509B                      
     156.  00:509B  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:509C  CB 3F                    srl    a
     158.  00:509E  E6 03                    and    00000011b
     159.  00:50A0                      						;we've got 000000DU on a
     160.  00:50A0  B1                       or    c    		;now we've got 00000LDU
     161.  00:50A1  4F                       ld    c,a    		;save for later
     162.  00:50A2                      
     163.  00:50A2  78                       ld    a,b
     164.  00:50A3  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:50A5  B1                       or    c
     166.  00:50A6                           ;finally    000TRLDU
     167.  00:50A6                      
     168.  00:50A6                           ;and    00000100   ;preserve just
     169.  00:50A6  47                       ld    b,a    		;save for later
     170.  00:50A7  CD E1 50                 call  check_4th_keyboard_row      ;check M & P
     171.  00:50AA  F5                       push  af
     172.  00:50AB  E6 04                    and    00000100b	;filter just M
     173.  00:50AD  87                       add    a
     174.  00:50AE  87                       add    a
     175.  00:50AF  87                       add    a           ;shift 3th to 6th bit
     176.  00:50B0  B0                       or    b           	;merge with previous
     177.  00:50B1  47                       ld    b,a
     178.  00:50B2                      
     179.  00:50B2  CD BF 50                 call  read_joy_0
     180.  00:50B5  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:50B7  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:50B8  47                       ld    b,a      	;save for later
     183.  00:50B9  F1                       pop    af      	; let's check on P
     184.  00:50BA                      
     185.  00:50BA  E6 20                    and    00100000b
     186.  00:50BC  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:50BD  B0                       or    b
