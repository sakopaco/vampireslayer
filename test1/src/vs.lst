      45:  00:7BC7  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7BC9                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7BC9                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7BC9  78                  		ld	a,b				; a:=b (new ayFX stream index)
