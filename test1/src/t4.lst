     176:  00:976C  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:976F  CB 51               		bit	2,c				; If tone is off...
     178:  00:9771  C0                  		ret	nz				; ...return
     179:  00:9772  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:9775  C9                  		ret					; Return
     181:  00:9776                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:9776  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:9778  0F                  		rrca					; Rotate right OR mask
     184:  00:9779  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:977B                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:977B  CD 98 97            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:977E  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:9781  CB 49               		bit	1,c				; If tone is off...
     189:  00:9783  C0                  		ret	nz				; ...return
     190:  00:9784  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:9787  C9                  		ret					; Return
     192:  00:9788                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:9788  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:978A  0F                  		rrca					; Rotate right OR mask
     195:  00:978B                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:978B  CD 98 97            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:978E  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:9791  CB 41               		bit	0,c				; If tone is off...
     199:  00:9793  C0                  		ret	nz				; ...return
     200:  00:9794  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:9797  C9                  		ret					; Return
     202:  00:9798                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:9798  4F                  		ld	c,a				; c:=OR mask
     204:  00:9799  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:979C  A2                  		and	d				; AND mask
     206:  00:979D  B1                  		or	c				; OR mask
