     594.  00:50A5                      .deteccioncolision_paso1:
     595.  00:50A5  FD 21 00 C2         		LD		IY, puntomira	;IY punto de mira / IX puerta
     596.  00:50A9  FD 7E 00            		LD		 A, (IY + ESTRUCTURA_PUNTOMIRA.posx)
     597.  00:50AC  C6 08               		ADD		 8				;8-es fijo, offset del punto de mira ya que se mueve segn la esquina superior izquierda y el centro del punto de mira est en el centro del sprite
     598.  00:50AE                      	
     599.  00:50AE                      		;ya tengo en A la coordenada X del centro del punto de mira					
     600.  00:50AE  DD 96 01            		SUB		(IX + ESTRUCTURA_PUERTA.posx)	;le resto el punto x en la puerta
     601.  00:50B1                      	
     602.  00:50B1  30 02               		JR		NC, .deteccioncolision_paso2	;si no es negativo comparo con el radio
     603.  00:50B3                      
     604.  00:50B3  ED 44               		NEG										;si es negativo lo niego (valor absoluto)
     605.  00:50B5                      	
     606.  00:50B5                      .deteccioncolision_paso2:
     607.  00:50B5  DD BE 03            		CP		(IX + ESTRUCTURA_PUERTA.radiox)	;comparo con el radio X de la puerta
     608.  00:50B8                      	
     609.  00:50B8  38 02               		JR		 C, .deteccioncolision_paso3	;SI NC la distancia es >= por lo que sale y no es necesario verificar nada ms
     610.  00:50BA                      	
     611.  00:50BA  AF                  		XOR		 A								;el resultado es falso y se guarda en A y ya no hay que seguir coprobando
     612.  00:50BB  C9                  		RET
     613.  00:50BC                      	
     614.  00:50BC                      .deteccioncolision_paso3:					;la distancia X es vlida, comprobamos la distancia Y
     615.  00:50BC  FD 7E 01            		LD		 A, (IY + ESTRUCTURA_PUNTOMIRA.posy)
     616.  00:50BF  C6 08               		ADD		 8								;le sumo el offset del punto de mira (8 es fijo)
     617.  00:50C1                      
     618.  00:50C1                      		;ya tengo en A la coordenada Y del centro del punto de mira					
     619.  00:50C1  DD 96 02            		SUB		(IX + ESTRUCTURA_PUERTA.posy)	;le resto el punto y en la puerta
     620.  00:50C4                      	
     621.  00:50C4  30 02               		JR		NC, .deteccioncolision_paso4	;si no es negativo comparo con el radio
     622.  00:50C6                      
     623.  00:50C6  ED 44               		NEG										;si es negativo lo niego (valor absoluto)
     624.  00:50C8                      
     625.  00:50C8                      .deteccioncolision_paso4:
     626.  00:50C8  DD BE 04            		CP		(IX + ESTRUCTURA_PUERTA.radioy)	;comparo con el radio Y de la puerta
     627.  00:50CB                      
     628.  00:50CB  38 02               		JR		 C, .deteccioncolision_paso5	;SI NC la distancia es >= por lo que sale y no es necesario verificar nada ms
     629.  00:50CD                      	
     630.  00:50CD  AF                  		XOR		 A								;el resultado es falso y se guarda en A un 0 y al ser la 2 comprobacin salimos
     631.  00:50CE  C9                  		RET
     632.  00:50CF                      
