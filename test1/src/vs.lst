      29:  00:76CB                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:76CB                      		; --- INPUT: A -> sound to be played ---
      31:  00:76CB                      		; ---        C -> sound priority     ---
      32:  00:76CB  C5                  		push	bc				; Store bc in stack
      33:  00:76CC  D5                  		push	de				; Store de in stack
