      29:  00:9216                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:9216                      		; --- INPUT: A -> sound to be played ---
      31:  00:9216                      		; ---        C -> sound priority     ---
      32:  00:9216  C5                  		push	bc				; Store bc in stack
      33:  00:9217  D5                  		push	de				; Store de in stack
      34:  00:9218  E5                  		push	hl				; Store hl in stack
