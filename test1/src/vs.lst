     167.  00:56AF                      
     168.  00:56AF                           ;and    00000100   ;preserve just
     169.  00:56AF  47                       ld    b,a    		;save for later
     170.  00:56B0  CD EA 56                 call  check_4th_keyboard_row      ;check M & P
