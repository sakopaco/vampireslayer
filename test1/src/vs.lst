     197:  00:7E98  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:7E9B  CB 41               		bit	0,c				; If tone is off...
     199:  00:7E9D  C0                  		ret	nz				; ...return
     200:  00:7E9E  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:7EA1  C9                  		ret					; Return
     202:  00:7EA2                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:7EA2  4F                  		ld	c,a				; c:=OR mask
