     180.  00:8578  CB 27                    sla    a
     181.  00:857A  CB 27                    sla    a    		;now 00000L00
     182.  00:857C  4F                       ld    c,a    		;save for later
     183.  00:857D                      
     184.  00:857D  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     185.  00:857E  CB 3F                    srl    a
     186.  00:8580  E6 03                    and    00000011b
     187.  00:8582                      						;we've got 000000DU on a
     188.  00:8582  B1                       or    c    		;now we've got 00000LDU
     189.  00:8583  4F                       ld    c,a    		;save for later
     190.  00:8584                      
     191.  00:8584  78                       ld    a,b
     192.  00:8585  E6 18                    and    00011000b 	;we've got 000TR000
     193.  00:8587  B1                       or    c
