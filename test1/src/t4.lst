     152.  00:904F                      
     153.  00:904F  E6 01                    and    #01    		;save L
     154.  00:9051  CB 27                    sla    a
     155.  00:9053  CB 27                    sla    a    		;now 00000L00
     156.  00:9055  4F                       ld    c,a    		;save for later
     157.  00:9056                      
     158.  00:9056  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     159.  00:9057  CB 3F                    srl    a
     160.  00:9059  E6 03                    and    00000011b
     161.  00:905B                      						;we've got 000000DU on a
     162.  00:905B  B1                       or    c    		;now we've got 00000LDU
     163.  00:905C  4F                       ld    c,a    		;save for later
     164.  00:905D                      
     165.  00:905D  78                       ld    a,b
     166.  00:905E  E6 18                    and    00011000b 	;we've got 000TR000
     167.  00:9060  B1                       or    c
