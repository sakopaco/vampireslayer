      64:  00:8249                    ~ 			add	a,a				; a:=a*2
      65:  00:8249                    ~ 			add	a,a				; a:=a*4
      66:  00:8249                    ~ 			add	a,a				; a:=a*8
      67:  00:8249                    ~ 			add	a,a				; a:=a*16
