      64:  00:90C1                    ~ 			add	a,a				; a:=a*2
      65:  00:90C1                    ~ 			add	a,a				; a:=a*4
      66:  00:90C1                    ~ 			add	a,a				; a:=a*8
      67:  00:90C1                    ~ 			add	a,a				; a:=a*16
