      29:  00:8B27                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:8B27                      		; --- INPUT: A -> sound to be played ---
      31:  00:8B27                      		; ---        C -> sound priority     ---
      32:  00:8B27  C5                  		push	bc				; Store bc in stack
      33:  00:8B28  D5                  		push	de				; Store de in stack
      34:  00:8B29  E5                  		push	hl				; Store hl in stack
      35:  00:8B2A                      		; --- Check if the index is in the bank ---
      36:  00:8B2A  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:8B2B  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:8B2E  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:8B2F  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:8B30  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
