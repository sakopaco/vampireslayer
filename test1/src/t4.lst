      17:  00:99D4                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:99D4                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:99D4  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:99D7  3E 01               		ld	a,1				; a:=0
      21:  00:99D9  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:99DC  3C                  		inc	a				; Starting channel (=1)
      23:  00:99DD  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:99E0                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:99E0  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:99E2  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:99E5  C9                  		ret					; Return
      28:  00:99E6                      
      29:  00:99E6                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:99E6                      		; --- INPUT: A -> sound to be played ---
      31:  00:99E6                      		; ---        C -> sound priority     ---
      32:  00:99E6  C5                  		push	bc				; Store bc in stack
      33:  00:99E7  D5                  		push	de				; Store de in stack
      34:  00:99E8  E5                  		push	hl				; Store hl in stack
      35:  00:99E9                      		; --- Check if the index is in the bank ---
      36:  00:99E9  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:99EA  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:99ED  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:99EE  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:99EF  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:99F1                      		; The bank has less than 256 samples
      42:  00:99F1  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:99F2  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:99F3  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:99F5  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:99F7                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:99F7                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:99F7  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:99F8  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:99FB  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:99FC  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:99FE  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:9A00                      		; --- Set new priority ---
      54:  00:9A00  79                  		ld	a,c				; a:=New priority
      55:  00:9A01  E6 0F               		and	$0F				; We mask the priority
      56:  00:9A03  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:9A06                      
      58:  00:9A06                      		IF ( AYFXRELATIVE == 1 )
      59:  00:9A06                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:9A06                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:9A06                    ~ 			ld	a,15				; a:=15
      62:  00:9A06                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:9A06                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:9A06                    ~ 			add	a,a				; a:=a*2
      65:  00:9A06                    ~ 			add	a,a				; a:=a*4
      66:  00:9A06                    ~ 			add	a,a				; a:=a*8
      67:  00:9A06                    ~ 			add	a,a				; a:=a*16
      68:  00:9A06                    ~ 			ld	e,a				; e:=a
      69:  00:9A06                    ~ 			ld	d,0				; de:=a
      70:  00:9A06                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:9A06                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:9A06                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:9A06                    ~ 		ENDIF
      74:  00:9A06                      
      75:  00:9A06                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:9A06  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:9A0A  13                  		inc	de				; de points to the increments table of the bank
      78:  00:9A0B  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:9A0C  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:9A0E  29                  		add	hl,hl				; hl:=hl*2
      81:  00:9A0F  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:9A10  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:9A11  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:9A12  56                  		ld	d,[hl]				; de:=increment
      85:  00:9A13  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:9A14  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:9A17  AF                  		xor	a				; a:=0 (no errors)
      88:  00:9A18  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:9A19  D1                  		pop	de				; Retrieve de from stack
      90:  00:9A1A  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:9A1B  C9                  		ret					; Return
      92:  00:9A1C                      
      93:  00:9A1C                      		IF ( AYFXRELATIVE == 1 )
      94:  00:9A1C                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:9A1C                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:9A1C                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:9A1C                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:9A1C                    ~ 		ENDIF
