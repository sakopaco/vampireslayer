      17:  00:6C80                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:6C80                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:6C80  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:6C83  3E 01               		ld	a,1				; a:=0
      21:  00:6C85  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:6C88  3C                  		inc	a				; Starting channel (=1)
      23:  00:6C89  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:6C8C                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:6C8C  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:6C8E  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:6C91  C9                  		ret					; Return
