     173.  00:8A21                      ;;update_controllers_status
     174.  00:8A21                      ;;=====================================================	
     175.  00:8A21                      ;depends on the next rountine
     176.  00:8A21                      ;  returns
     177.  00:8A21                      ;  a - 0P2TRLDU
     178.  00:8A21                      
     179.  00:8A21                      update_controllers_status:
     180.  00:8A21  CD 6C 8A                 call  check_8th_keyboard_row
     181.  00:8A24                      [4]  rrc    a
     181.  00:8A24  CB 0F CB 0F CB 0F CB 0F 
     182.  00:8A2C  47                       ld    b,a    		;save for later  xxxTRDUL
     183.  00:8A2D                      
     184.  00:8A2D  E6 01                    and    #01    		;save L
     185.  00:8A2F  CB 27                    sla    a
     186.  00:8A31  CB 27                    sla    a    		;now 00000L00
     187.  00:8A33  4F                       ld    c,a    		;save for later
     188.  00:8A34                      
     189.  00:8A34  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     190.  00:8A35  CB 3F                    srl    a
     191.  00:8A37  E6 03                    and    00000011b
     192.  00:8A39                      						;we've got 000000DU on a
     193.  00:8A39  B1                       or    c    		;now we've got 00000LDU
     194.  00:8A3A  4F                       ld    c,a    		;save for later
     195.  00:8A3B                      
     196.  00:8A3B  78                       ld    a,b
     197.  00:8A3C  E6 18                    and    00011000b 	;we've got 000TR000
     198.  00:8A3E  B1                       or    c
     199.  00:8A3F                           ;finally    000TRLDU
     200.  00:8A3F                      
     201.  00:8A3F                           ;and    00000100   ;preserve just
     202.  00:8A3F  47                       ld    b,a    		;save for later
     203.  00:8A40  CD 7A 8A                 call  check_4th_keyboard_row      ;check M & P
     204.  00:8A43  F5                       push  af
     205.  00:8A44  E6 04                    and    00000100b	;filter just M
     206.  00:8A46  87                       add    a
     207.  00:8A47  87                       add    a
     208.  00:8A48  87                       add    a           ;shift 3th to 6th bit
     209.  00:8A49  B0                       or    b           	;merge with previous
     210.  00:8A4A  47                       ld    b,a
     211.  00:8A4B                      
     212.  00:8A4B  CD 58 8A                 call  read_joy_0
     213.  00:8A4E  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     214.  00:8A50  B0                       or    b      		;added 000TRLDU from keyboard
     215.  00:8A51  47                       ld    b,a      	;save for later
     216.  00:8A52  F1                       pop    af      	; let's check on P
     217.  00:8A53                      
     218.  00:8A53  E6 20                    and    00100000b
     219.  00:8A55  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     220.  00:8A56  B0                       or    b
     221.  00:8A57                      
     222.  00:8A57  C9                       ret
     223.  00:8A58                      
     224.  00:8A58                      
     225.  00:8A58                      ;ret
