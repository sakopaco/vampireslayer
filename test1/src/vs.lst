     150.  00:5412                      
     151.  00:5412  E6 01                    and    #01    		;save L
     152.  00:5414  CB 27                    sla    a
     153.  00:5416  CB 27                    sla    a    		;now 00000L00
     154.  00:5418  4F                       ld    c,a    		;save for later
     155.  00:5419                      
     156.  00:5419  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
