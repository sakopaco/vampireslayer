     100:  00:8271                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:8271  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:8274  B7                  		or	a				; If priority has bit 7 on...
     103:  00:8275  F8                  		ret	m				; ...return
     104:  00:8276                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:8276  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:8279  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:827B  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:827D  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:8280  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:8281  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:8283  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:8285                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:8285  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:8288  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:8289  23                  		inc	hl				; Increment pointer
     116:  00:828A                      		; --- Check if there's new tone on stream ---
     117:  00:828A  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:828C  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:828E                      		; --- Extract new tone from stream ---
     120:  00:828E  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:828F  23                  		inc	hl				; Increment pointer
     122:  00:8290  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:8291  23                  		inc	hl				; Increment pointer
     124:  00:8292  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:8296                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:8296  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:8298  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:829A                      		; --- Extract new noise from stream ---
     129:  00:829A  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:829B  23                  		inc	hl				; Increment pointer
     131:  00:829C  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:829E  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:82A0  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:82A3                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:82A3  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:82A6                      		; --- Extract volume ---
     137:  00:82A6  79                  		ld	a,c				; a:=Control byte
     138:  00:82A7  E6 0F               		and	$0F				; lower nibble
     139:  00:82A9                      
     140:  00:82A9                      		IF ( AYFXRELATIVE == 1 )
     141:  00:82A9                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:82A9                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:82A9                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:82A9                    ~ 			ld	d,0				; d:=0
     145:  00:82A9                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:82A9                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:82A9                    ~ 			or	a				; If relative volume is zero...
     148:  00:82A9                    ~ 		ENDIF
