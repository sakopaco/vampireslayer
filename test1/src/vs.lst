      29:  00:7C43                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7C43                      		; --- INPUT: A -> sound to be played ---
      31:  00:7C43                      		; ---        C -> sound priority     ---
      32:  00:7C43  C5                  		push	bc				; Store bc in stack
      33:  00:7C44  D5                  		push	de				; Store de in stack
      34:  00:7C45  E5                  		push	hl				; Store hl in stack
      35:  00:7C46                      		; --- Check if the index is in the bank ---
      36:  00:7C46  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7C47  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7C4A  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7C4B  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7C4C  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7C4E                      		; The bank has less than 256 samples
      42:  00:7C4E  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7C4F  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7C50  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7C52  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7C54                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7C54                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7C54  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7C55  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7C58  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7C59  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7C5B  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7C5D                      		; --- Set new priority ---
      54:  00:7C5D  79                  		ld	a,c				; a:=New priority
      55:  00:7C5E  E6 0F               		and	$0F				; We mask the priority
      56:  00:7C60  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7C63                      
      58:  00:7C63                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7C63                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7C63                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7C63                    ~ 			ld	a,15				; a:=15
      62:  00:7C63                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7C63                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7C63                    ~ 			add	a,a				; a:=a*2
      65:  00:7C63                    ~ 			add	a,a				; a:=a*4
      66:  00:7C63                    ~ 			add	a,a				; a:=a*8
      67:  00:7C63                    ~ 			add	a,a				; a:=a*16
      68:  00:7C63                    ~ 			ld	e,a				; e:=a
      69:  00:7C63                    ~ 			ld	d,0				; de:=a
      70:  00:7C63                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:7C63                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:7C63                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:7C63                    ~ 		ENDIF
      74:  00:7C63                      
      75:  00:7C63                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:7C63  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:7C67  13                  		inc	de				; de points to the increments table of the bank
      78:  00:7C68  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:7C69  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:7C6B  29                  		add	hl,hl				; hl:=hl*2
      81:  00:7C6C  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:7C6D  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:7C6E  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:7C6F  56                  		ld	d,[hl]				; de:=increment
      85:  00:7C70  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:7C71  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:7C74  AF                  		xor	a				; a:=0 (no errors)
      88:  00:7C75  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:7C76  D1                  		pop	de				; Retrieve de from stack
      90:  00:7C77  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:7C78  C9                  		ret					; Return
      92:  00:7C79                      
      93:  00:7C79                      		IF ( AYFXRELATIVE == 1 )
      94:  00:7C79                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:7C79                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:7C79                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:7C79                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:7C79                    ~ 		ENDIF
