     167.  00:53F7                      
     168.  00:53F7                           ;and    00000100   ;preserve just
     169.  00:53F7  47                       ld    b,a    		;save for later
     170.  00:53F8  CD 32 54                 call  check_4th_keyboard_row      ;check M & P
