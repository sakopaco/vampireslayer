     165:  00:918F                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:918F  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:9190  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:9191  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:9193                      		; --- Dump to correct channel ---
     170:  00:9193  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:9196  46                  		ld	b,[hl]				; Channel counter
     172:  00:9197                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:9197  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:9199                      .PLAY_C:	; --- Play ayFX stream on channel C ---
