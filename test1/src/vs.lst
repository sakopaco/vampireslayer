      35:  00:75CA                      		; --- Check if the index is in the bank ---
      36:  00:75CA  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:75CB  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:75CE  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:75CF  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:75D0  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:75D2                      		; The bank has less than 256 samples
      42:  00:75D2  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:75D3  BE                  		cp	[hl]				; If new index is not in the bank...
