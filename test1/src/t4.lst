      75:  00:970B                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:970B  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:970F  13                  		inc	de				; de points to the increments table of the bank
      78:  00:9710  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:9711  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
