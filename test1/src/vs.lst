      35:  00:813F                      		; --- Check if the index is in the bank ---
      36:  00:813F  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:8140  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
