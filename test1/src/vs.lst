     150.  00:52D2                      
     151.  00:52D2  E6 01                    and    #01    		;save L
     152.  00:52D4  CB 27                    sla    a
     153.  00:52D6  CB 27                    sla    a    		;now 00000L00
     154.  00:52D8  4F                       ld    c,a    		;save for later
     155.  00:52D9                      
     156.  00:52D9  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:52DA  CB 3F                    srl    a
     158.  00:52DC  E6 03                    and    00000011b
     159.  00:52DE                      						;we've got 000000DU on a
     160.  00:52DE  B1                       or    c    		;now we've got 00000LDU
     161.  00:52DF  4F                       ld    c,a    		;save for later
     162.  00:52E0                      
     163.  00:52E0  78                       ld    a,b
     164.  00:52E1  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:52E3  B1                       or    c
