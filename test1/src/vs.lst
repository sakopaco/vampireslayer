      29:  00:75C7                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:75C7                      		; --- INPUT: A -> sound to be played ---
      31:  00:75C7                      		; ---        C -> sound priority     ---
      32:  00:75C7  C5                  		push	bc				; Store bc in stack
      33:  00:75C8  D5                  		push	de				; Store de in stack
