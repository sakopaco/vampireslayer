     154:  00:7C4D                      		; -------------------------------------
     155:  00:7C4D                      		; --- Set noise channel ---
     156:  00:7C4D  CB 79               		bit	7,c				; If noise is off...
     157:  00:7C4F  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:7C51  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:7C54  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:7C57                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:7C57  79                  		ld	a,c				; a:=Control byte
     162:  00:7C58  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:7C5A  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:7C5C  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:7C5D                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:7C5D  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:7C5E  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:7C5F  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:7C61                      		; --- Dump to correct channel ---
     170:  00:7C61  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:7C64  46                  		ld	b,[hl]				; Channel counter
     172:  00:7C65                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:7C65  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:7C67                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:7C67  CD 96 7C            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:7C6A  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:7C6D  CB 51               		bit	2,c				; If tone is off...
     178:  00:7C6F  C0                  		ret	nz				; ...return
     179:  00:7C70  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:7C73  C9                  		ret					; Return
     181:  00:7C74                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:7C74  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:7C76  0F                  		rrca					; Rotate right OR mask
     184:  00:7C77  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:7C79                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:7C79  CD 96 7C            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:7C7C  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:7C7F  CB 49               		bit	1,c				; If tone is off...
     189:  00:7C81  C0                  		ret	nz				; ...return
     190:  00:7C82  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:7C85  C9                  		ret					; Return
     192:  00:7C86                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:7C86  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:7C88  0F                  		rrca					; Rotate right OR mask
     195:  00:7C89                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:7C89  CD 96 7C            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:7C8C  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:7C8F  CB 41               		bit	0,c				; If tone is off...
     199:  00:7C91  C0                  		ret	nz				; ...return
     200:  00:7C92  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:7C95  C9                  		ret					; Return
     202:  00:7C96                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:7C96  4F                  		ld	c,a				; c:=OR mask
     204:  00:7C97  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:7C9A  A2                  		and	d				; AND mask
     206:  00:7C9B  B1                  		or	c				; OR mask
     207:  00:7C9C  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
     208:  00:7C9F  3A 49 F2            		ld	a,[ayFX_VOLUME]			; a:=ayFX volume value
     209:  00:7CA2  2A 46 F2            		ld	hl,[ayFX_TONE]			; ayFX tone value
     210:  00:7CA5  C9                  		ret					; Return
