     178.  00:523F                      
     179.  00:523F  CD 4C 52                 call  read_joy_0
     180.  00:5242  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:5244  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:5245  47                       ld    b,a      	;save for later
     183.  00:5246  F1                       pop    af      	; let's check on P
     184.  00:5247                      
     185.  00:5247  E6 20                    and    00100000b
     186.  00:5249  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:524A  B0                       or    b
