     140.  00:539B                      ;;update_controllers_status
     141.  00:539B                      ;;=====================================================	
     142.  00:539B                      ;depends on the next rountine
     143.  00:539B                      ;  returns
     144.  00:539B                      ;  a - 0P2TRLDU
     145.  00:539B                      
     146.  00:539B                      update_controllers_status:
     147.  00:539B  CD E6 53                 call  check_8th_keyboard_row
     148.  00:539E                      [4]  rrc    a
     148.  00:539E  CB 0F CB 0F CB 0F CB 0F 
     149.  00:53A6  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:53A7                      
     151.  00:53A7  E6 01                    and    #01    		;save L
     152.  00:53A9  CB 27                    sla    a
     153.  00:53AB  CB 27                    sla    a    		;now 00000L00
     154.  00:53AD  4F                       ld    c,a    		;save for later
     155.  00:53AE                      
     156.  00:53AE  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:53AF  CB 3F                    srl    a
     158.  00:53B1  E6 03                    and    00000011b
     159.  00:53B3                      						;we've got 000000DU on a
     160.  00:53B3  B1                       or    c    		;now we've got 00000LDU
     161.  00:53B4  4F                       ld    c,a    		;save for later
     162.  00:53B5                      
     163.  00:53B5  78                       ld    a,b
     164.  00:53B6  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:53B8  B1                       or    c
     166.  00:53B9                           ;finally    000TRLDU
     167.  00:53B9                      
     168.  00:53B9                           ;and    00000100   ;preserve just
     169.  00:53B9  47                       ld    b,a    		;save for later
     170.  00:53BA  CD F4 53                 call  check_4th_keyboard_row      ;check M & P
     171.  00:53BD  F5                       push  af
     172.  00:53BE  E6 04                    and    00000100b	;filter just M
     173.  00:53C0  87                       add    a
     174.  00:53C1  87                       add    a
     175.  00:53C2  87                       add    a           ;shift 3th to 6th bit
     176.  00:53C3  B0                       or    b           	;merge with previous
     177.  00:53C4  47                       ld    b,a
     178.  00:53C5                      
     179.  00:53C5  CD D2 53                 call  read_joy_0
     180.  00:53C8  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:53CA  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:53CB  47                       ld    b,a      	;save for later
     183.  00:53CC  F1                       pop    af      	; let's check on P
     184.  00:53CD                      
     185.  00:53CD  E6 20                    and    00100000b
     186.  00:53CF  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:53D0  B0                       or    b
     188.  00:53D1                      
     189.  00:53D1  C9                       ret
     190.  00:53D2                      
     191.  00:53D2                      
