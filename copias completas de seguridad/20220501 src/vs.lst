     140.  00:4FA6                      ;;update_controllers_status
     141.  00:4FA6                      ;;=====================================================	
     142.  00:4FA6                      ;depends on the next rountine
     143.  00:4FA6                      ;  returns
     144.  00:4FA6                      ;  a - 0P2TRLDU
     145.  00:4FA6                      
     146.  00:4FA6                      update_controllers_status:
     147.  00:4FA6  CD F1 4F                 call  check_8th_keyboard_row
     148.  00:4FA9                      [4]  rrc    a
     148.  00:4FA9  CB 0F CB 0F CB 0F CB 0F 
     149.  00:4FB1  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:4FB2                      
     151.  00:4FB2  E6 01                    and    #01    		;save L
     152.  00:4FB4  CB 27                    sla    a
     153.  00:4FB6  CB 27                    sla    a    		;now 00000L00
     154.  00:4FB8  4F                       ld    c,a    		;save for later
     155.  00:4FB9                      
     156.  00:4FB9  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:4FBA  CB 3F                    srl    a
     158.  00:4FBC  E6 03                    and    00000011b
     159.  00:4FBE                      						;we've got 000000DU on a
     160.  00:4FBE  B1                       or    c    		;now we've got 00000LDU
     161.  00:4FBF  4F                       ld    c,a    		;save for later
     162.  00:4FC0                      
     163.  00:4FC0  78                       ld    a,b
     164.  00:4FC1  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:4FC3  B1                       or    c
     166.  00:4FC4                           ;finally    000TRLDU
     167.  00:4FC4                      
     168.  00:4FC4                           ;and    00000100   ;preserve just
     169.  00:4FC4  47                       ld    b,a    		;save for later
     170.  00:4FC5  CD FF 4F                 call  check_4th_keyboard_row      ;check M & P
     171.  00:4FC8  F5                       push  af
     172.  00:4FC9  E6 04                    and    00000100b	;filter just M
     173.  00:4FCB  87                       add    a
     174.  00:4FCC  87                       add    a
     175.  00:4FCD  87                       add    a           ;shift 3th to 6th bit
     176.  00:4FCE  B0                       or    b           	;merge with previous
     177.  00:4FCF  47                       ld    b,a
     178.  00:4FD0                      
     179.  00:4FD0  CD DD 4F                 call  read_joy_0
     180.  00:4FD3  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:4FD5  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:4FD6  47                       ld    b,a      	;save for later
     183.  00:4FD7  F1                       pop    af      	; let's check on P
     184.  00:4FD8                      
     185.  00:4FD8  E6 20                    and    00100000b
     186.  00:4FDA  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:4FDB  B0                       or    b
     188.  00:4FDC                      
     189.  00:4FDC  C9                       ret
     190.  00:4FDD                      
     191.  00:4FDD                      
