     148.  00:513A                      [4]  rrc    a
     148.  00:513A  CB 0F CB 0F CB 0F CB 0F 
     149.  00:5142  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:5143                      
     151.  00:5143  E6 01                    and    #01    		;save L
     152.  00:5145  CB 27                    sla    a
     153.  00:5147  CB 27                    sla    a    		;now 00000L00
     154.  00:5149  4F                       ld    c,a    		;save for later
     155.  00:514A                      
     156.  00:514A  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:514B  CB 3F                    srl    a
     158.  00:514D  E6 03                    and    00000011b
     159.  00:514F                      						;we've got 000000DU on a
     160.  00:514F  B1                       or    c    		;now we've got 00000LDU
     161.  00:5150  4F                       ld    c,a    		;save for later
     162.  00:5151                      
     163.  00:5151  78                       ld    a,b
     164.  00:5152  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5154  B1                       or    c
     166.  00:5155                           ;finally    000TRLDU
     167.  00:5155                      
     168.  00:5155                           ;and    00000100   ;preserve just
     169.  00:5155  47                       ld    b,a    		;save for later
     170.  00:5156  CD 90 51                 call  check_4th_keyboard_row      ;check M & P
     171.  00:5159  F5                       push  af
     172.  00:515A  E6 04                    and    00000100b	;filter just M
     173.  00:515C  87                       add    a
     174.  00:515D  87                       add    a
     175.  00:515E  87                       add    a           ;shift 3th to 6th bit
     176.  00:515F  B0                       or    b           	;merge with previous
     177.  00:5160  47                       ld    b,a
     178.  00:5161                      
     179.  00:5161  CD 6E 51                 call  read_joy_0
     180.  00:5164  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:5166  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:5167  47                       ld    b,a      	;save for later
     183.  00:5168  F1                       pop    af      	; let's check on P
     184.  00:5169                      
     185.  00:5169  E6 20                    and    00100000b
     186.  00:516B  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:516C  B0                       or    b
     188.  00:516D                      
     189.  00:516D  C9                       ret
     190.  00:516E                      
     191.  00:516E                      
