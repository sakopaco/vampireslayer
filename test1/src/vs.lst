     125:  00:8291                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:8291  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:8293  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:8295                      		; --- Extract new noise from stream ---
