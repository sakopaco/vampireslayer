      17:  00:7BA6                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7BA6                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7BA6  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
