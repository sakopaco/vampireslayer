      29:  00:7E0D                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7E0D                      		; --- INPUT: A -> sound to be played ---
      31:  00:7E0D                      		; ---        C -> sound priority     ---
      32:  00:7E0D  C5                  		push	bc				; Store bc in stack
      33:  00:7E0E  D5                  		push	de				; Store de in stack
      34:  00:7E0F  E5                  		push	hl				; Store hl in stack
      35:  00:7E10                      		; --- Check if the index is in the bank ---
      36:  00:7E10  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7E11  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7E14  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7E15  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7E16  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7E18                      		; The bank has less than 256 samples
      42:  00:7E18  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7E19  BE                  		cp	[hl]				; If new index is not in the bank...
