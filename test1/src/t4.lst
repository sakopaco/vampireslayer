      45:  00:90D8  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:90DA                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:90DA                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:90DA  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:90DB  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:90DE  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:90DF  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:90E1  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:90E3                      		; --- Set new priority ---
      54:  00:90E3  79                  		ld	a,c				; a:=New priority
      55:  00:90E4  E6 0F               		and	$0F				; We mask the priority
      56:  00:90E6  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:90E9                      
      58:  00:90E9                      		IF ( AYFXRELATIVE == 1 )
