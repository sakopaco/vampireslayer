     188:  00:954D  CB 49               		bit	1,c				; If tone is off...
     189:  00:954F  C0                  		ret	nz				; ...return
     190:  00:9550  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:9553  C9                  		ret					; Return
     192:  00:9554                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:9554  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:9556  0F                  		rrca					; Rotate right OR mask
