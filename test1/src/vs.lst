      54:  00:8243  79                  		ld	a,c				; a:=New priority
      55:  00:8244  E6 0F               		and	$0F				; We mask the priority
      56:  00:8246  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:8249                      
      58:  00:8249                      		IF ( AYFXRELATIVE == 1 )
      59:  00:8249                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:8249                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:8249                    ~ 			ld	a,15				; a:=15
      62:  00:8249                    ~ 			sub	c				; a:=15-New priority = relative volume
