     201.  00:86BC  87                       add    a
     202.  00:86BD  87                       add    a
     203.  00:86BE  87                       add    a           ;shift 3th to 6th bit
     204.  00:86BF  B0                       or    b           	;merge with previous
     205.  00:86C0  47                       ld    b,a
     206.  00:86C1                      
     207.  00:86C1  CD CE 86                 call  read_joy_0
     208.  00:86C4  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     209.  00:86C6  B0                       or    b      		;added 000TRLDU from keyboard
     210.  00:86C7  47                       ld    b,a      	;save for later
     211.  00:86C8  F1                       pop    af      	; let's check on P
     212.  00:86C9                      
     213.  00:86C9  E6 20                    and    00100000b
     214.  00:86CB  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     215.  00:86CC  B0                       or    b
