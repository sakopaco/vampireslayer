     152:  00:82AD                      		; -------------------------------------
     153:  00:82AD                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:82AD                      		; -------------------------------------
     155:  00:82AD                      		; --- Set noise channel ---
     156:  00:82AD  CB 79               		bit	7,c				; If noise is off...
     157:  00:82AF  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
