     173:  00:9B78  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:9B7A                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:9B7A  CD A9 9B            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:9B7D  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:9B80  CB 51               		bit	2,c				; If tone is off...
     178:  00:9B82  C0                  		ret	nz				; ...return
     179:  00:9B83  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:9B86  C9                  		ret					; Return
     181:  00:9B87                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:9B87  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:9B89  0F                  		rrca					; Rotate right OR mask
     184:  00:9B8A  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:9B8C                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:9B8C  CD A9 9B            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:9B8F  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:9B92  CB 49               		bit	1,c				; If tone is off...
     189:  00:9B94  C0                  		ret	nz				; ...return
     190:  00:9B95  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:9B98  C9                  		ret					; Return
     192:  00:9B99                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:9B99  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:9B9B  0F                  		rrca					; Rotate right OR mask
     195:  00:9B9C                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:9B9C  CD A9 9B            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:9B9F  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:9BA2  CB 41               		bit	0,c				; If tone is off...
     199:  00:9BA4  C0                  		ret	nz				; ...return
     200:  00:9BA5  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:9BA8  C9                  		ret					; Return
     202:  00:9BA9                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:9BA9  4F                  		ld	c,a				; c:=OR mask
     204:  00:9BAA  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:9BAD  A2                  		and	d				; AND mask
     206:  00:9BAE  B1                  		or	c				; OR mask
     207:  00:9BAF  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
