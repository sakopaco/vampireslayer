      29:  00:8C12                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:8C12                      		; --- INPUT: A -> sound to be played ---
      31:  00:8C12                      		; ---        C -> sound priority     ---
      32:  00:8C12  C5                  		push	bc				; Store bc in stack
      33:  00:8C13  D5                  		push	de				; Store de in stack
      34:  00:8C14  E5                  		push	hl				; Store hl in stack
      35:  00:8C15                      		; --- Check if the index is in the bank ---
      36:  00:8C15  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:8C16  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:8C19  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:8C1A  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:8C1B  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:8C1D                      		; The bank has less than 256 samples
      42:  00:8C1D  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:8C1E  BE                  		cp	[hl]				; If new index is not in the bank...
