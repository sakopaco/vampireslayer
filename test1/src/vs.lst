      29:  00:75D5                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:75D5                      		; --- INPUT: A -> sound to be played ---
      31:  00:75D5                      		; ---        C -> sound priority     ---
      32:  00:75D5  C5                  		push	bc				; Store bc in stack
      33:  00:75D6  D5                  		push	de				; Store de in stack
      34:  00:75D7  E5                  		push	hl				; Store hl in stack
      35:  00:75D8                      		; --- Check if the index is in the bank ---
      36:  00:75D8  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:75D9  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:75DC  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:75DD  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:75DE  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:75E0                      		; The bank has less than 256 samples
      42:  00:75E0  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:75E1  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:75E2  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:75E4  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:75E6                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:75E6                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:75E6  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:75E7  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:75EA  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:75EB  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:75ED  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
