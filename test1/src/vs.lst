      17:  00:7A0B                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7A0B                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7A0B  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
