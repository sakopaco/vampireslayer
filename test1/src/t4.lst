     123:  00:9B9D  23                  		inc	hl				; Increment pointer
     124:  00:9B9E  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:9BA2                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:9BA2  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:9BA4  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:9BA6                      		; --- Extract new noise from stream ---
     129:  00:9BA6  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:9BA7  23                  		inc	hl				; Increment pointer
     131:  00:9BA8  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:9BAA  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:9BAC  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:9BAF                      .SETPOINTER:	; --- Update ayFX pointer ---
