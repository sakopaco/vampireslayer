      92:  00:8F51                      
      93:  00:8F51                      		IF ( AYFXRELATIVE == 1 )
      94:  00:8F51                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:8F51                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:8F51                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:8F51                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:8F51                    ~ 		ENDIF
