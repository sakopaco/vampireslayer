     187:  00:92C1  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:92C4  CB 49               		bit	1,c				; If tone is off...
     189:  00:92C6  C0                  		ret	nz				; ...return
     190:  00:92C7  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:92CA  C9                  		ret					; Return
     192:  00:92CB                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:92CB  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:92CD  0F                  		rrca					; Rotate right OR mask
     195:  00:92CE                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:92CE  CD DB 92            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:92D1  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:92D4  CB 41               		bit	0,c				; If tone is off...
     199:  00:92D6  C0                  		ret	nz				; ...return
     200:  00:92D7  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:92DA  C9                  		ret					; Return
     202:  00:92DB                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:92DB  4F                  		ld	c,a				; c:=OR mask
     204:  00:92DC  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
