     112:  00:76CD                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:76CD  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:76D0  4E                  		ld	c,[hl]				; c:=Control byte
