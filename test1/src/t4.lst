      94:  00:98D2                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:98D2                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:98D2                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:98D2                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
