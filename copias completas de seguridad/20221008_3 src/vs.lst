      17:  00:7A05                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7A05                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7A05  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7A08  3E 01               		ld	a,1				; a:=0
      21:  00:7A0A  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7A0D  3C                  		inc	a				; Starting channel (=1)
      23:  00:7A0E  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7A11                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7A11  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7A13  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7A16  C9                  		ret					; Return
