     183:  00:82D7  0F                  		rrca					; Rotate right OR mask
     184:  00:82D8  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:82DA                      .PLAY_B:	; --- Play ayFX stream on channel B ---
