     100:  00:9A1C                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:9A1C  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:9A1F  B7                  		or	a				; If priority has bit 7 on...
     103:  00:9A20  F8                  		ret	m				; ...return
     104:  00:9A21                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:9A21  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:9A24  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:9A26  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:9A28  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:9A2B  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:9A2C  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:9A2E  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:9A30                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:9A30  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:9A33  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:9A34  23                  		inc	hl				; Increment pointer
     116:  00:9A35                      		; --- Check if there's new tone on stream ---
     117:  00:9A35  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:9A37  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:9A39                      		; --- Extract new tone from stream ---
     120:  00:9A39  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:9A3A  23                  		inc	hl				; Increment pointer
     122:  00:9A3B  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:9A3C  23                  		inc	hl				; Increment pointer
     124:  00:9A3D  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:9A41                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:9A41  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:9A43  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:9A45                      		; --- Extract new noise from stream ---
     129:  00:9A45  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:9A46  23                  		inc	hl				; Increment pointer
     131:  00:9A47  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:9A49  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:9A4B  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:9A4E                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:9A4E  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:9A51                      		; --- Extract volume ---
     137:  00:9A51  79                  		ld	a,c				; a:=Control byte
     138:  00:9A52  E6 0F               		and	$0F				; lower nibble
     139:  00:9A54                      
     140:  00:9A54                      		IF ( AYFXRELATIVE == 1 )
     141:  00:9A54                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:9A54                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:9A54                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:9A54                    ~ 			ld	d,0				; d:=0
     145:  00:9A54                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:9A54                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:9A54                    ~ 			or	a				; If relative volume is zero...
     148:  00:9A54                    ~ 		ENDIF
     149:  00:9A54                      
     150:  00:9A54  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:9A57  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:9A58                      		; -------------------------------------
     153:  00:9A58                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:9A58                      		; -------------------------------------
     155:  00:9A58                      		; --- Set noise channel ---
     156:  00:9A58  CB 79               		bit	7,c				; If noise is off...
