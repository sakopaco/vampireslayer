     178.  00:86A3                      
     179.  00:86A3  E6 01                    and    #01    		;save L
     180.  00:86A5  CB 27                    sla    a
     181.  00:86A7  CB 27                    sla    a    		;now 00000L00
     182.  00:86A9  4F                       ld    c,a    		;save for later
     183.  00:86AA                      
     184.  00:86AA  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     185.  00:86AB  CB 3F                    srl    a
     186.  00:86AD  E6 03                    and    00000011b
     187.  00:86AF                      						;we've got 000000DU on a
     188.  00:86AF  B1                       or    c    		;now we've got 00000LDU
     189.  00:86B0  4F                       ld    c,a    		;save for later
     190.  00:86B1                      
     191.  00:86B1  78                       ld    a,b
     192.  00:86B2  E6 18                    and    00011000b 	;we've got 000TR000
     193.  00:86B4  B1                       or    c
     194.  00:86B5                           ;finally    000TRLDU
     195.  00:86B5                      
     196.  00:86B5                           ;and    00000100   ;preserve just
     197.  00:86B5  47                       ld    b,a    		;save for later
     198.  00:86B6  CD F0 86                 call  check_4th_keyboard_row      ;check M & P
     199.  00:86B9  F5                       push  af
