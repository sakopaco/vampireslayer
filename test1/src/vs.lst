      83:  00:8262  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:8263  56                  		ld	d,[hl]				; de:=increment
      85:  00:8264  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:8265  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:8268  AF                  		xor	a				; a:=0 (no errors)
      88:  00:8269  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:826A  D1                  		pop	de				; Retrieve de from stack
      90:  00:826B  C1                  		pop	bc				; Retrieve bc from stack
