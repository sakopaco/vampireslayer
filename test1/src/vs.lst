      82:  00:7E54  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:7E55  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:7E56  56                  		ld	d,[hl]				; de:=increment
      85:  00:7E57  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:7E58  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:7E5B  AF                  		xor	a				; a:=0 (no errors)
      88:  00:7E5C  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:7E5D  D1                  		pop	de				; Retrieve de from stack
