      82:  00:8260  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:8261  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:8262  56                  		ld	d,[hl]				; de:=increment
      85:  00:8263  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:8264  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:8267  AF                  		xor	a				; a:=0 (no errors)
      88:  00:8268  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:8269  D1                  		pop	de				; Retrieve de from stack
      90:  00:826A  C1                  		pop	bc				; Retrieve bc from stack
