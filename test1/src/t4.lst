     173.  00:8A10                      ;;update_controllers_status
     174.  00:8A10                      ;;=====================================================	
     175.  00:8A10                      ;depends on the next rountine
     176.  00:8A10                      ;  returns
     177.  00:8A10                      ;  a - 0P2TRLDU
     178.  00:8A10                      
     179.  00:8A10                      update_controllers_status:
     180.  00:8A10  CD 5B 8A                 call  check_8th_keyboard_row
     181.  00:8A13                      [4]  rrc    a
     181.  00:8A13  CB 0F CB 0F CB 0F CB 0F 
     182.  00:8A1B  47                       ld    b,a    		;save for later  xxxTRDUL
     183.  00:8A1C                      
     184.  00:8A1C  E6 01                    and    #01    		;save L
     185.  00:8A1E  CB 27                    sla    a
     186.  00:8A20  CB 27                    sla    a    		;now 00000L00
     187.  00:8A22  4F                       ld    c,a    		;save for later
     188.  00:8A23                      
     189.  00:8A23  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     190.  00:8A24  CB 3F                    srl    a
     191.  00:8A26  E6 03                    and    00000011b
     192.  00:8A28                      						;we've got 000000DU on a
     193.  00:8A28  B1                       or    c    		;now we've got 00000LDU
     194.  00:8A29  4F                       ld    c,a    		;save for later
     195.  00:8A2A                      
     196.  00:8A2A  78                       ld    a,b
     197.  00:8A2B  E6 18                    and    00011000b 	;we've got 000TR000
     198.  00:8A2D  B1                       or    c
     199.  00:8A2E                           ;finally    000TRLDU
     200.  00:8A2E                      
     201.  00:8A2E                           ;and    00000100   ;preserve just
     202.  00:8A2E  47                       ld    b,a    		;save for later
     203.  00:8A2F  CD 69 8A                 call  check_4th_keyboard_row      ;check M & P
     204.  00:8A32  F5                       push  af
     205.  00:8A33  E6 04                    and    00000100b	;filter just M
     206.  00:8A35  87                       add    a
     207.  00:8A36  87                       add    a
     208.  00:8A37  87                       add    a           ;shift 3th to 6th bit
     209.  00:8A38  B0                       or    b           	;merge with previous
     210.  00:8A39  47                       ld    b,a
     211.  00:8A3A                      
     212.  00:8A3A  CD 47 8A                 call  read_joy_0
     213.  00:8A3D  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     214.  00:8A3F  B0                       or    b      		;added 000TRLDU from keyboard
     215.  00:8A40  47                       ld    b,a      	;save for later
     216.  00:8A41  F1                       pop    af      	; let's check on P
     217.  00:8A42                      
     218.  00:8A42  E6 20                    and    00100000b
     219.  00:8A44  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     220.  00:8A45  B0                       or    b
     221.  00:8A46                      
     222.  00:8A46  C9                       ret
     223.  00:8A47                      
     224.  00:8A47                      
     225.  00:8A47                      ;ret
