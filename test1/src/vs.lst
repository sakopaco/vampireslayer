     140.  00:4FA3                      ;;update_controllers_status
     141.  00:4FA3                      ;;=====================================================	
     142.  00:4FA3                      ;depends on the next rountine
     143.  00:4FA3                      ;  returns
     144.  00:4FA3                      ;  a - 0P2TRLDU
     145.  00:4FA3                      
     146.  00:4FA3                      update_controllers_status:
     147.  00:4FA3  CD EE 4F                 call  check_8th_keyboard_row
     148.  00:4FA6                      [4]  rrc    a
     148.  00:4FA6  CB 0F CB 0F CB 0F CB 0F 
     149.  00:4FAE  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:4FAF                      
     151.  00:4FAF  E6 01                    and    #01    		;save L
     152.  00:4FB1  CB 27                    sla    a
     153.  00:4FB3  CB 27                    sla    a    		;now 00000L00
     154.  00:4FB5  4F                       ld    c,a    		;save for later
     155.  00:4FB6                      
     156.  00:4FB6  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:4FB7  CB 3F                    srl    a
     158.  00:4FB9  E6 03                    and    00000011b
     159.  00:4FBB                      						;we've got 000000DU on a
     160.  00:4FBB  B1                       or    c    		;now we've got 00000LDU
     161.  00:4FBC  4F                       ld    c,a    		;save for later
     162.  00:4FBD                      
     163.  00:4FBD  78                       ld    a,b
     164.  00:4FBE  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:4FC0  B1                       or    c
     166.  00:4FC1                           ;finally    000TRLDU
     167.  00:4FC1                      
     168.  00:4FC1                           ;and    00000100   ;preserve just
     169.  00:4FC1  47                       ld    b,a    		;save for later
     170.  00:4FC2  CD FC 4F                 call  check_4th_keyboard_row      ;check M & P
     171.  00:4FC5  F5                       push  af
     172.  00:4FC6  E6 04                    and    00000100b	;filter just M
     173.  00:4FC8  87                       add    a
     174.  00:4FC9  87                       add    a
     175.  00:4FCA  87                       add    a           ;shift 3th to 6th bit
     176.  00:4FCB  B0                       or    b           	;merge with previous
     177.  00:4FCC  47                       ld    b,a
     178.  00:4FCD                      
     179.  00:4FCD  CD DA 4F                 call  read_joy_0
     180.  00:4FD0  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:4FD2  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:4FD3  47                       ld    b,a      	;save for later
     183.  00:4FD4  F1                       pop    af      	; let's check on P
     184.  00:4FD5                      
     185.  00:4FD5  E6 20                    and    00100000b
     186.  00:4FD7  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:4FD8  B0                       or    b
     188.  00:4FD9                      
     189.  00:4FD9  C9                       ret
     190.  00:4FDA                      
     191.  00:4FDA                      
     192.  00:4FDA                      ;ret
     193.  00:4FDA                      ;  a -            xx2TRLDU
     194.  00:4FDA                      read_joy_0:
     195.  00:4FDA  F3                         di
     196.  00:4FDB  3E 0F                      ld    a,15
     197.  00:4FDD  D3 A0                      out    [PSG_A],a
     198.  00:4FDF  DB A2                      in    a,[PSG_R]
     199.  00:4FE1  E6 BF                      and    10111111b        ;joy0
     200.  00:4FE3                      
     201.  00:4FE3  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:4FE5                      
     203.  00:4FE5  3E 0E                      ld    a,14
     204.  00:4FE7  D3 A0                      out    [PSG_A],a
     205.  00:4FE9  DB A2                      in    a,[PSG_R]
     206.  00:4FEB                      
     207.  00:4FEB  FB                         ei
     208.  00:4FEC  2F                         cpl
