      46:  00:75D7                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:75D7                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:75D7  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:75D8  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
