      83:  00:9C00  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:9C01  56                  		ld	d,[hl]				; de:=increment
      85:  00:9C02  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:9C03  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:9C06  AF                  		xor	a				; a:=0 (no errors)
      88:  00:9C07  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
