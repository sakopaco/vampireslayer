     100:  00:8C48                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:8C48  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
