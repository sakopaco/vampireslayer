      56:  00:7B9B  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7B9E                      
      58:  00:7B9E                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7B9E                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7B9E                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7B9E                    ~ 			ld	a,15				; a:=15
      62:  00:7B9E                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7B9E                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7B9E                    ~ 			add	a,a				; a:=a*2
      65:  00:7B9E                    ~ 			add	a,a				; a:=a*4
      66:  00:7B9E                    ~ 			add	a,a				; a:=a*8
      67:  00:7B9E                    ~ 			add	a,a				; a:=a*16
