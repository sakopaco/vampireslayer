     173.  00:4FB4  87                       add    a
     174.  00:4FB5  87                       add    a
     175.  00:4FB6  87                       add    a           ;shift 3th to 6th bit
     176.  00:4FB7  B0                       or    b           	;merge with previous
     177.  00:4FB8  47                       ld    b,a
     178.  00:4FB9                      
     179.  00:4FB9  CD C6 4F                 call  read_joy_0
     180.  00:4FBC  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:4FBE  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:4FBF  47                       ld    b,a      	;save for later
     183.  00:4FC0  F1                       pop    af      	; let's check on P
     184.  00:4FC1                      
     185.  00:4FC1  E6 20                    and    00100000b
     186.  00:4FC3  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:4FC4  B0                       or    b
