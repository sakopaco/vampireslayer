     135:  00:8FC0  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:8FC3                      		; --- Extract volume ---
     137:  00:8FC3  79                  		ld	a,c				; a:=Control byte
