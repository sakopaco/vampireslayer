      83:  00:7CB7  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:7CB8  56                  		ld	d,[hl]				; de:=increment
      85:  00:7CB9  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:7CBA  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:7CBD  AF                  		xor	a				; a:=0 (no errors)
      88:  00:7CBE  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:7CBF  D1                  		pop	de				; Retrieve de from stack
