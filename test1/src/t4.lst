     206:  00:A094  B1                  		or	c				; OR mask
     207:  00:A095  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
     208:  00:A098  3A 49 F2            		ld	a,[ayFX_VOLUME]			; a:=ayFX volume value
     209:  00:A09B  2A 46 F2            		ld	hl,[ayFX_TONE]			; ayFX tone value
