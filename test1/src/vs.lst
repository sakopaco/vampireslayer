     178.  00:5403                      
     179.  00:5403  CD 10 54                 call  read_joy_0
     180.  00:5406  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:5408  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:5409  47                       ld    b,a      	;save for later
     183.  00:540A  F1                       pop    af      	; let's check on P
     184.  00:540B                      
     185.  00:540B  E6 20                    and    00100000b
     186.  00:540D  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
