     100:  00:9255                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:9255  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:9258  B7                  		or	a				; If priority has bit 7 on...
     103:  00:9259  F8                  		ret	m				; ...return
     104:  00:925A                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:925A  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:925D  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:925F  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:9261  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:9264  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:9265  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:9267  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:9269                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:9269  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:926C  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:926D  23                  		inc	hl				; Increment pointer
     116:  00:926E                      		; --- Check if there's new tone on stream ---
     117:  00:926E  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:9270  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:9272                      		; --- Extract new tone from stream ---
     120:  00:9272  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:9273  23                  		inc	hl				; Increment pointer
     122:  00:9274  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:9275  23                  		inc	hl				; Increment pointer
     124:  00:9276  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:927A                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:927A  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:927C  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:927E                      		; --- Extract new noise from stream ---
     129:  00:927E  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:927F  23                  		inc	hl				; Increment pointer
     131:  00:9280  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:9282  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:9284  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:9287                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:9287  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:928A                      		; --- Extract volume ---
     137:  00:928A  79                  		ld	a,c				; a:=Control byte
     138:  00:928B  E6 0F               		and	$0F				; lower nibble
     139:  00:928D                      
     140:  00:928D                      		IF ( AYFXRELATIVE == 1 )
     141:  00:928D                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:928D                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:928D                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:928D                    ~ 			ld	d,0				; d:=0
     145:  00:928D                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:928D                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:928D                    ~ 			or	a				; If relative volume is zero...
     148:  00:928D                    ~ 		ENDIF
     149:  00:928D                      
     150:  00:928D  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:9290  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:9291                      		; -------------------------------------
     153:  00:9291                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:9291                      		; -------------------------------------
     155:  00:9291                      		; --- Set noise channel ---
     156:  00:9291  CB 79               		bit	7,c				; If noise is off...
     157:  00:9293  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:9295  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:9298  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:929B                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:929B  79                  		ld	a,c				; a:=Control byte
     162:  00:929C  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:929E  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:92A0  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:92A1                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:92A1  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:92A2  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:92A3  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:92A5                      		; --- Dump to correct channel ---
     170:  00:92A5  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:92A8  46                  		ld	b,[hl]				; Channel counter
     172:  00:92A9                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:92A9  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:92AB                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:92AB  CD DA 92            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:92AE  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:92B1  CB 51               		bit	2,c				; If tone is off...
     178:  00:92B3  C0                  		ret	nz				; ...return
     179:  00:92B4  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:92B7  C9                  		ret					; Return
     181:  00:92B8                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:92B8  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:92BA  0F                  		rrca					; Rotate right OR mask
     184:  00:92BB  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:92BD                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:92BD  CD DA 92            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:92C0  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:92C3  CB 49               		bit	1,c				; If tone is off...
     189:  00:92C5  C0                  		ret	nz				; ...return
     190:  00:92C6  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:92C9  C9                  		ret					; Return
     192:  00:92CA                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:92CA  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:92CC  0F                  		rrca					; Rotate right OR mask
     195:  00:92CD                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:92CD  CD DA 92            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:92D0  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:92D3  CB 41               		bit	0,c				; If tone is off...
     199:  00:92D5  C0                  		ret	nz				; ...return
     200:  00:92D6  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:92D9  C9                  		ret					; Return
     202:  00:92DA                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:92DA  4F                  		ld	c,a				; c:=OR mask
     204:  00:92DB  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:92DE  A2                  		and	d				; AND mask
     206:  00:92DF  B1                  		or	c				; OR mask
     207:  00:92E0  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
     208:  00:92E3  3A 49 F2            		ld	a,[ayFX_VOLUME]			; a:=ayFX volume value
     209:  00:92E6  2A 46 F2            		ld	hl,[ayFX_TONE]			; ayFX tone value
     210:  00:92E9  C9                  		ret					; Return
