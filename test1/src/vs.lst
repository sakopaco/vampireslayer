     181.  00:8E00  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:8E01  47                       ld    b,a      	;save for later
     183.  00:8E02  F1                       pop    af      	; let's check on P
     184.  00:8E03                      
     185.  00:8E03  E6 20                    and    00100000b
     186.  00:8E05  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
