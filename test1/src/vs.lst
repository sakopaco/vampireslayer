      17:  00:7E7E                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7E7E                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7E7E  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7E81  3E 01               		ld	a,1				; a:=0
      21:  00:7E83  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7E86  3C                  		inc	a				; Starting channel (=1)
      23:  00:7E87  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7E8A                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7E8A  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7E8C  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7E8F  C9                  		ret					; Return
      28:  00:7E90                      
      29:  00:7E90                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7E90                      		; --- INPUT: A -> sound to be played ---
      31:  00:7E90                      		; ---        C -> sound priority     ---
      32:  00:7E90  C5                  		push	bc				; Store bc in stack
      33:  00:7E91  D5                  		push	de				; Store de in stack
      34:  00:7E92  E5                  		push	hl				; Store hl in stack
      35:  00:7E93                      		; --- Check if the index is in the bank ---
      36:  00:7E93  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7E94  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7E97  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7E98  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7E99  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7E9B                      		; The bank has less than 256 samples
      42:  00:7E9B  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7E9C  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7E9D  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7E9F  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7EA1                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7EA1                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7EA1  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7EA2  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7EA5  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7EA6  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7EA8  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7EAA                      		; --- Set new priority ---
      54:  00:7EAA  79                  		ld	a,c				; a:=New priority
      55:  00:7EAB  E6 0F               		and	$0F				; We mask the priority
      56:  00:7EAD  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7EB0                      
      58:  00:7EB0                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7EB0                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7EB0                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7EB0                    ~ 			ld	a,15				; a:=15
      62:  00:7EB0                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7EB0                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7EB0                    ~ 			add	a,a				; a:=a*2
