      17:  00:76A1                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:76A1                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:76A1  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:76A4  3E 01               		ld	a,1				; a:=0
      21:  00:76A6  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:76A9  3C                  		inc	a				; Starting channel (=1)
      23:  00:76AA  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:76AD                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:76AD  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:76AF  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:76B2  C9                  		ret					; Return
      28:  00:76B3                      
      29:  00:76B3                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:76B3                      		; --- INPUT: A -> sound to be played ---
      31:  00:76B3                      		; ---        C -> sound priority     ---
      32:  00:76B3  C5                  		push	bc				; Store bc in stack
      33:  00:76B4  D5                  		push	de				; Store de in stack
      34:  00:76B5  E5                  		push	hl				; Store hl in stack
      35:  00:76B6                      		; --- Check if the index is in the bank ---
      36:  00:76B6  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:76B7  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:76BA  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:76BB  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:76BC  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:76BE                      		; The bank has less than 256 samples
      42:  00:76BE  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:76BF  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:76C0  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:76C2  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:76C4                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:76C4                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:76C4  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:76C5  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:76C8  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:76C9  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:76CB  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:76CD                      		; --- Set new priority ---
      54:  00:76CD  79                  		ld	a,c				; a:=New priority
      55:  00:76CE  E6 0F               		and	$0F				; We mask the priority
      56:  00:76D0  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:76D3                      
      58:  00:76D3                      		IF ( AYFXRELATIVE == 1 )
      59:  00:76D3                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:76D3                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:76D3                    ~ 			ld	a,15				; a:=15
      62:  00:76D3                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:76D3                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:76D3                    ~ 			add	a,a				; a:=a*2
      65:  00:76D3                    ~ 			add	a,a				; a:=a*4
      66:  00:76D3                    ~ 			add	a,a				; a:=a*8
      67:  00:76D3                    ~ 			add	a,a				; a:=a*16
      68:  00:76D3                    ~ 			ld	e,a				; e:=a
      69:  00:76D3                    ~ 			ld	d,0				; de:=a
      70:  00:76D3                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:76D3                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:76D3                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:76D3                    ~ 		ENDIF
