     160.  00:55CE  B1                       or    c    		;now we've got 00000LDU
     161.  00:55CF  4F                       ld    c,a    		;save for later
     162.  00:55D0                      
     163.  00:55D0  78                       ld    a,b
     164.  00:55D1  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:55D3  B1                       or    c
     166.  00:55D4                           ;finally    000TRLDU
     167.  00:55D4                      
     168.  00:55D4                           ;and    00000100   ;preserve just
     169.  00:55D4  47                       ld    b,a    		;save for later
