      17:  00:7C71                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7C71                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7C71  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
