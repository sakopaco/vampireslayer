     126.  00:80CD                      		;inicializo valores a 0
     127.  00:80CD  AF                  		XOR		 A
     128.  00:80CE  57                  		LD		 D, A
     129.  00:80CF  5F                  		LD		 E, A
     130.  00:80D0  67                  		LD		 H, A
     131.  00:80D1  6F                  		LD		 L, A
     132.  00:80D2                      
     133.  00:80D2  58                  		LD		 E, B				;pongo el (4x7) en HL (estaba en B)
     134.  00:80D3                      		
     135.  00:80D3  06 10               		LD		 B, 16
     136.  00:80D5                      .segundo_producto
     137.  00:80D5  19                  		ADD		HL, DE				;segundo producto x 16
     138.  00:80D6  10 FD               		DJNZ	.segundo_producto	
