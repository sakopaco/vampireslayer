      46:  00:7A5F                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7A5F                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7A5F  78                  		ld	a,b				; a:=b (new ayFX stream index)
