      33:  00:9BD6  D5                  		push	de				; Store de in stack
      34:  00:9BD7  E5                  		push	hl				; Store hl in stack
      35:  00:9BD8                      		; --- Check if the index is in the bank ---
      36:  00:9BD8  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:9BD9  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:9BDC  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:9BDD  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:9BDE  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:9BE0                      		; The bank has less than 256 samples
