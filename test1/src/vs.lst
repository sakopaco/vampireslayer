      63:  00:76A3                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:76A3                    ~ 			add	a,a				; a:=a*2
      65:  00:76A3                    ~ 			add	a,a				; a:=a*4
      66:  00:76A3                    ~ 			add	a,a				; a:=a*8
      67:  00:76A3                    ~ 			add	a,a				; a:=a*16
