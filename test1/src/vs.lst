     202.  00:5086                      ; toca:		AF, HL, BC, DE
     203.  00:5086                      pinta_obj_ayuda:
     204.  00:5086                      	;se recibe objeto desde fuera, por ejmplo LD IX, ayuda_oracion
     205.  00:5086                      
     206.  00:5086                      	;si A=1 dibujo on (si 0 off)
     207.  00:5086  B7                  	OR		 A
     208.  00:5087  C2 96 50            	JP		 NZ,.dibujooff
     209.  00:508A                      .dibujoon:	
     210.  00:508A  DD 6E 09            		LD		 L, (IX + ESTRUCTURA_AYUDA.tiles_ayudaoff)
     211.  00:508D  DD 66 0A            		LD		 H, (IX + ESTRUCTURA_AYUDA.tiles_ayudaoff + 1)
     212.  00:5090  22 E0 C1            		LD		(wordaux2), HL	;pongo el valor en wordaux2
     213.  00:5093  C3 9F 50            		JP		.param_wordaux2
