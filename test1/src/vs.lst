     149.  00:5417  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:5418                      
     151.  00:5418  E6 01                    and    #01    		;save L
     152.  00:541A  CB 27                    sla    a
     153.  00:541C  CB 27                    sla    a    		;now 00000L00
     154.  00:541E  4F                       ld    c,a    		;save for later
     155.  00:541F                      
     156.  00:541F  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:5420  CB 3F                    srl    a
     158.  00:5422  E6 03                    and    00000011b
     159.  00:5424                      						;we've got 000000DU on a
     160.  00:5424  B1                       or    c    		;now we've got 00000LDU
     161.  00:5425  4F                       ld    c,a    		;save for later
     162.  00:5426                      
     163.  00:5426  78                       ld    a,b
     164.  00:5427  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5429  B1                       or    c
     166.  00:542A                           ;finally    000TRLDU
     167.  00:542A                      
     168.  00:542A                           ;and    00000100   ;preserve just
     169.  00:542A  47                       ld    b,a    		;save for later
     170.  00:542B  CD 65 54                 call  check_4th_keyboard_row      ;check M & P
     171.  00:542E  F5                       push  af
     172.  00:542F  E6 04                    and    00000100b	;filter just M
     173.  00:5431  87                       add    a
     174.  00:5432  87                       add    a
     175.  00:5433  87                       add    a           ;shift 3th to 6th bit
     176.  00:5434  B0                       or    b           	;merge with previous
     177.  00:5435  47                       ld    b,a
     178.  00:5436                      
     179.  00:5436  CD 43 54                 call  read_joy_0
     180.  00:5439  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:543B  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:543C  47                       ld    b,a      	;save for later
     183.  00:543D  F1                       pop    af      	; let's check on P
     184.  00:543E                      
     185.  00:543E  E6 20                    and    00100000b
     186.  00:5440  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:5441  B0                       or    b
