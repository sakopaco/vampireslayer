      17:  00:8B0F                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:8B0F                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:8B0F  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:8B12  3E 01               		ld	a,1				; a:=0
      21:  00:8B14  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:8B17  3C                  		inc	a				; Starting channel (=1)
      23:  00:8B18  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:8B1B                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:8B1B  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:8B1D  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:8B20  C9                  		ret					; Return
