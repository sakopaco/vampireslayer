      75:  00:825C                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:825C  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:8260  13                  		inc	de				; de points to the increments table of the bank
      78:  00:8261  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:8262  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:8264  29                  		add	hl,hl				; hl:=hl*2
      81:  00:8265  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:8266  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:8267  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:8268  56                  		ld	d,[hl]				; de:=increment
      85:  00:8269  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:826A  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:826D  AF                  		xor	a				; a:=0 (no errors)
      88:  00:826E  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:826F  D1                  		pop	de				; Retrieve de from stack
      90:  00:8270  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:8271  C9                  		ret					; Return
      92:  00:8272                      
      93:  00:8272                      		IF ( AYFXRELATIVE == 1 )
      94:  00:8272                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:8272                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:8272                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:8272                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:8272                    ~ 		ENDIF
