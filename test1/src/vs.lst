     140.  00:4F92                      ;;update_controllers_status
     141.  00:4F92                      ;;=====================================================	
     142.  00:4F92                      ;depends on the next rountine
     143.  00:4F92                      ;  returns
     144.  00:4F92                      ;  a - 0P2TRLDU
