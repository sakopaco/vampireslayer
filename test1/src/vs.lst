      17:  00:7671                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7671                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7671  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7674  3E 01               		ld	a,1				; a:=0
      21:  00:7676  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7679  3C                  		inc	a				; Starting channel (=1)
      23:  00:767A  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:767D                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:767D  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:767F  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7682  C9                  		ret					; Return
