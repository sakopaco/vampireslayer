     179:  00:8EA3  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:8EA6  C9                  		ret					; Return
     181:  00:8EA7                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:8EA7  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:8EA9  0F                  		rrca					; Rotate right OR mask
     184:  00:8EAA  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:8EAC                      .PLAY_B:	; --- Play ayFX stream on channel B ---
