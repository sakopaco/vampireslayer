     146.  00:50BD                      update_controllers_status:
     147.  00:50BD  CD 08 51                 call  check_8th_keyboard_row
     148.  00:50C0                      [4]  rrc    a
     148.  00:50C0  CB 0F CB 0F CB 0F CB 0F 
     149.  00:50C8  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:50C9                      
     151.  00:50C9  E6 01                    and    #01    		;save L
     152.  00:50CB  CB 27                    sla    a
     153.  00:50CD  CB 27                    sla    a    		;now 00000L00
     154.  00:50CF  4F                       ld    c,a    		;save for later
     155.  00:50D0                      
     156.  00:50D0  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:50D1  CB 3F                    srl    a
     158.  00:50D3  E6 03                    and    00000011b
     159.  00:50D5                      						;we've got 000000DU on a
     160.  00:50D5  B1                       or    c    		;now we've got 00000LDU
     161.  00:50D6  4F                       ld    c,a    		;save for later
     162.  00:50D7                      
     163.  00:50D7  78                       ld    a,b
     164.  00:50D8  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:50DA  B1                       or    c
     166.  00:50DB                           ;finally    000TRLDU
     167.  00:50DB                      
     168.  00:50DB                           ;and    00000100   ;preserve just
     169.  00:50DB  47                       ld    b,a    		;save for later
     170.  00:50DC  CD 16 51                 call  check_4th_keyboard_row      ;check M & P
     171.  00:50DF  F5                       push  af
     172.  00:50E0  E6 04                    and    00000100b	;filter just M
     173.  00:50E2  87                       add    a
     174.  00:50E3  87                       add    a
     175.  00:50E4  87                       add    a           ;shift 3th to 6th bit
     176.  00:50E5  B0                       or    b           	;merge with previous
     177.  00:50E6  47                       ld    b,a
     178.  00:50E7                      
     179.  00:50E7  CD F4 50                 call  read_joy_0
     180.  00:50EA  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:50EC  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:50ED  47                       ld    b,a      	;save for later
     183.  00:50EE  F1                       pop    af      	; let's check on P
     184.  00:50EF                      
     185.  00:50EF  E6 20                    and    00100000b
     186.  00:50F1  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:50F2  B0                       or    b
