     150.  00:5097                      
     151.  00:5097  E6 01                    and    #01    		;save L
     152.  00:5099  CB 27                    sla    a
     153.  00:509B  CB 27                    sla    a    		;now 00000L00
     154.  00:509D  4F                       ld    c,a    		;save for later
     155.  00:509E                      
     156.  00:509E  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:509F  CB 3F                    srl    a
     158.  00:50A1  E6 03                    and    00000011b
     159.  00:50A3                      						;we've got 000000DU on a
     160.  00:50A3  B1                       or    c    		;now we've got 00000LDU
     161.  00:50A4  4F                       ld    c,a    		;save for later
     162.  00:50A5                      
     163.  00:50A5  78                       ld    a,b
     164.  00:50A6  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:50A8  B1                       or    c
     166.  00:50A9                           ;finally    000TRLDU
     167.  00:50A9                      
     168.  00:50A9                           ;and    00000100   ;preserve just
     169.  00:50A9  47                       ld    b,a    		;save for later
     170.  00:50AA  CD E4 50                 call  check_4th_keyboard_row      ;check M & P
     171.  00:50AD  F5                       push  af
     172.  00:50AE  E6 04                    and    00000100b	;filter just M
     173.  00:50B0  87                       add    a
     174.  00:50B1  87                       add    a
     175.  00:50B2  87                       add    a           ;shift 3th to 6th bit
     176.  00:50B3  B0                       or    b           	;merge with previous
     177.  00:50B4  47                       ld    b,a
     178.  00:50B5                      
     179.  00:50B5  CD C2 50                 call  read_joy_0
     180.  00:50B8  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:50BA  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:50BB  47                       ld    b,a      	;save for later
     183.  00:50BC  F1                       pop    af      	; let's check on P
     184.  00:50BD                      
     185.  00:50BD  E6 20                    and    00100000b
     186.  00:50BF  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:50C0  B0                       or    b
