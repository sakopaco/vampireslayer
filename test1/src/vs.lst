     161.  00:536E  4F                       ld    c,a    		;save for later
     162.  00:536F                      
     163.  00:536F  78                       ld    a,b
     164.  00:5370  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5372  B1                       or    c
