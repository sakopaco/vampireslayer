     202.  00:5065                      ; toca:		AF, HL, BC, DE
     203.  00:5065                      pinta_obj_ayuda:
     204.  00:5065                      	;se recibe objeto desde fuera, por ejmplo LD IX, ayuda_oracion
     205.  00:5065                      
     206.  00:5065                      	;si A=1 dibujo on (si 0 off)
     207.  00:5065  B7                  	OR		 A
     208.  00:5066  C2 75 50            	JP		 NZ,.dibujooff
