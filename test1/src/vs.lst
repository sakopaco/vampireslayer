     100:  00:7E43                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:7E43  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:7E46  B7                  		or	a				; If priority has bit 7 on...
     103:  00:7E47  F8                  		ret	m				; ...return
