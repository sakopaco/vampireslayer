      64:  00:7BD8                    ~ 			add	a,a				; a:=a*2
      65:  00:7BD8                    ~ 			add	a,a				; a:=a*4
      66:  00:7BD8                    ~ 			add	a,a				; a:=a*8
      67:  00:7BD8                    ~ 			add	a,a				; a:=a*16
