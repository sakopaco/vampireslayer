      17:  00:7CCC                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7CCC                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7CCC  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7CCF  3E 01               		ld	a,1				; a:=0
      21:  00:7CD1  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7CD4  3C                  		inc	a				; Starting channel (=1)
      23:  00:7CD5  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7CD8                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7CD8  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7CDA  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7CDD  C9                  		ret					; Return
      28:  00:7CDE                      
      29:  00:7CDE                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7CDE                      		; --- INPUT: A -> sound to be played ---
      31:  00:7CDE                      		; ---        C -> sound priority     ---
      32:  00:7CDE  C5                  		push	bc				; Store bc in stack
      33:  00:7CDF  D5                  		push	de				; Store de in stack
      34:  00:7CE0  E5                  		push	hl				; Store hl in stack
      35:  00:7CE1                      		; --- Check if the index is in the bank ---
      36:  00:7CE1  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7CE2  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7CE5  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7CE6  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7CE7  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7CE9                      		; The bank has less than 256 samples
      42:  00:7CE9  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7CEA  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7CEB  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7CED  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7CEF                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7CEF                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7CEF  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7CF0  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7CF3  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7CF4  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7CF6  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7CF8                      		; --- Set new priority ---
      54:  00:7CF8  79                  		ld	a,c				; a:=New priority
      55:  00:7CF9  E6 0F               		and	$0F				; We mask the priority
      56:  00:7CFB  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7CFE                      
      58:  00:7CFE                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7CFE                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7CFE                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7CFE                    ~ 			ld	a,15				; a:=15
      62:  00:7CFE                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7CFE                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7CFE                    ~ 			add	a,a				; a:=a*2
      65:  00:7CFE                    ~ 			add	a,a				; a:=a*4
      66:  00:7CFE                    ~ 			add	a,a				; a:=a*8
      67:  00:7CFE                    ~ 			add	a,a				; a:=a*16
      68:  00:7CFE                    ~ 			ld	e,a				; e:=a
      69:  00:7CFE                    ~ 			ld	d,0				; de:=a
      70:  00:7CFE                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:7CFE                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:7CFE                    ~ 			ld	[ayFX_VT],hl			; Save pointer
