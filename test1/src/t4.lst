     100:  00:98D2                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:98D2  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:98D5  B7                  		or	a				; If priority has bit 7 on...
     103:  00:98D6  F8                  		ret	m				; ...return
