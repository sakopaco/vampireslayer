     181.  00:56BF  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:56C0  47                       ld    b,a      	;save for later
     183.  00:56C1  F1                       pop    af      	; let's check on P
     184.  00:56C2                      
     185.  00:56C2  E6 20                    and    00100000b
     186.  00:56C4  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
