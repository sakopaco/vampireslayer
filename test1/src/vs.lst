     150.  00:4F9B                      
     151.  00:4F9B  E6 01                    and    #01    		;save L
     152.  00:4F9D  CB 27                    sla    a
     153.  00:4F9F  CB 27                    sla    a    		;now 00000L00
     154.  00:4FA1  4F                       ld    c,a    		;save for later
     155.  00:4FA2                      
     156.  00:4FA2  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:4FA3  CB 3F                    srl    a
     158.  00:4FA5  E6 03                    and    00000011b
     159.  00:4FA7                      						;we've got 000000DU on a
     160.  00:4FA7  B1                       or    c    		;now we've got 00000LDU
     161.  00:4FA8  4F                       ld    c,a    		;save for later
     162.  00:4FA9                      
     163.  00:4FA9  78                       ld    a,b
     164.  00:4FAA  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:4FAC  B1                       or    c
     166.  00:4FAD                           ;finally    000TRLDU
     167.  00:4FAD                      
     168.  00:4FAD                           ;and    00000100   ;preserve just
     169.  00:4FAD  47                       ld    b,a    		;save for later
     170.  00:4FAE  CD E8 4F                 call  check_4th_keyboard_row      ;check M & P
