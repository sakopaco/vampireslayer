      92:  00:7E60                      
      93:  00:7E60                      		IF ( AYFXRELATIVE == 1 )
      94:  00:7E60                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:7E60                    ~ 			ld	a,255				; Lowest ayFX priority
