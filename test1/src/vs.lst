     153:  00:6D04                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:6D04                      		; -------------------------------------
     155:  00:6D04                      		; --- Set noise channel ---
     156:  00:6D04  CB 79               		bit	7,c				; If noise is off...
