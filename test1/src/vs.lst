     151.  00:56B5  E6 01                    and    #01    		;save L
     152.  00:56B7  CB 27                    sla    a
     153.  00:56B9  CB 27                    sla    a    		;now 00000L00
     154.  00:56BB  4F                       ld    c,a    		;save for later
     155.  00:56BC                      
     156.  00:56BC  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
