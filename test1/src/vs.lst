     154.  00:55CB  4F                       ld    c,a    		;save for later
     155.  00:55CC                      
     156.  00:55CC  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:55CD  CB 3F                    srl    a
