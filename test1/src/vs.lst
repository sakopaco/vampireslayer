      17:  00:76B9                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:76B9                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:76B9  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
