      33:  00:9A10  D5                  		push	de				; Store de in stack
      34:  00:9A11  E5                  		push	hl				; Store hl in stack
      35:  00:9A12                      		; --- Check if the index is in the bank ---
      36:  00:9A12  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:9A13  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:9A16  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:9A17  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:9A18  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:9A1A                      		; The bank has less than 256 samples
      42:  00:9A1A  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:9A1B  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:9A1C  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:9A1E  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:9A20                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:9A20                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:9A20  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:9A21  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:9A24  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:9A25  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
