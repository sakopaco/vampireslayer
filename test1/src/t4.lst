     152.  00:8D65                      
     153.  00:8D65  E6 01                    and    #01    		;save L
     154.  00:8D67  CB 27                    sla    a
     155.  00:8D69  CB 27                    sla    a    		;now 00000L00
     156.  00:8D6B  4F                       ld    c,a    		;save for later
     157.  00:8D6C                      
     158.  00:8D6C  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     159.  00:8D6D  CB 3F                    srl    a
     160.  00:8D6F  E6 03                    and    00000011b
     161.  00:8D71                      						;we've got 000000DU on a
     162.  00:8D71  B1                       or    c    		;now we've got 00000LDU
     163.  00:8D72  4F                       ld    c,a    		;save for later
     164.  00:8D73                      
     165.  00:8D73  78                       ld    a,b
     166.  00:8D74  E6 18                    and    00011000b 	;we've got 000TR000
     167.  00:8D76  B1                       or    c
     168.  00:8D77                           ;finally    000TRLDU
     169.  00:8D77                      
     170.  00:8D77                           ;and    00000100   ;preserve just
     171.  00:8D77  47                       ld    b,a    		;save for later
     172.  00:8D78  CD B2 8D                 call  check_4th_keyboard_row      ;check M & P
