      17:  00:8F09                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:8F09                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:8F09  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:8F0C  3E 01               		ld	a,1				; a:=0
      21:  00:8F0E  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:8F11  3C                  		inc	a				; Starting channel (=1)
      23:  00:8F12  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:8F15                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:8F15  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:8F17  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:8F1A  C9                  		ret					; Return
