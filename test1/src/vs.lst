      29:  00:7DCC                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7DCC                      		; --- INPUT: A -> sound to be played ---
      31:  00:7DCC                      		; ---        C -> sound priority     ---
      32:  00:7DCC  C5                  		push	bc				; Store bc in stack
      33:  00:7DCD  D5                  		push	de				; Store de in stack
      34:  00:7DCE  E5                  		push	hl				; Store hl in stack
      35:  00:7DCF                      		; --- Check if the index is in the bank ---
      36:  00:7DCF  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7DD0  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7DD3  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7DD4  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7DD5  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7DD7                      		; The bank has less than 256 samples
      42:  00:7DD7  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7DD8  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7DD9  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7DDB  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7DDD                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7DDD                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7DDD  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7DDE  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7DE1  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7DE2  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7DE4  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
