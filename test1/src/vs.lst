      79:  00:7BA4  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:7BA6  29                  		add	hl,hl				; hl:=hl*2
      81:  00:7BA7  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:7BA8  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:7BA9  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:7BAA  56                  		ld	d,[hl]				; de:=increment
      85:  00:7BAB  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:7BAC  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:7BAF  AF                  		xor	a				; a:=0 (no errors)
      88:  00:7BB0  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:7BB1  D1                  		pop	de				; Retrieve de from stack
      90:  00:7BB2  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:7BB3  C9                  		ret					; Return
