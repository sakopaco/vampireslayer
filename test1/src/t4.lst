      82:  00:8F82  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:8F83  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:8F84  56                  		ld	d,[hl]				; de:=increment
      85:  00:8F85  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:8F86  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:8F89  AF                  		xor	a				; a:=0 (no errors)
      88:  00:8F8A  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:8F8B  D1                  		pop	de				; Retrieve de from stack
      90:  00:8F8C  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:8F8D  C9                  		ret					; Return
      92:  00:8F8E                      
      93:  00:8F8E                      		IF ( AYFXRELATIVE == 1 )
      94:  00:8F8E                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:8F8E                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:8F8E                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:8F8E                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:8F8E                    ~ 		ENDIF
