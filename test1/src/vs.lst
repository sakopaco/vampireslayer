     199.  00:4FD0  E6 BF                      and    10111111b        ;joy0
     200.  00:4FD2                      
     201.  00:4FD2  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:4FD4                      
     203.  00:4FD4  3E 0E                      ld    a,14
