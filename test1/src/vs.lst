      36:  00:7A20  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7A21  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7A24  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7A25  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7A26  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7A28                      		; The bank has less than 256 samples
      42:  00:7A28  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7A29  BE                  		cp	[hl]				; If new index is not in the bank...
