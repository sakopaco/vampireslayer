     100:  00:6CC8                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:6CC8  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:6CCB  B7                  		or	a				; If priority has bit 7 on...
     103:  00:6CCC  F8                  		ret	m				; ...return
     104:  00:6CCD                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:6CCD  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:6CD0  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:6CD2  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:6CD4  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:6CD7  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:6CD8  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:6CDA  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:6CDC                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:6CDC  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:6CDF  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:6CE0  23                  		inc	hl				; Increment pointer
     116:  00:6CE1                      		; --- Check if there's new tone on stream ---
     117:  00:6CE1  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:6CE3  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:6CE5                      		; --- Extract new tone from stream ---
     120:  00:6CE5  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:6CE6  23                  		inc	hl				; Increment pointer
     122:  00:6CE7  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:6CE8  23                  		inc	hl				; Increment pointer
     124:  00:6CE9  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:6CED                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:6CED  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:6CEF  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:6CF1                      		; --- Extract new noise from stream ---
     129:  00:6CF1  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:6CF2  23                  		inc	hl				; Increment pointer
     131:  00:6CF3  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:6CF5  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:6CF7  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:6CFA                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:6CFA  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:6CFD                      		; --- Extract volume ---
     137:  00:6CFD  79                  		ld	a,c				; a:=Control byte
     138:  00:6CFE  E6 0F               		and	$0F				; lower nibble
     139:  00:6D00                      
     140:  00:6D00                      		IF ( AYFXRELATIVE == 1 )
     141:  00:6D00                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:6D00                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:6D00                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:6D00                    ~ 			ld	d,0				; d:=0
     145:  00:6D00                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:6D00                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:6D00                    ~ 			or	a				; If relative volume is zero...
     148:  00:6D00                    ~ 		ENDIF
