      29:  00:7CFB                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7CFB                      		; --- INPUT: A -> sound to be played ---
      31:  00:7CFB                      		; ---        C -> sound priority     ---
      32:  00:7CFB  C5                  		push	bc				; Store bc in stack
      33:  00:7CFC  D5                  		push	de				; Store de in stack
      34:  00:7CFD  E5                  		push	hl				; Store hl in stack
      35:  00:7CFE                      		; --- Check if the index is in the bank ---
      36:  00:7CFE  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7CFF  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7D02  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7D03  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7D04  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7D06                      		; The bank has less than 256 samples
      42:  00:7D06  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7D07  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7D08  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7D0A  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7D0C                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7D0C                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7D0C  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7D0D  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7D10  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7D11  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7D13  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7D15                      		; --- Set new priority ---
      54:  00:7D15  79                  		ld	a,c				; a:=New priority
      55:  00:7D16  E6 0F               		and	$0F				; We mask the priority
