     176:  00:919C  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:919F  CB 51               		bit	2,c				; If tone is off...
     178:  00:91A1  C0                  		ret	nz				; ...return
     179:  00:91A2  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:91A5  C9                  		ret					; Return
     181:  00:91A6                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:91A6  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:91A8  0F                  		rrca					; Rotate right OR mask
     184:  00:91A9  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:91AB                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:91AB  CD C8 91            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:91AE  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:91B1  CB 49               		bit	1,c				; If tone is off...
     189:  00:91B3  C0                  		ret	nz				; ...return
     190:  00:91B4  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:91B7  C9                  		ret					; Return
     192:  00:91B8                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:91B8  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:91BA  0F                  		rrca					; Rotate right OR mask
     195:  00:91BB                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:91BB  CD C8 91            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:91BE  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:91C1  CB 41               		bit	0,c				; If tone is off...
     199:  00:91C3  C0                  		ret	nz				; ...return
