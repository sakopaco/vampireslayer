      75:  00:96FE                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:96FE  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:9702  13                  		inc	de				; de points to the increments table of the bank
      78:  00:9703  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:9704  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:9706  29                  		add	hl,hl				; hl:=hl*2
      81:  00:9707  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:9708  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:9709  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:970A  56                  		ld	d,[hl]				; de:=increment
      85:  00:970B  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:970C  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:970F  AF                  		xor	a				; a:=0 (no errors)
      88:  00:9710  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:9711  D1                  		pop	de				; Retrieve de from stack
      90:  00:9712  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:9713  C9                  		ret					; Return
      92:  00:9714                      
      93:  00:9714                      		IF ( AYFXRELATIVE == 1 )
      94:  00:9714                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:9714                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:9714                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:9714                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:9714                    ~ 		ENDIF
