     150.  00:53FF                      
     151.  00:53FF  E6 01                    and    #01    		;save L
     152.  00:5401  CB 27                    sla    a
     153.  00:5403  CB 27                    sla    a    		;now 00000L00
     154.  00:5405  4F                       ld    c,a    		;save for later
     155.  00:5406                      
     156.  00:5406  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:5407  CB 3F                    srl    a
     158.  00:5409  E6 03                    and    00000011b
     159.  00:540B                      						;we've got 000000DU on a
     160.  00:540B  B1                       or    c    		;now we've got 00000LDU
     161.  00:540C  4F                       ld    c,a    		;save for later
     162.  00:540D                      
     163.  00:540D  78                       ld    a,b
     164.  00:540E  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5410  B1                       or    c
     166.  00:5411                           ;finally    000TRLDU
     167.  00:5411                      
     168.  00:5411                           ;and    00000100   ;preserve just
     169.  00:5411  47                       ld    b,a    		;save for later
     170.  00:5412  CD 4C 54                 call  check_4th_keyboard_row      ;check M & P
