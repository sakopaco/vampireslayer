     149.  00:539A  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:539B                      
     151.  00:539B  E6 01                    and    #01    		;save L
     152.  00:539D  CB 27                    sla    a
     153.  00:539F  CB 27                    sla    a    		;now 00000L00
     154.  00:53A1  4F                       ld    c,a    		;save for later
     155.  00:53A2                      
     156.  00:53A2  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:53A3  CB 3F                    srl    a
     158.  00:53A5  E6 03                    and    00000011b
     159.  00:53A7                      						;we've got 000000DU on a
     160.  00:53A7  B1                       or    c    		;now we've got 00000LDU
     161.  00:53A8  4F                       ld    c,a    		;save for later
     162.  00:53A9                      
     163.  00:53A9  78                       ld    a,b
     164.  00:53AA  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:53AC  B1                       or    c
     166.  00:53AD                           ;finally    000TRLDU
     167.  00:53AD                      
     168.  00:53AD                           ;and    00000100   ;preserve just
     169.  00:53AD  47                       ld    b,a    		;save for later
     170.  00:53AE  CD E8 53                 call  check_4th_keyboard_row      ;check M & P
     171.  00:53B1  F5                       push  af
     172.  00:53B2  E6 04                    and    00000100b	;filter just M
     173.  00:53B4  87                       add    a
     174.  00:53B5  87                       add    a
     175.  00:53B6  87                       add    a           ;shift 3th to 6th bit
     176.  00:53B7  B0                       or    b           	;merge with previous
     177.  00:53B8  47                       ld    b,a
     178.  00:53B9                      
     179.  00:53B9  CD C6 53                 call  read_joy_0
     180.  00:53BC  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:53BE  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:53BF  47                       ld    b,a      	;save for later
     183.  00:53C0  F1                       pop    af      	; let's check on P
     184.  00:53C1                      
     185.  00:53C1  E6 20                    and    00100000b
     186.  00:53C3  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:53C4  B0                       or    b
     188.  00:53C5                      
     189.  00:53C5  C9                       ret
