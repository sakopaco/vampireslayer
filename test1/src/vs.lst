      35:  00:76CE                      		; --- Check if the index is in the bank ---
      36:  00:76CE  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:76CF  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
