      56:  00:76A0  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:76A3                      
      58:  00:76A3                      		IF ( AYFXRELATIVE == 1 )
      59:  00:76A3                    ~ 			; --- Volume adjust using PT3 volume table ---
