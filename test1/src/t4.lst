      37:  00:8C4D  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:8C50  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:8C51  B7                  		or	a				; If zero (means 256 samples)...
