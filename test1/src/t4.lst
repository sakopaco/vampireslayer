     142.  00:8B7C                      ;;update_controllers_status
     143.  00:8B7C                      ;;=====================================================	
     144.  00:8B7C                      ;depends on the next rountine
     145.  00:8B7C                      ;  returns
     146.  00:8B7C                      ;  a - 0P2TRLDU
     147.  00:8B7C                      
     148.  00:8B7C                      update_controllers_status:
     149.  00:8B7C  CD C7 8B                 call  check_8th_keyboard_row
     150.  00:8B7F                      [4]  rrc    a
     150.  00:8B7F  CB 0F CB 0F CB 0F CB 0F 
     151.  00:8B87  47                       ld    b,a    		;save for later  xxxTRDUL
     152.  00:8B88                      
     153.  00:8B88  E6 01                    and    #01    		;save L
     154.  00:8B8A  CB 27                    sla    a
     155.  00:8B8C  CB 27                    sla    a    		;now 00000L00
     156.  00:8B8E  4F                       ld    c,a    		;save for later
     157.  00:8B8F                      
     158.  00:8B8F  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     159.  00:8B90  CB 3F                    srl    a
     160.  00:8B92  E6 03                    and    00000011b
     161.  00:8B94                      						;we've got 000000DU on a
     162.  00:8B94  B1                       or    c    		;now we've got 00000LDU
     163.  00:8B95  4F                       ld    c,a    		;save for later
     164.  00:8B96                      
     165.  00:8B96  78                       ld    a,b
     166.  00:8B97  E6 18                    and    00011000b 	;we've got 000TR000
     167.  00:8B99  B1                       or    c
     168.  00:8B9A                           ;finally    000TRLDU
     169.  00:8B9A                      
     170.  00:8B9A                           ;and    00000100   ;preserve just
     171.  00:8B9A  47                       ld    b,a    		;save for later
     172.  00:8B9B  CD D5 8B                 call  check_4th_keyboard_row      ;check M & P
     173.  00:8B9E  F5                       push  af
     174.  00:8B9F  E6 04                    and    00000100b	;filter just M
     175.  00:8BA1  87                       add    a
     176.  00:8BA2  87                       add    a
     177.  00:8BA3  87                       add    a           ;shift 3th to 6th bit
     178.  00:8BA4  B0                       or    b           	;merge with previous
     179.  00:8BA5  47                       ld    b,a
     180.  00:8BA6                      
     181.  00:8BA6  CD B3 8B                 call  read_joy_0
     182.  00:8BA9  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     183.  00:8BAB  B0                       or    b      		;added 000TRLDU from keyboard
     184.  00:8BAC  47                       ld    b,a      	;save for later
     185.  00:8BAD  F1                       pop    af      	; let's check on P
