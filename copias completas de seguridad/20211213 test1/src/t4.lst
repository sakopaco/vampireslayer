     210.  00:9E02                      ;;PINTA_OBJ_AYUDA
     211.  00:9E02                      ;;=====================================================	
     212.  00:9E02                      ; función: 	pinta un objeto de tipo ayuda que se le pase por referencia IX y el tipo en A
     213.  00:9E02                      ; entrada: 	IX - puntero a objeto a mostrar, A - si está on u off
     214.  00:9E02                      ; salida: 	-
     215.  00:9E02                      ; toca:		AF, HL, BC, DE
     216.  00:9E02                      pinta_obj_ayuda:
     217.  00:9E02                      	;se recibe objeto desde fuera, por ejmplo LD IX, ayuda_oracion
     218.  00:9E02                      
     219.  00:9E02                      	;si A=1 dibujo on (si 0 off)
     220.  00:9E02  B7                  	OR		 A
     221.  00:9E03  C2 12 9E            	JP		 NZ,.dibujooff
     222.  00:9E06                      .dibujoon:	
     223.  00:9E06  DD 6E 09            		LD		 L, (IX + ESTRUCTURA_AYUDA.tiles_ayudaoff)
     224.  00:9E09  DD 66 0A            		LD		 H, (IX + ESTRUCTURA_AYUDA.tiles_ayudaoff + 1)
     225.  00:9E0C  22 1A 86            		LD		(wordaux2), HL	;pongo el valor en wordaux2
     226.  00:9E0F  C3 1B 9E            		JP		.param_wordaux2
     227.  00:9E12                      .dibujooff:
     228.  00:9E12  DD 6E 07            		LD		 L, (IX + ESTRUCTURA_AYUDA.tiles_ayudaon)
     229.  00:9E15  DD 66 08            		LD		 H, (IX + ESTRUCTURA_AYUDA.tiles_ayudaon + 1)
     230.  00:9E18  22 1A 86            		LD		(wordaux2), HL	;pongo el valor en wordaux2
     231.  00:9E1B                      ;fin si
     232.  00:9E1B                      .param_wordaux2:	
     233.  00:9E1B  DD 6E 0B            	LD		 L, (IX + ESTRUCTURA_AYUDA.pos_en_tilemap)
     234.  00:9E1E  DD 66 0C            	LD		 H, (IX + ESTRUCTURA_AYUDA.pos_en_tilemap + 1)
     235.  00:9E21  22 18 86            	LD		(wordaux1), HL		;pongo el valor en wordaux1
