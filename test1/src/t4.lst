      45:  00:9BE4  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:9BE6                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:9BE6                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:9BE6  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:9BE7  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:9BEA  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:9BEB  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:9BED  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:9BEF                      		; --- Set new priority ---
      54:  00:9BEF  79                  		ld	a,c				; a:=New priority
      55:  00:9BF0  E6 0F               		and	$0F				; We mask the priority
      56:  00:9BF2  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:9BF5                      
      58:  00:9BF5                      		IF ( AYFXRELATIVE == 1 )
