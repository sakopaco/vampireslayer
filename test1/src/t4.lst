     142.  00:91F4                      ;;update_controllers_status
     143.  00:91F4                      ;;=====================================================	
     144.  00:91F4                      ;depends on the next rountine
     145.  00:91F4                      ;  returns
     146.  00:91F4                      ;  a - 0P2TRLDU
     147.  00:91F4                      
     148.  00:91F4                      update_controllers_status:
     149.  00:91F4  CD 3F 92                 call  check_8th_keyboard_row
