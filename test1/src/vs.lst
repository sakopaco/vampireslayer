      36:  00:75C9  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:75CA  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:75CD  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:75CE  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:75CF  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:75D1                      		; The bank has less than 256 samples
