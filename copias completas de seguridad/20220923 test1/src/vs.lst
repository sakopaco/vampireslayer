     199.  00:52C4  E6 BF                      and    10111111b        ;joy0
     200.  00:52C6                      
     201.  00:52C6  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:52C8                      
     203.  00:52C8  3E 0E                      ld    a,14
