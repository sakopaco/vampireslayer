     100:  00:80F2                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:80F2  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:80F5  B7                  		or	a				; If priority has bit 7 on...
     103:  00:80F6  F8                  		ret	m				; ...return
     104:  00:80F7                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:80F7  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:80FA  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:80FC  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:80FE  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:8101  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:8102  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:8104  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:8106                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:8106  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:8109  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:810A  23                  		inc	hl				; Increment pointer
     116:  00:810B                      		; --- Check if there's new tone on stream ---
     117:  00:810B  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:810D  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:810F                      		; --- Extract new tone from stream ---
     120:  00:810F  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:8110  23                  		inc	hl				; Increment pointer
     122:  00:8111  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:8112  23                  		inc	hl				; Increment pointer
     124:  00:8113  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:8117                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:8117  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:8119  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:811B                      		; --- Extract new noise from stream ---
     129:  00:811B  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:811C  23                  		inc	hl				; Increment pointer
     131:  00:811D  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:811F  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:8121  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:8124                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:8124  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:8127                      		; --- Extract volume ---
     137:  00:8127  79                  		ld	a,c				; a:=Control byte
     138:  00:8128  E6 0F               		and	$0F				; lower nibble
     139:  00:812A                      
     140:  00:812A                      		IF ( AYFXRELATIVE == 1 )
     141:  00:812A                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:812A                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:812A                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:812A                    ~ 			ld	d,0				; d:=0
     145:  00:812A                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:812A                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:812A                    ~ 			or	a				; If relative volume is zero...
     148:  00:812A                    ~ 		ENDIF
     149:  00:812A                      
     150:  00:812A  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:812D  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:812E                      		; -------------------------------------
     153:  00:812E                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:812E                      		; -------------------------------------
     155:  00:812E                      		; --- Set noise channel ---
     156:  00:812E  CB 79               		bit	7,c				; If noise is off...
     157:  00:8130  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
