      17:  00:7873                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7873                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7873  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7876  3E 01               		ld	a,1				; a:=0
      21:  00:7878  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:787B  3C                  		inc	a				; Starting channel (=1)
      23:  00:787C  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:787F                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:787F  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7881  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7884  C9                  		ret					; Return
