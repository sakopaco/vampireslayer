     125:  00:8295                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:8295  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:8297  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:8299                      		; --- Extract new noise from stream ---
     129:  00:8299  7E                  		ld	a,[hl]				; a:=New noise
