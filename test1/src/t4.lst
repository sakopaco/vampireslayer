      35:  00:90CC                      		; --- Check if the index is in the bank ---
      36:  00:90CC  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:90CD  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:90D0  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:90D1  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:90D2  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:90D4                      		; The bank has less than 256 samples
