     199.  00:4FCD  E6 BF                      and    10111111b        ;joy0
     200.  00:4FCF                      
     201.  00:4FCF  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:4FD1                      
     203.  00:4FD1  3E 0E                      ld    a,14
     204.  00:4FD3  D3 A0                      out    [PSG_A],a
     205.  00:4FD5  DB A2                      in    a,[PSG_R]
