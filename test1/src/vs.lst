      75:  00:8256                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:8256  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:825A  13                  		inc	de				; de points to the increments table of the bank
      78:  00:825B  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:825C  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:825E  29                  		add	hl,hl				; hl:=hl*2
