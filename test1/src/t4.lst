     182.  00:8998  47                       ld    b,a    		;save for later  xxxTRDUL
     183.  00:8999                      
     184.  00:8999  E6 01                    and    #01    		;save L
     185.  00:899B  CB 27                    sla    a
     186.  00:899D  CB 27                    sla    a    		;now 00000L00
     187.  00:899F  4F                       ld    c,a    		;save for later
     188.  00:89A0                      
     189.  00:89A0  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     190.  00:89A1  CB 3F                    srl    a
     191.  00:89A3  E6 03                    and    00000011b
     192.  00:89A5                      						;we've got 000000DU on a
     193.  00:89A5  B1                       or    c    		;now we've got 00000LDU
     194.  00:89A6  4F                       ld    c,a    		;save for later
     195.  00:89A7                      
     196.  00:89A7  78                       ld    a,b
     197.  00:89A8  E6 18                    and    00011000b 	;we've got 000TR000
     198.  00:89AA  B1                       or    c
     199.  00:89AB                           ;finally    000TRLDU
     200.  00:89AB                      
     201.  00:89AB                           ;and    00000100   ;preserve just
     202.  00:89AB  47                       ld    b,a    		;save for later
     203.  00:89AC  CD E6 89                 call  check_4th_keyboard_row      ;check M & P
     204.  00:89AF  F5                       push  af
     205.  00:89B0  E6 04                    and    00000100b	;filter just M
     206.  00:89B2  87                       add    a
     207.  00:89B3  87                       add    a
     208.  00:89B4  87                       add    a           ;shift 3th to 6th bit
     209.  00:89B5  B0                       or    b           	;merge with previous
     210.  00:89B6  47                       ld    b,a
     211.  00:89B7                      
     212.  00:89B7  CD C4 89                 call  read_joy_0
     213.  00:89BA  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     214.  00:89BC  B0                       or    b      		;added 000TRLDU from keyboard
     215.  00:89BD  47                       ld    b,a      	;save for later
     216.  00:89BE  F1                       pop    af      	; let's check on P
     217.  00:89BF                      
     218.  00:89BF  E6 20                    and    00100000b
     219.  00:89C1  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     220.  00:89C2  B0                       or    b
     221.  00:89C3                      
