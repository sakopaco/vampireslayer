     233.  00:A2C6  DD 6E 0B            	LD		 L, (IX + ESTRUCTURA_AYUDA.pos_en_tilemap)
     234.  00:A2C9  DD 66 0C            	LD		 H, (IX + ESTRUCTURA_AYUDA.pos_en_tilemap + 1)
     235.  00:A2CC  22 F7 85            	LD		(wordaux1), HL		;pongo el valor en wordaux1
