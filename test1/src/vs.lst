     149.  00:5170  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:5171                      
     151.  00:5171  E6 01                    and    #01    		;save L
     152.  00:5173  CB 27                    sla    a
     153.  00:5175  CB 27                    sla    a    		;now 00000L00
     154.  00:5177  4F                       ld    c,a    		;save for later
     155.  00:5178                      
     156.  00:5178  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:5179  CB 3F                    srl    a
     158.  00:517B  E6 03                    and    00000011b
     159.  00:517D                      						;we've got 000000DU on a
     160.  00:517D  B1                       or    c    		;now we've got 00000LDU
     161.  00:517E  4F                       ld    c,a    		;save for later
     162.  00:517F                      
     163.  00:517F  78                       ld    a,b
     164.  00:5180  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5182  B1                       or    c
