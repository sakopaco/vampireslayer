     174.  00:8479                      update_controllers_status:
     175.  00:8479  CD C4 84                 call  check_8th_keyboard_row
     176.  00:847C                      [4]  rrc    a
     176.  00:847C  CB 0F CB 0F CB 0F CB 0F 
     177.  00:8484  47                       ld    b,a    		;save for later  xxxTRDUL
     178.  00:8485                      
     179.  00:8485  E6 01                    and    #01    		;save L
     180.  00:8487  CB 27                    sla    a
     181.  00:8489  CB 27                    sla    a    		;now 00000L00
     182.  00:848B  4F                       ld    c,a    		;save for later
     183.  00:848C                      
     184.  00:848C  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     185.  00:848D  CB 3F                    srl    a
     186.  00:848F  E6 03                    and    00000011b
     187.  00:8491                      						;we've got 000000DU on a
     188.  00:8491  B1                       or    c    		;now we've got 00000LDU
     189.  00:8492  4F                       ld    c,a    		;save for later
     190.  00:8493                      
     191.  00:8493  78                       ld    a,b
     192.  00:8494  E6 18                    and    00011000b 	;we've got 000TR000
     193.  00:8496  B1                       or    c
     194.  00:8497                           ;finally    000TRLDU
     195.  00:8497                      
     196.  00:8497                           ;and    00000100   ;preserve just
     197.  00:8497  47                       ld    b,a    		;save for later
     198.  00:8498  CD D2 84                 call  check_4th_keyboard_row      ;check M & P
     199.  00:849B  F5                       push  af
     200.  00:849C  E6 04                    and    00000100b	;filter just M
     201.  00:849E  87                       add    a
     202.  00:849F  87                       add    a
     203.  00:84A0  87                       add    a           ;shift 3th to 6th bit
     204.  00:84A1  B0                       or    b           	;merge with previous
     205.  00:84A2  47                       ld    b,a
     206.  00:84A3                      
     207.  00:84A3  CD B0 84                 call  read_joy_0
     208.  00:84A6  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     209.  00:84A8  B0                       or    b      		;added 000TRLDU from keyboard
     210.  00:84A9  47                       ld    b,a      	;save for later
     211.  00:84AA  F1                       pop    af      	; let's check on P
     212.  00:84AB                      
     213.  00:84AB  E6 20                    and    00100000b
     214.  00:84AD  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     215.  00:84AE  B0                       or    b
