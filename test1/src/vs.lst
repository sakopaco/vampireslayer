      17:  00:79F5                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:79F5                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:79F5  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
