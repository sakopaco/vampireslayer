      93:  00:7E43                      		IF ( AYFXRELATIVE == 1 )
      94:  00:7E43                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:7E43                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:7E43                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:7E43                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:7E43                    ~ 		ENDIF
