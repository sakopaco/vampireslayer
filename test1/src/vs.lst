     150.  00:8DDD                      
     151.  00:8DDD  E6 01                    and    #01    		;save L
     152.  00:8DDF  CB 27                    sla    a
     153.  00:8DE1  CB 27                    sla    a    		;now 00000L00
     154.  00:8DE3  4F                       ld    c,a    		;save for later
     155.  00:8DE4                      
     156.  00:8DE4  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:8DE5  CB 3F                    srl    a
     158.  00:8DE7  E6 03                    and    00000011b
     159.  00:8DE9                      						;we've got 000000DU on a
     160.  00:8DE9  B1                       or    c    		;now we've got 00000LDU
     161.  00:8DEA  4F                       ld    c,a    		;save for later
     162.  00:8DEB                      
     163.  00:8DEB  78                       ld    a,b
     164.  00:8DEC  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:8DEE  B1                       or    c
