     180:  00:92B8  C9                  		ret					; Return
     181:  00:92B9                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:92B9  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:92BB  0F                  		rrca					; Rotate right OR mask
     184:  00:92BC  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:92BE                      .PLAY_B:	; --- Play ayFX stream on channel B ---
