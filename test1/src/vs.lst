     161.  00:53F2  4F                       ld    c,a    		;save for later
     162.  00:53F3                      
     163.  00:53F3  78                       ld    a,b
     164.  00:53F4  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:53F6  B1                       or    c
