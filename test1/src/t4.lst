     163:  00:929F  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:92A1  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:92A2                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:92A2  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:92A3  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:92A4  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:92A6                      		; --- Dump to correct channel ---
