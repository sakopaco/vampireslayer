     220.  00:509F  DD 6E 0B            	LD		 L, (IX + ESTRUCTURA_AYUDA.pos_en_tilemap)
     221.  00:50A2  DD 66 0C            	LD		 H, (IX + ESTRUCTURA_AYUDA.pos_en_tilemap + 1)
     222.  00:50A5  22 DE C1            	LD		(wordaux1), HL		;pongo el valor en wordaux1
