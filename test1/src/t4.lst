      36:  00:9474  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:9475  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:9478  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:9479  B7                  		or	a				; If zero (means 256 samples)...
