     152.  00:9066                      
     153.  00:9066  E6 01                    and    #01    		;save L
     154.  00:9068  CB 27                    sla    a
     155.  00:906A  CB 27                    sla    a    		;now 00000L00
     156.  00:906C  4F                       ld    c,a    		;save for later
     157.  00:906D                      
     158.  00:906D  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     159.  00:906E  CB 3F                    srl    a
