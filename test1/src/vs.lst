     183:  00:82D6  0F                  		rrca					; Rotate right OR mask
     184:  00:82D7  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:82D9                      .PLAY_B:	; --- Play ayFX stream on channel B ---
