      29:  00:6C92                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:6C92                      		; --- INPUT: A -> sound to be played ---
      31:  00:6C92                      		; ---        C -> sound priority     ---
      32:  00:6C92  C5                  		push	bc				; Store bc in stack
      33:  00:6C93  D5                  		push	de				; Store de in stack
      34:  00:6C94  E5                  		push	hl				; Store hl in stack
      35:  00:6C95                      		; --- Check if the index is in the bank ---
      36:  00:6C95  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:6C96  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:6C99  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:6C9A  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:6C9B  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:6C9D                      		; The bank has less than 256 samples
      42:  00:6C9D  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:6C9E  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:6C9F  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:6CA1  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:6CA3                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:6CA3                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:6CA3  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:6CA4  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:6CA7  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:6CA8  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:6CAA  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:6CAC                      		; --- Set new priority ---
      54:  00:6CAC  79                  		ld	a,c				; a:=New priority
      55:  00:6CAD  E6 0F               		and	$0F				; We mask the priority
      56:  00:6CAF  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:6CB2                      
      58:  00:6CB2                      		IF ( AYFXRELATIVE == 1 )
      59:  00:6CB2                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:6CB2                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:6CB2                    ~ 			ld	a,15				; a:=15
      62:  00:6CB2                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:6CB2                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:6CB2                    ~ 			add	a,a				; a:=a*2
      65:  00:6CB2                    ~ 			add	a,a				; a:=a*4
      66:  00:6CB2                    ~ 			add	a,a				; a:=a*8
      67:  00:6CB2                    ~ 			add	a,a				; a:=a*16
      68:  00:6CB2                    ~ 			ld	e,a				; e:=a
      69:  00:6CB2                    ~ 			ld	d,0				; de:=a
      70:  00:6CB2                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:6CB2                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:6CB2                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:6CB2                    ~ 		ENDIF
