      17:  00:90B7                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:90B7                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:90B7  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:90BA  3E 01               		ld	a,1				; a:=0
      21:  00:90BC  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
