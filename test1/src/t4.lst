      75:  00:9705                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:9705  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:9709  13                  		inc	de				; de points to the increments table of the bank
      78:  00:970A  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:970B  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:970D  29                  		add	hl,hl				; hl:=hl*2
      81:  00:970E  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:970F  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:9710  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:9711  56                  		ld	d,[hl]				; de:=increment
      85:  00:9712  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:9713  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:9716  AF                  		xor	a				; a:=0 (no errors)
      88:  00:9717  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:9718  D1                  		pop	de				; Retrieve de from stack
      90:  00:9719  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:971A  C9                  		ret					; Return
      92:  00:971B                      
      93:  00:971B                      		IF ( AYFXRELATIVE == 1 )
      94:  00:971B                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:971B                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:971B                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:971B                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:971B                    ~ 		ENDIF
