     100:  00:7BB4                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:7BB4  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
