      75:  00:825A                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:825A  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:825E  13                  		inc	de				; de points to the increments table of the bank
      78:  00:825F  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:8260  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
