     175:  00:A060  CD 8F A0            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:A063  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:A066  CB 51               		bit	2,c				; If tone is off...
     178:  00:A068  C0                  		ret	nz				; ...return
     179:  00:A069  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:A06C  C9                  		ret					; Return
     181:  00:A06D                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:A06D  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:A06F  0F                  		rrca					; Rotate right OR mask
     184:  00:A070  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:A072                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:A072  CD 8F A0            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:A075  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:A078  CB 49               		bit	1,c				; If tone is off...
     189:  00:A07A  C0                  		ret	nz				; ...return
     190:  00:A07B  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:A07E  C9                  		ret					; Return
     192:  00:A07F                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:A07F  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:A081  0F                  		rrca					; Rotate right OR mask
     195:  00:A082                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:A082  CD 8F A0            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:A085  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:A088  CB 41               		bit	0,c				; If tone is off...
     199:  00:A08A  C0                  		ret	nz				; ...return
     200:  00:A08B  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:A08E  C9                  		ret					; Return
     202:  00:A08F                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:A08F  4F                  		ld	c,a				; c:=OR mask
     204:  00:A090  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
