     174.  00:9217  E6 04                    and    00000100b	;filter just M
     175.  00:9219  87                       add    a
     176.  00:921A  87                       add    a
     177.  00:921B  87                       add    a           ;shift 3th to 6th bit
     178.  00:921C  B0                       or    b           	;merge with previous
     179.  00:921D  47                       ld    b,a
     180.  00:921E                      
     181.  00:921E  CD 2B 92                 call  read_joy_0
