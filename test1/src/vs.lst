      17:  00:7B6C                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7B6C                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7B6C  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7B6F  3E 01               		ld	a,1				; a:=0
      21:  00:7B71  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7B74  3C                  		inc	a				; Starting channel (=1)
      23:  00:7B75  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7B78                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7B78  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7B7A  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
