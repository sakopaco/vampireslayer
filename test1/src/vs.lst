     134:  00:82A2                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:82A2  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:82A5                      		; --- Extract volume ---
