     100:  00:7AFF                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:7AFF  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:7B02  B7                  		or	a				; If priority has bit 7 on...
     103:  00:7B03  F8                  		ret	m				; ...return
     104:  00:7B04                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:7B04  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:7B07  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:7B09  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:7B0B  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:7B0E  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:7B0F  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:7B11  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:7B13                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:7B13  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:7B16  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:7B17  23                  		inc	hl				; Increment pointer
     116:  00:7B18                      		; --- Check if there's new tone on stream ---
     117:  00:7B18  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:7B1A  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:7B1C                      		; --- Extract new tone from stream ---
     120:  00:7B1C  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:7B1D  23                  		inc	hl				; Increment pointer
     122:  00:7B1E  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:7B1F  23                  		inc	hl				; Increment pointer
     124:  00:7B20  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:7B24                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:7B24  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:7B26  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:7B28                      		; --- Extract new noise from stream ---
     129:  00:7B28  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:7B29  23                  		inc	hl				; Increment pointer
     131:  00:7B2A  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:7B2C  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:7B2E  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:7B31                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:7B31  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:7B34                      		; --- Extract volume ---
     137:  00:7B34  79                  		ld	a,c				; a:=Control byte
     138:  00:7B35  E6 0F               		and	$0F				; lower nibble
     139:  00:7B37                      
     140:  00:7B37                      		IF ( AYFXRELATIVE == 1 )
     141:  00:7B37                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:7B37                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:7B37                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:7B37                    ~ 			ld	d,0				; d:=0
     145:  00:7B37                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:7B37                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:7B37                    ~ 			or	a				; If relative volume is zero...
     148:  00:7B37                    ~ 		ENDIF
     149:  00:7B37                      
     150:  00:7B37  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:7B3A  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:7B3B                      		; -------------------------------------
     153:  00:7B3B                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:7B3B                      		; -------------------------------------
     155:  00:7B3B                      		; --- Set noise channel ---
     156:  00:7B3B  CB 79               		bit	7,c				; If noise is off...
     157:  00:7B3D  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:7B3F  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:7B42  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:7B45                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:7B45  79                  		ld	a,c				; a:=Control byte
