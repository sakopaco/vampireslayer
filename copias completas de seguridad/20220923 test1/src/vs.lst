     140.  00:5286                      ;;update_controllers_status
     141.  00:5286                      ;;=====================================================	
     142.  00:5286                      ;depends on the next rountine
     143.  00:5286                      ;  returns
     144.  00:5286                      ;  a - 0P2TRLDU
     145.  00:5286                      
     146.  00:5286                      update_controllers_status:
     147.  00:5286  CD D1 52                 call  check_8th_keyboard_row
     148.  00:5289                      [4]  rrc    a
     148.  00:5289  CB 0F CB 0F CB 0F CB 0F 
     149.  00:5291  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:5292                      
     151.  00:5292  E6 01                    and    #01    		;save L
     152.  00:5294  CB 27                    sla    a
     153.  00:5296  CB 27                    sla    a    		;now 00000L00
     154.  00:5298  4F                       ld    c,a    		;save for later
     155.  00:5299                      
     156.  00:5299  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:529A  CB 3F                    srl    a
     158.  00:529C  E6 03                    and    00000011b
     159.  00:529E                      						;we've got 000000DU on a
     160.  00:529E  B1                       or    c    		;now we've got 00000LDU
     161.  00:529F  4F                       ld    c,a    		;save for later
     162.  00:52A0                      
     163.  00:52A0  78                       ld    a,b
     164.  00:52A1  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:52A3  B1                       or    c
     166.  00:52A4                           ;finally    000TRLDU
     167.  00:52A4                      
     168.  00:52A4                           ;and    00000100   ;preserve just
     169.  00:52A4  47                       ld    b,a    		;save for later
     170.  00:52A5  CD DF 52                 call  check_4th_keyboard_row      ;check M & P
     171.  00:52A8  F5                       push  af
     172.  00:52A9  E6 04                    and    00000100b	;filter just M
     173.  00:52AB  87                       add    a
     174.  00:52AC  87                       add    a
     175.  00:52AD  87                       add    a           ;shift 3th to 6th bit
     176.  00:52AE  B0                       or    b           	;merge with previous
     177.  00:52AF  47                       ld    b,a
     178.  00:52B0                      
     179.  00:52B0  CD BD 52                 call  read_joy_0
     180.  00:52B3  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:52B5  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:52B6  47                       ld    b,a      	;save for later
     183.  00:52B7  F1                       pop    af      	; let's check on P
     184.  00:52B8                      
     185.  00:52B8  E6 20                    and    00100000b
     186.  00:52BA  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:52BB  B0                       or    b
     188.  00:52BC                      
     189.  00:52BC  C9                       ret
     190.  00:52BD                      
     191.  00:52BD                      
     192.  00:52BD                      ;ret
     193.  00:52BD                      ;  a -            xx2TRLDU
