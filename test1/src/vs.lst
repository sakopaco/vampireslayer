     194.  00:53F3                      read_joy_0:
     195.  00:53F3  F3                         di
     196.  00:53F4  3E 0F                      ld    a,15
     197.  00:53F6  D3 A0                      out    [PSG_A],a
     198.  00:53F8  DB A2                      in    a,[PSG_R]
     199.  00:53FA  E6 BF                      and    10111111b        ;joy0
     200.  00:53FC                      
     201.  00:53FC  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:53FE                      
     203.  00:53FE  3E 0E                      ld    a,14
     204.  00:5400  D3 A0                      out    [PSG_A],a
     205.  00:5402  DB A2                      in    a,[PSG_R]
