      79:  00:9A35  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:9A37  29                  		add	hl,hl				; hl:=hl*2
      81:  00:9A38  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:9A39  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:9A3A  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:9A3B  56                  		ld	d,[hl]				; de:=increment
      85:  00:9A3C  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:9A3D  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:9A40  AF                  		xor	a				; a:=0 (no errors)
      88:  00:9A41  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:9A42  D1                  		pop	de				; Retrieve de from stack
      90:  00:9A43  C1                  		pop	bc				; Retrieve bc from stack
