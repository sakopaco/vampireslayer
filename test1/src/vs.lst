     140.  00:50C8                      ;;update_controllers_status
     141.  00:50C8                      ;;=====================================================	
     142.  00:50C8                      ;depends on the next rountine
     143.  00:50C8                      ;  returns
     144.  00:50C8                      ;  a - 0P2TRLDU
     145.  00:50C8                      
     146.  00:50C8                      update_controllers_status:
     147.  00:50C8  CD 13 51                 call  check_8th_keyboard_row
     148.  00:50CB                      [4]  rrc    a
     148.  00:50CB  CB 0F CB 0F CB 0F CB 0F 
     149.  00:50D3  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:50D4                      
     151.  00:50D4  E6 01                    and    #01    		;save L
     152.  00:50D6  CB 27                    sla    a
     153.  00:50D8  CB 27                    sla    a    		;now 00000L00
     154.  00:50DA  4F                       ld    c,a    		;save for later
     155.  00:50DB                      
     156.  00:50DB  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:50DC  CB 3F                    srl    a
     158.  00:50DE  E6 03                    and    00000011b
     159.  00:50E0                      						;we've got 000000DU on a
     160.  00:50E0  B1                       or    c    		;now we've got 00000LDU
     161.  00:50E1  4F                       ld    c,a    		;save for later
     162.  00:50E2                      
     163.  00:50E2  78                       ld    a,b
     164.  00:50E3  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:50E5  B1                       or    c
     166.  00:50E6                           ;finally    000TRLDU
     167.  00:50E6                      
     168.  00:50E6                           ;and    00000100   ;preserve just
     169.  00:50E6  47                       ld    b,a    		;save for later
     170.  00:50E7  CD 21 51                 call  check_4th_keyboard_row      ;check M & P
     171.  00:50EA  F5                       push  af
     172.  00:50EB  E6 04                    and    00000100b	;filter just M
     173.  00:50ED  87                       add    a
     174.  00:50EE  87                       add    a
     175.  00:50EF  87                       add    a           ;shift 3th to 6th bit
     176.  00:50F0  B0                       or    b           	;merge with previous
     177.  00:50F1  47                       ld    b,a
     178.  00:50F2                      
     179.  00:50F2  CD FF 50                 call  read_joy_0
     180.  00:50F5  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:50F7  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:50F8  47                       ld    b,a      	;save for later
     183.  00:50F9  F1                       pop    af      	; let's check on P
     184.  00:50FA                      
     185.  00:50FA  E6 20                    and    00100000b
     186.  00:50FC  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:50FD  B0                       or    b
     188.  00:50FE                      
