     195.  00:512C  F3                         di
     196.  00:512D  3E 0F                      ld    a,15
     197.  00:512F  D3 A0                      out    [PSG_A],a
     198.  00:5131  DB A2                      in    a,[PSG_R]
     199.  00:5133  E6 BF                      and    10111111b        ;joy0
     200.  00:5135                      
     201.  00:5135  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:5137                      
     203.  00:5137  3E 0E                      ld    a,14
