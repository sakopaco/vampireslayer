      93:  00:76B9                      		IF ( AYFXRELATIVE == 1 )
      94:  00:76B9                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:76B9                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:76B9                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:76B9                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:76B9                    ~ 		ENDIF
