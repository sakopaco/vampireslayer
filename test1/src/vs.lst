     148.  00:4F92                      [4]  rrc    a
     148.  00:4F92  CB 0F CB 0F CB 0F CB 0F 
     149.  00:4F9A  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:4F9B                      
     151.  00:4F9B  E6 01                    and    #01    		;save L
     152.  00:4F9D  CB 27                    sla    a
     153.  00:4F9F  CB 27                    sla    a    		;now 00000L00
     154.  00:4FA1  4F                       ld    c,a    		;save for later
     155.  00:4FA2                      
     156.  00:4FA2  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:4FA3  CB 3F                    srl    a
     158.  00:4FA5  E6 03                    and    00000011b
     159.  00:4FA7                      						;we've got 000000DU on a
     160.  00:4FA7  B1                       or    c    		;now we've got 00000LDU
     161.  00:4FA8  4F                       ld    c,a    		;save for later
     162.  00:4FA9                      
     163.  00:4FA9  78                       ld    a,b
     164.  00:4FAA  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:4FAC  B1                       or    c
     166.  00:4FAD                           ;finally    000TRLDU
     167.  00:4FAD                      
     168.  00:4FAD                           ;and    00000100   ;preserve just
     169.  00:4FAD  47                       ld    b,a    		;save for later
     170.  00:4FAE  CD E8 4F                 call  check_4th_keyboard_row      ;check M & P
     171.  00:4FB1  F5                       push  af
     172.  00:4FB2  E6 04                    and    00000100b	;filter just M
     173.  00:4FB4  87                       add    a
     174.  00:4FB5  87                       add    a
     175.  00:4FB6  87                       add    a           ;shift 3th to 6th bit
     176.  00:4FB7  B0                       or    b           	;merge with previous
     177.  00:4FB8  47                       ld    b,a
     178.  00:4FB9                      
     179.  00:4FB9  CD C6 4F                 call  read_joy_0
     180.  00:4FBC  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:4FBE  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:4FBF  47                       ld    b,a      	;save for later
     183.  00:4FC0  F1                       pop    af      	; let's check on P
     184.  00:4FC1                      
     185.  00:4FC1  E6 20                    and    00100000b
     186.  00:4FC3  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:4FC4  B0                       or    b
     188.  00:4FC5                      
     189.  00:4FC5  C9                       ret
