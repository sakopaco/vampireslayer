     150.  00:5410                      
     151.  00:5410  E6 01                    and    #01    		;save L
     152.  00:5412  CB 27                    sla    a
     153.  00:5414  CB 27                    sla    a    		;now 00000L00
     154.  00:5416  4F                       ld    c,a    		;save for later
     155.  00:5417                      
     156.  00:5417  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:5418  CB 3F                    srl    a
     158.  00:541A  E6 03                    and    00000011b
     159.  00:541C                      						;we've got 000000DU on a
