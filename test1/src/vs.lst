     195.  00:5401  F3                         di
     196.  00:5402  3E 0F                      ld    a,15
     197.  00:5404  D3 A0                      out    [PSG_A],a
     198.  00:5406  DB A2                      in    a,[PSG_R]
     199.  00:5408  E6 BF                      and    10111111b        ;joy0
     200.  00:540A                      
     201.  00:540A  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:540C                      
     203.  00:540C  3E 0E                      ld    a,14
     204.  00:540E  D3 A0                      out    [PSG_A],a
     205.  00:5410  DB A2                      in    a,[PSG_R]
     206.  00:5412                      
     207.  00:5412  FB                         ei
     208.  00:5413  2F                         cpl
