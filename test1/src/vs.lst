      17:  00:6F41                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:6F41                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:6F41  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:6F44  3E 01               		ld	a,1				; a:=0
      21:  00:6F46  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:6F49  3C                  		inc	a				; Starting channel (=1)
      23:  00:6F4A  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:6F4D                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:6F4D  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:6F4F  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:6F52  C9                  		ret					; Return
      28:  00:6F53                      
      29:  00:6F53                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:6F53                      		; --- INPUT: A -> sound to be played ---
      31:  00:6F53                      		; ---        C -> sound priority     ---
      32:  00:6F53  C5                  		push	bc				; Store bc in stack
      33:  00:6F54  D5                  		push	de				; Store de in stack
      34:  00:6F55  E5                  		push	hl				; Store hl in stack
      35:  00:6F56                      		; --- Check if the index is in the bank ---
      36:  00:6F56  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:6F57  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:6F5A  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:6F5B  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:6F5C  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:6F5E                      		; The bank has less than 256 samples
      42:  00:6F5E  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:6F5F  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:6F60  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:6F62  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:6F64                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:6F64                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:6F64  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:6F65  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:6F68  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:6F69  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:6F6B  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:6F6D                      		; --- Set new priority ---
      54:  00:6F6D  79                  		ld	a,c				; a:=New priority
      55:  00:6F6E  E6 0F               		and	$0F				; We mask the priority
      56:  00:6F70  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:6F73                      
      58:  00:6F73                      		IF ( AYFXRELATIVE == 1 )
      59:  00:6F73                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:6F73                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:6F73                    ~ 			ld	a,15				; a:=15
      62:  00:6F73                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:6F73                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:6F73                    ~ 			add	a,a				; a:=a*2
      65:  00:6F73                    ~ 			add	a,a				; a:=a*4
