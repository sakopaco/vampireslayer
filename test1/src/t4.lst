     160:  00:9BC3                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:9BC3  79                  		ld	a,c				; a:=Control byte
     162:  00:9BC4  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:9BC6  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:9BC8  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:9BC9                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:9BC9  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:9BCA  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:9BCB  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:9BCD                      		; --- Dump to correct channel ---
     170:  00:9BCD  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:9BD0  46                  		ld	b,[hl]				; Channel counter
     172:  00:9BD1                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:9BD1  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:9BD3                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:9BD3  CD 02 9C            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:9BD6  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:9BD9  CB 51               		bit	2,c				; If tone is off...
     178:  00:9BDB  C0                  		ret	nz				; ...return
     179:  00:9BDC  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:9BDF  C9                  		ret					; Return
     181:  00:9BE0                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:9BE0  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:9BE2  0F                  		rrca					; Rotate right OR mask
     184:  00:9BE3  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:9BE5                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:9BE5  CD 02 9C            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:9BE8  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:9BEB  CB 49               		bit	1,c				; If tone is off...
     189:  00:9BED  C0                  		ret	nz				; ...return
     190:  00:9BEE  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:9BF1  C9                  		ret					; Return
     192:  00:9BF2                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:9BF2  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:9BF4  0F                  		rrca					; Rotate right OR mask
     195:  00:9BF5                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:9BF5  CD 02 9C            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:9BF8  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:9BFB  CB 41               		bit	0,c				; If tone is off...
     199:  00:9BFD  C0                  		ret	nz				; ...return
     200:  00:9BFE  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:9C01  C9                  		ret					; Return
     202:  00:9C02                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:9C02  4F                  		ld	c,a				; c:=OR mask
     204:  00:9C03  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:9C06  A2                  		and	d				; AND mask
     206:  00:9C07  B1                  		or	c				; OR mask
     207:  00:9C08  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
     208:  00:9C0B  3A 49 F2            		ld	a,[ayFX_VOLUME]			; a:=ayFX volume value
     209:  00:9C0E  2A 46 F2            		ld	hl,[ayFX_TONE]			; ayFX tone value
     210:  00:9C11  C9                  		ret					; Return
