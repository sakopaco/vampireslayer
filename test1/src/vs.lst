      17:  00:7DFB                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7DFB                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7DFB  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7DFE  3E 01               		ld	a,1				; a:=0
      21:  00:7E00  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7E03  3C                  		inc	a				; Starting channel (=1)
      23:  00:7E04  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7E07                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7E07  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7E09  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7E0C  C9                  		ret					; Return
