      17:  00:7DDE                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7DDE                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7DDE  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7DE1  3E 01               		ld	a,1				; a:=0
      21:  00:7DE3  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7DE6  3C                  		inc	a				; Starting channel (=1)
      23:  00:7DE7  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7DEA                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7DEA  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7DEC  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7DEF  C9                  		ret					; Return
      28:  00:7DF0                      
      29:  00:7DF0                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7DF0                      		; --- INPUT: A -> sound to be played ---
      31:  00:7DF0                      		; ---        C -> sound priority     ---
      32:  00:7DF0  C5                  		push	bc				; Store bc in stack
      33:  00:7DF1  D5                  		push	de				; Store de in stack
      34:  00:7DF2  E5                  		push	hl				; Store hl in stack
      35:  00:7DF3                      		; --- Check if the index is in the bank ---
      36:  00:7DF3  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7DF4  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7DF7  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7DF8  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7DF9  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7DFB                      		; The bank has less than 256 samples
      42:  00:7DFB  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7DFC  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7DFD  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7DFF  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7E01                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7E01                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7E01  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7E02  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7E05  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7E06  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7E08  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7E0A                      		; --- Set new priority ---
      54:  00:7E0A  79                  		ld	a,c				; a:=New priority
      55:  00:7E0B  E6 0F               		and	$0F				; We mask the priority
      56:  00:7E0D  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7E10                      
      58:  00:7E10                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7E10                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7E10                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7E10                    ~ 			ld	a,15				; a:=15
      62:  00:7E10                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7E10                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7E10                    ~ 			add	a,a				; a:=a*2
      65:  00:7E10                    ~ 			add	a,a				; a:=a*4
      66:  00:7E10                    ~ 			add	a,a				; a:=a*8
      67:  00:7E10                    ~ 			add	a,a				; a:=a*16
      68:  00:7E10                    ~ 			ld	e,a				; e:=a
      69:  00:7E10                    ~ 			ld	d,0				; de:=a
      70:  00:7E10                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:7E10                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:7E10                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:7E10                    ~ 		ENDIF
      74:  00:7E10                      
      75:  00:7E10                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:7E10  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:7E14  13                  		inc	de				; de points to the increments table of the bank
      78:  00:7E15  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:7E16  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:7E18  29                  		add	hl,hl				; hl:=hl*2
      81:  00:7E19  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:7E1A  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:7E1B  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:7E1C  56                  		ld	d,[hl]				; de:=increment
      85:  00:7E1D  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:7E1E  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:7E21  AF                  		xor	a				; a:=0 (no errors)
      88:  00:7E22  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:7E23  D1                  		pop	de				; Retrieve de from stack
      90:  00:7E24  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:7E25  C9                  		ret					; Return
      92:  00:7E26                      
      93:  00:7E26                      		IF ( AYFXRELATIVE == 1 )
      94:  00:7E26                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:7E26                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:7E26                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:7E26                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:7E26                    ~ 		ENDIF
      99:  00:7E26                      
     100:  00:7E26                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:7E26  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:7E29  B7                  		or	a				; If priority has bit 7 on...
     103:  00:7E2A  F8                  		ret	m				; ...return
     104:  00:7E2B                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:7E2B  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:7E2E  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:7E30  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:7E32  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:7E35  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:7E36  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:7E38  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:7E3A                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:7E3A  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:7E3D  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:7E3E  23                  		inc	hl				; Increment pointer
     116:  00:7E3F                      		; --- Check if there's new tone on stream ---
     117:  00:7E3F  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:7E41  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:7E43                      		; --- Extract new tone from stream ---
     120:  00:7E43  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:7E44  23                  		inc	hl				; Increment pointer
     122:  00:7E45  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:7E46  23                  		inc	hl				; Increment pointer
     124:  00:7E47  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:7E4B                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:7E4B  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:7E4D  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:7E4F                      		; --- Extract new noise from stream ---
     129:  00:7E4F  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:7E50  23                  		inc	hl				; Increment pointer
     131:  00:7E51  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:7E53  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:7E55  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:7E58                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:7E58  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:7E5B                      		; --- Extract volume ---
     137:  00:7E5B  79                  		ld	a,c				; a:=Control byte
     138:  00:7E5C  E6 0F               		and	$0F				; lower nibble
     139:  00:7E5E                      
     140:  00:7E5E                      		IF ( AYFXRELATIVE == 1 )
     141:  00:7E5E                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:7E5E                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:7E5E                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:7E5E                    ~ 			ld	d,0				; d:=0
     145:  00:7E5E                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:7E5E                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:7E5E                    ~ 			or	a				; If relative volume is zero...
     148:  00:7E5E                    ~ 		ENDIF
     149:  00:7E5E                      
     150:  00:7E5E  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:7E61  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:7E62                      		; -------------------------------------
     153:  00:7E62                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:7E62                      		; -------------------------------------
     155:  00:7E62                      		; --- Set noise channel ---
     156:  00:7E62  CB 79               		bit	7,c				; If noise is off...
     157:  00:7E64  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:7E66  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:7E69  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:7E6C                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:7E6C  79                  		ld	a,c				; a:=Control byte
     162:  00:7E6D  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:7E6F  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:7E71  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:7E72                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:7E72  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:7E73  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:7E74  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:7E76                      		; --- Dump to correct channel ---
     170:  00:7E76  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:7E79  46                  		ld	b,[hl]				; Channel counter
     172:  00:7E7A                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:7E7A  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:7E7C                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:7E7C  CD AB 7E            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:7E7F  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
