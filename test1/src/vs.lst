     161.  00:56AA  4F                       ld    c,a    		;save for later
     162.  00:56AB                      
     163.  00:56AB  78                       ld    a,b
     164.  00:56AC  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:56AE  B1                       or    c
