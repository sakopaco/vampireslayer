     112:  00:8F65                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:8F65  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:8F68  4E                  		ld	c,[hl]				; c:=Control byte
