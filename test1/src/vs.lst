      92:  00:7E8F                      
      93:  00:7E8F                      		IF ( AYFXRELATIVE == 1 )
      94:  00:7E8F                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:7E8F                    ~ 			ld	a,255				; Lowest ayFX priority
