     146.  00:510C                      update_controllers_status:
     147.  00:510C  CD 57 51                 call  check_8th_keyboard_row
     148.  00:510F                      [4]  rrc    a
     148.  00:510F  CB 0F CB 0F CB 0F CB 0F 
     149.  00:5117  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:5118                      
     151.  00:5118  E6 01                    and    #01    		;save L
     152.  00:511A  CB 27                    sla    a
     153.  00:511C  CB 27                    sla    a    		;now 00000L00
     154.  00:511E  4F                       ld    c,a    		;save for later
     155.  00:511F                      
     156.  00:511F  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:5120  CB 3F                    srl    a
     158.  00:5122  E6 03                    and    00000011b
     159.  00:5124                      						;we've got 000000DU on a
     160.  00:5124  B1                       or    c    		;now we've got 00000LDU
     161.  00:5125  4F                       ld    c,a    		;save for later
     162.  00:5126                      
     163.  00:5126  78                       ld    a,b
     164.  00:5127  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5129  B1                       or    c
     166.  00:512A                           ;finally    000TRLDU
     167.  00:512A                      
     168.  00:512A                           ;and    00000100   ;preserve just
     169.  00:512A  47                       ld    b,a    		;save for later
     170.  00:512B  CD 65 51                 call  check_4th_keyboard_row      ;check M & P
     171.  00:512E  F5                       push  af
     172.  00:512F  E6 04                    and    00000100b	;filter just M
     173.  00:5131  87                       add    a
     174.  00:5132  87                       add    a
     175.  00:5133  87                       add    a           ;shift 3th to 6th bit
     176.  00:5134  B0                       or    b           	;merge with previous
     177.  00:5135  47                       ld    b,a
     178.  00:5136                      
     179.  00:5136  CD 43 51                 call  read_joy_0
     180.  00:5139  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:513B  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:513C  47                       ld    b,a      	;save for later
     183.  00:513D  F1                       pop    af      	; let's check on P
     184.  00:513E                      
     185.  00:513E  E6 20                    and    00100000b
     186.  00:5140  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:5141  B0                       or    b
     188.  00:5142                      
     189.  00:5142  C9                       ret
     190.  00:5143                      
     191.  00:5143                      
     192.  00:5143                      ;ret
