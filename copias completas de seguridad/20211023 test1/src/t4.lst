      37:  00:8F68  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:8F6B  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:8F6C  B7                  		or	a				; If zero (means 256 samples)...
