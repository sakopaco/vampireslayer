     151.  00:5221  E6 01                    and    #01    		;save L
     152.  00:5223  CB 27                    sla    a
     153.  00:5225  CB 27                    sla    a    		;now 00000L00
     154.  00:5227  4F                       ld    c,a    		;save for later
     155.  00:5228                      
     156.  00:5228  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:5229  CB 3F                    srl    a
     158.  00:522B  E6 03                    and    00000011b
     159.  00:522D                      						;we've got 000000DU on a
     160.  00:522D  B1                       or    c    		;now we've got 00000LDU
     161.  00:522E  4F                       ld    c,a    		;save for later
     162.  00:522F                      
     163.  00:522F  78                       ld    a,b
     164.  00:5230  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5232  B1                       or    c
