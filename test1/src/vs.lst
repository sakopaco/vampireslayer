      29:  00:813C                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:813C                      		; --- INPUT: A -> sound to be played ---
      31:  00:813C                      		; ---        C -> sound priority     ---
      32:  00:813C  C5                  		push	bc				; Store bc in stack
