     100:  00:7E21                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:7E21  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:7E24  B7                  		or	a				; If priority has bit 7 on...
     103:  00:7E25  F8                  		ret	m				; ...return
     104:  00:7E26                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:7E26  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:7E29  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:7E2B  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:7E2D  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:7E30  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:7E31  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:7E33  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:7E35                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:7E35  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:7E38  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:7E39  23                  		inc	hl				; Increment pointer
     116:  00:7E3A                      		; --- Check if there's new tone on stream ---
     117:  00:7E3A  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:7E3C  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:7E3E                      		; --- Extract new tone from stream ---
     120:  00:7E3E  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:7E3F  23                  		inc	hl				; Increment pointer
     122:  00:7E40  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:7E41  23                  		inc	hl				; Increment pointer
     124:  00:7E42  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:7E46                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:7E46  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:7E48  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:7E4A                      		; --- Extract new noise from stream ---
     129:  00:7E4A  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:7E4B  23                  		inc	hl				; Increment pointer
     131:  00:7E4C  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:7E4E  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:7E50  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:7E53                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:7E53  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:7E56                      		; --- Extract volume ---
     137:  00:7E56  79                  		ld	a,c				; a:=Control byte
     138:  00:7E57  E6 0F               		and	$0F				; lower nibble
     139:  00:7E59                      
     140:  00:7E59                      		IF ( AYFXRELATIVE == 1 )
     141:  00:7E59                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:7E59                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:7E59                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:7E59                    ~ 			ld	d,0				; d:=0
     145:  00:7E59                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:7E59                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:7E59                    ~ 			or	a				; If relative volume is zero...
     148:  00:7E59                    ~ 		ENDIF
     149:  00:7E59                      
     150:  00:7E59  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:7E5C  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:7E5D                      		; -------------------------------------
     153:  00:7E5D                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:7E5D                      		; -------------------------------------
     155:  00:7E5D                      		; --- Set noise channel ---
     156:  00:7E5D  CB 79               		bit	7,c				; If noise is off...
     157:  00:7E5F  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:7E61  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:7E64  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:7E67                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:7E67  79                  		ld	a,c				; a:=Control byte
     162:  00:7E68  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:7E6A  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:7E6C  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:7E6D                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:7E6D  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:7E6E  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:7E6F  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:7E71                      		; --- Dump to correct channel ---
     170:  00:7E71  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:7E74  46                  		ld	b,[hl]				; Channel counter
     172:  00:7E75                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:7E75  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:7E77                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:7E77  CD A6 7E            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:7E7A  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:7E7D  CB 51               		bit	2,c				; If tone is off...
     178:  00:7E7F  C0                  		ret	nz				; ...return
     179:  00:7E80  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:7E83  C9                  		ret					; Return
     181:  00:7E84                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:7E84  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:7E86  0F                  		rrca					; Rotate right OR mask
     184:  00:7E87  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:7E89                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:7E89  CD A6 7E            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:7E8C  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:7E8F  CB 49               		bit	1,c				; If tone is off...
     189:  00:7E91  C0                  		ret	nz				; ...return
     190:  00:7E92  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:7E95  C9                  		ret					; Return
     192:  00:7E96                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:7E96  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:7E98  0F                  		rrca					; Rotate right OR mask
     195:  00:7E99                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:7E99  CD A6 7E            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:7E9C  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:7E9F  CB 41               		bit	0,c				; If tone is off...
     199:  00:7EA1  C0                  		ret	nz				; ...return
     200:  00:7EA2  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:7EA5  C9                  		ret					; Return
     202:  00:7EA6                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:7EA6  4F                  		ld	c,a				; c:=OR mask
