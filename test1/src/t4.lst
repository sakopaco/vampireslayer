      29:  00:960F                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:960F                      		; --- INPUT: A -> sound to be played ---
      31:  00:960F                      		; ---        C -> sound priority     ---
      32:  00:960F  C5                  		push	bc				; Store bc in stack
      33:  00:9610  D5                  		push	de				; Store de in stack
      34:  00:9611  E5                  		push	hl				; Store hl in stack
      35:  00:9612                      		; --- Check if the index is in the bank ---
      36:  00:9612  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:9613  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:9616  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:9617  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:9618  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:961A                      		; The bank has less than 256 samples
      42:  00:961A  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:961B  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:961C  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:961E  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:9620                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:9620                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:9620  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:9621  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:9624  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:9625  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:9627  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:9629                      		; --- Set new priority ---
      54:  00:9629  79                  		ld	a,c				; a:=New priority
      55:  00:962A  E6 0F               		and	$0F				; We mask the priority
      56:  00:962C  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:962F                      
      58:  00:962F                      		IF ( AYFXRELATIVE == 1 )
      59:  00:962F                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:962F                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:962F                    ~ 			ld	a,15				; a:=15
      62:  00:962F                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:962F                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:962F                    ~ 			add	a,a				; a:=a*2
      65:  00:962F                    ~ 			add	a,a				; a:=a*4
      66:  00:962F                    ~ 			add	a,a				; a:=a*8
      67:  00:962F                    ~ 			add	a,a				; a:=a*16
