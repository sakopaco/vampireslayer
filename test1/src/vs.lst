     148.  00:5091                      [4]  rrc    a
     148.  00:5091  CB 0F CB 0F CB 0F CB 0F 
     149.  00:5099  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:509A                      
     151.  00:509A  E6 01                    and    #01    		;save L
     152.  00:509C  CB 27                    sla    a
     153.  00:509E  CB 27                    sla    a    		;now 00000L00
     154.  00:50A0  4F                       ld    c,a    		;save for later
     155.  00:50A1                      
     156.  00:50A1  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:50A2  CB 3F                    srl    a
     158.  00:50A4  E6 03                    and    00000011b
     159.  00:50A6                      						;we've got 000000DU on a
     160.  00:50A6  B1                       or    c    		;now we've got 00000LDU
     161.  00:50A7  4F                       ld    c,a    		;save for later
     162.  00:50A8                      
     163.  00:50A8  78                       ld    a,b
     164.  00:50A9  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:50AB  B1                       or    c
     166.  00:50AC                           ;finally    000TRLDU
     167.  00:50AC                      
     168.  00:50AC                           ;and    00000100   ;preserve just
     169.  00:50AC  47                       ld    b,a    		;save for later
     170.  00:50AD  CD E7 50                 call  check_4th_keyboard_row      ;check M & P
     171.  00:50B0  F5                       push  af
     172.  00:50B1  E6 04                    and    00000100b	;filter just M
     173.  00:50B3  87                       add    a
     174.  00:50B4  87                       add    a
     175.  00:50B5  87                       add    a           ;shift 3th to 6th bit
     176.  00:50B6  B0                       or    b           	;merge with previous
     177.  00:50B7  47                       ld    b,a
     178.  00:50B8                      
     179.  00:50B8  CD C5 50                 call  read_joy_0
     180.  00:50BB  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:50BD  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:50BE  47                       ld    b,a      	;save for later
     183.  00:50BF  F1                       pop    af      	; let's check on P
     184.  00:50C0                      
     185.  00:50C0  E6 20                    and    00100000b
     186.  00:50C2  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:50C3  B0                       or    b
     188.  00:50C4                      
     189.  00:50C4  C9                       ret
