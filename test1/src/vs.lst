     167.  00:56AE                      
     168.  00:56AE                           ;and    00000100   ;preserve just
     169.  00:56AE  47                       ld    b,a    		;save for later
     170.  00:56AF  CD E9 56                 call  check_4th_keyboard_row      ;check M & P
     171.  00:56B2  F5                       push  af
     172.  00:56B3  E6 04                    and    00000100b	;filter just M
     173.  00:56B5  87                       add    a
     174.  00:56B6  87                       add    a
     175.  00:56B7  87                       add    a           ;shift 3th to 6th bit
     176.  00:56B8  B0                       or    b           	;merge with previous
     177.  00:56B9  47                       ld    b,a
     178.  00:56BA                      
     179.  00:56BA  CD C7 56                 call  read_joy_0
