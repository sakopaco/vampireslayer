     177:  00:7AE0  CB 51               		bit	2,c				; If tone is off...
     178:  00:7AE2  C0                  		ret	nz				; ...return
     179:  00:7AE3  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:7AE6  C9                  		ret					; Return
     181:  00:7AE7                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:7AE7  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:7AE9  0F                  		rrca					; Rotate right OR mask
     184:  00:7AEA  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:7AEC                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:7AEC  CD 09 7B            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:7AEF  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:7AF2  CB 49               		bit	1,c				; If tone is off...
     189:  00:7AF4  C0                  		ret	nz				; ...return
     190:  00:7AF5  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:7AF8  C9                  		ret					; Return
     192:  00:7AF9                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:7AF9  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:7AFB  0F                  		rrca					; Rotate right OR mask
     195:  00:7AFC                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:7AFC  CD 09 7B            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:7AFF  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:7B02  CB 41               		bit	0,c				; If tone is off...
     199:  00:7B04  C0                  		ret	nz				; ...return
     200:  00:7B05  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:7B08  C9                  		ret					; Return
     202:  00:7B09                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:7B09  4F                  		ld	c,a				; c:=OR mask
     204:  00:7B0A  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:7B0D  A2                  		and	d				; AND mask
     206:  00:7B0E  B1                  		or	c				; OR mask
     207:  00:7B0F  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
