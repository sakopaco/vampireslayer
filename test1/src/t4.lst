     142.  00:8DC9                      ;;update_controllers_status
     143.  00:8DC9                      ;;=====================================================	
     144.  00:8DC9                      ;depends on the next rountine
     145.  00:8DC9                      ;  returns
     146.  00:8DC9                      ;  a - 0P2TRLDU
     147.  00:8DC9                      
     148.  00:8DC9                      update_controllers_status:
     149.  00:8DC9  CD 14 8E                 call  check_8th_keyboard_row
     150.  00:8DCC                      [4]  rrc    a
     150.  00:8DCC  CB 0F CB 0F CB 0F CB 0F 
     151.  00:8DD4  47                       ld    b,a    		;save for later  xxxTRDUL
     152.  00:8DD5                      
     153.  00:8DD5  E6 01                    and    #01    		;save L
     154.  00:8DD7  CB 27                    sla    a
     155.  00:8DD9  CB 27                    sla    a    		;now 00000L00
     156.  00:8DDB  4F                       ld    c,a    		;save for later
     157.  00:8DDC                      
     158.  00:8DDC  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     159.  00:8DDD  CB 3F                    srl    a
     160.  00:8DDF  E6 03                    and    00000011b
     161.  00:8DE1                      						;we've got 000000DU on a
     162.  00:8DE1  B1                       or    c    		;now we've got 00000LDU
     163.  00:8DE2  4F                       ld    c,a    		;save for later
     164.  00:8DE3                      
     165.  00:8DE3  78                       ld    a,b
     166.  00:8DE4  E6 18                    and    00011000b 	;we've got 000TR000
     167.  00:8DE6  B1                       or    c
     168.  00:8DE7                           ;finally    000TRLDU
     169.  00:8DE7                      
     170.  00:8DE7                           ;and    00000100   ;preserve just
     171.  00:8DE7  47                       ld    b,a    		;save for later
     172.  00:8DE8  CD 22 8E                 call  check_4th_keyboard_row      ;check M & P
     173.  00:8DEB  F5                       push  af
     174.  00:8DEC  E6 04                    and    00000100b	;filter just M
     175.  00:8DEE  87                       add    a
     176.  00:8DEF  87                       add    a
     177.  00:8DF0  87                       add    a           ;shift 3th to 6th bit
     178.  00:8DF1  B0                       or    b           	;merge with previous
     179.  00:8DF2  47                       ld    b,a
     180.  00:8DF3                      
     181.  00:8DF3  CD 00 8E                 call  read_joy_0
     182.  00:8DF6  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     183.  00:8DF8  B0                       or    b      		;added 000TRLDU from keyboard
     184.  00:8DF9  47                       ld    b,a      	;save for later
     185.  00:8DFA  F1                       pop    af      	; let's check on P
     186.  00:8DFB                      
     187.  00:8DFB  E6 20                    and    00100000b
     188.  00:8DFD  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     189.  00:8DFE  B0                       or    b
     190.  00:8DFF                      
     191.  00:8DFF  C9                       ret
     192.  00:8E00                      
     193.  00:8E00                      
     194.  00:8E00                      ;ret
