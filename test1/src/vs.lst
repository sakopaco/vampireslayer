     174.  00:55DC  87                       add    a
     175.  00:55DD  87                       add    a           ;shift 3th to 6th bit
     176.  00:55DE  B0                       or    b           	;merge with previous
     177.  00:55DF  47                       ld    b,a
     178.  00:55E0                      
     179.  00:55E0  CD ED 55                 call  read_joy_0
     180.  00:55E3  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:55E5  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:55E6  47                       ld    b,a      	;save for later
     183.  00:55E7  F1                       pop    af      	; let's check on P
     184.  00:55E8                      
     185.  00:55E8  E6 20                    and    00100000b
     186.  00:55EA  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
