      29:  00:8236                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:8236                      		; --- INPUT: A -> sound to be played ---
      31:  00:8236                      		; ---        C -> sound priority     ---
      32:  00:8236  C5                  		push	bc				; Store bc in stack
      33:  00:8237  D5                  		push	de				; Store de in stack
      34:  00:8238  E5                  		push	hl				; Store hl in stack
      35:  00:8239                      		; --- Check if the index is in the bank ---
      36:  00:8239  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:823A  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:823D  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:823E  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:823F  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:8241                      		; The bank has less than 256 samples
      42:  00:8241  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:8242  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:8243  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:8245  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:8247                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:8247                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:8247  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:8248  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:824B  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:824C  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:824E  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:8250                      		; --- Set new priority ---
      54:  00:8250  79                  		ld	a,c				; a:=New priority
      55:  00:8251  E6 0F               		and	$0F				; We mask the priority
      56:  00:8253  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:8256                      
      58:  00:8256                      		IF ( AYFXRELATIVE == 1 )
      59:  00:8256                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:8256                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:8256                    ~ 			ld	a,15				; a:=15
      62:  00:8256                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:8256                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:8256                    ~ 			add	a,a				; a:=a*2
      65:  00:8256                    ~ 			add	a,a				; a:=a*4
      66:  00:8256                    ~ 			add	a,a				; a:=a*8
      67:  00:8256                    ~ 			add	a,a				; a:=a*16
      68:  00:8256                    ~ 			ld	e,a				; e:=a
      69:  00:8256                    ~ 			ld	d,0				; de:=a
      70:  00:8256                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:8256                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
