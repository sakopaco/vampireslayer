      29:  00:75C6                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:75C6                      		; --- INPUT: A -> sound to be played ---
      31:  00:75C6                      		; ---        C -> sound priority     ---
      32:  00:75C6  C5                  		push	bc				; Store bc in stack
