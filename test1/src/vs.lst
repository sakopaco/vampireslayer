      37:  00:6C8F  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:6C92  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:6C93  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:6C94  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
