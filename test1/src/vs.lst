      29:  00:8229                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:8229                      		; --- INPUT: A -> sound to be played ---
      31:  00:8229                      		; ---        C -> sound priority     ---
      32:  00:8229  C5                  		push	bc				; Store bc in stack
      33:  00:822A  D5                  		push	de				; Store de in stack
      34:  00:822B  E5                  		push	hl				; Store hl in stack
      35:  00:822C                      		; --- Check if the index is in the bank ---
      36:  00:822C  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:822D  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:8230  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:8231  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:8232  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:8234                      		; The bank has less than 256 samples
      42:  00:8234  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:8235  BE                  		cp	[hl]				; If new index is not in the bank...
