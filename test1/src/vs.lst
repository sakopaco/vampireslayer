      17:  00:7AB7                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7AB7                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7AB7  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7ABA  3E 01               		ld	a,1				; a:=0
      21:  00:7ABC  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7ABF  3C                  		inc	a				; Starting channel (=1)
      23:  00:7AC0  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7AC3                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7AC3  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7AC5  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7AC8  C9                  		ret					; Return
      28:  00:7AC9                      
      29:  00:7AC9                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7AC9                      		; --- INPUT: A -> sound to be played ---
      31:  00:7AC9                      		; ---        C -> sound priority     ---
      32:  00:7AC9  C5                  		push	bc				; Store bc in stack
      33:  00:7ACA  D5                  		push	de				; Store de in stack
      34:  00:7ACB  E5                  		push	hl				; Store hl in stack
      35:  00:7ACC                      		; --- Check if the index is in the bank ---
      36:  00:7ACC  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7ACD  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7AD0  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7AD1  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7AD2  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7AD4                      		; The bank has less than 256 samples
      42:  00:7AD4  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7AD5  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7AD6  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7AD8  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7ADA                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7ADA                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7ADA  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7ADB  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7ADE  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7ADF  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7AE1  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7AE3                      		; --- Set new priority ---
      54:  00:7AE3  79                  		ld	a,c				; a:=New priority
      55:  00:7AE4  E6 0F               		and	$0F				; We mask the priority
      56:  00:7AE6  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7AE9                      
      58:  00:7AE9                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7AE9                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7AE9                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7AE9                    ~ 			ld	a,15				; a:=15
      62:  00:7AE9                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7AE9                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7AE9                    ~ 			add	a,a				; a:=a*2
      65:  00:7AE9                    ~ 			add	a,a				; a:=a*4
      66:  00:7AE9                    ~ 			add	a,a				; a:=a*8
      67:  00:7AE9                    ~ 			add	a,a				; a:=a*16
      68:  00:7AE9                    ~ 			ld	e,a				; e:=a
      69:  00:7AE9                    ~ 			ld	d,0				; de:=a
      70:  00:7AE9                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:7AE9                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:7AE9                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:7AE9                    ~ 		ENDIF
      74:  00:7AE9                      
      75:  00:7AE9                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:7AE9  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:7AED  13                  		inc	de				; de points to the increments table of the bank
      78:  00:7AEE  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:7AEF  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:7AF1  29                  		add	hl,hl				; hl:=hl*2
      81:  00:7AF2  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:7AF3  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:7AF4  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:7AF5  56                  		ld	d,[hl]				; de:=increment
      85:  00:7AF6  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:7AF7  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:7AFA  AF                  		xor	a				; a:=0 (no errors)
      88:  00:7AFB  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:7AFC  D1                  		pop	de				; Retrieve de from stack
      90:  00:7AFD  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:7AFE  C9                  		ret					; Return
      92:  00:7AFF                      
      93:  00:7AFF                      		IF ( AYFXRELATIVE == 1 )
      94:  00:7AFF                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:7AFF                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:7AFF                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:7AFF                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:7AFF                    ~ 		ENDIF
