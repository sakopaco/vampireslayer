     150.  00:525D                      
     151.  00:525D  E6 01                    and    #01    		;save L
     152.  00:525F  CB 27                    sla    a
     153.  00:5261  CB 27                    sla    a    		;now 00000L00
     154.  00:5263  4F                       ld    c,a    		;save for later
     155.  00:5264                      
     156.  00:5264  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:5265  CB 3F                    srl    a
     158.  00:5267  E6 03                    and    00000011b
     159.  00:5269                      						;we've got 000000DU on a
