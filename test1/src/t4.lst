     114.  00:810F  3A 24 86            		LD		 A, (prota_nivel)	
     115.  00:8112  B7                  		OR		 A
     116.  00:8113  28 16               		JR		 Z, .no_suma_niveles_previos	;el primero nivel (0) no sumaría ni uno por lo que salto al segundo sumando
     117.  00:8115                      	;sumo 7 veces el nº de nivel si no es cero
     118.  00:8115  57                  		LD		 D, A
     119.  00:8116  06 06               		LD		 B, 6
     120.  00:8118                      .primer_producto
     121.  00:8118  82                  		ADD		 A, D
     122.  00:8119  10 FD               		DJNZ	 .primer_producto
     123.  00:811B  47                  		LD		 B, A				;guardo el resultado en B
     124.  00:811C                      
     125.  00:811C                      ;el resultado previo está en B. Ahoro hay que multiplicar A x 16, lo guardo en DE y lo sumo en HL
     126.  00:811C                      		;inicializo valores a 0
     127.  00:811C  AF                  		XOR		 A
     128.  00:811D  57                  		LD		 D, A
     129.  00:811E  5F                  		LD		 E, A
     130.  00:811F  67                  		LD		 H, A
     131.  00:8120  6F                  		LD		 L, A
     132.  00:8121                      
     133.  00:8121  58                  		LD		 E, B				;pongo el (4x7) en HL (estaba en B)
     134.  00:8122                      		
     135.  00:8122  06 10               		LD		 B, 16
     136.  00:8124                      .segundo_producto
     137.  00:8124  19                  		ADD		HL, DE				;segundo producto x 16
     138.  00:8125  10 FD               		DJNZ	.segundo_producto	
