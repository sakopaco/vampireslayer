      46:  00:823A                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:823A                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:823A  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:823B  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:823E  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
