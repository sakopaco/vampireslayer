     152.  00:5298  CB 27                    sla    a
     153.  00:529A  CB 27                    sla    a    		;now 00000L00
     154.  00:529C  4F                       ld    c,a    		;save for later
     155.  00:529D                      
     156.  00:529D  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:529E  CB 3F                    srl    a
     158.  00:52A0  E6 03                    and    00000011b
     159.  00:52A2                      						;we've got 000000DU on a
     160.  00:52A2  B1                       or    c    		;now we've got 00000LDU
     161.  00:52A3  4F                       ld    c,a    		;save for later
     162.  00:52A4                      
     163.  00:52A4  78                       ld    a,b
     164.  00:52A5  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:52A7  B1                       or    c
