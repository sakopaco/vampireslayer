      29:  00:7A25                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7A25                      		; --- INPUT: A -> sound to be played ---
      31:  00:7A25                      		; ---        C -> sound priority     ---
      32:  00:7A25  C5                  		push	bc				; Store bc in stack
      33:  00:7A26  D5                  		push	de				; Store de in stack
      34:  00:7A27  E5                  		push	hl				; Store hl in stack
      35:  00:7A28                      		; --- Check if the index is in the bank ---
      36:  00:7A28  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7A29  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7A2C  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7A2D  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7A2E  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7A30                      		; The bank has less than 256 samples
      42:  00:7A30  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7A31  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7A32  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7A34  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7A36                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7A36                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7A36  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7A37  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7A3A  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7A3B  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7A3D  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7A3F                      		; --- Set new priority ---
      54:  00:7A3F  79                  		ld	a,c				; a:=New priority
      55:  00:7A40  E6 0F               		and	$0F				; We mask the priority
