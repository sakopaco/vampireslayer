     101:  00:90D7  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:90DA  B7                  		or	a				; If priority has bit 7 on...
     103:  00:90DB  F8                  		ret	m				; ...return
