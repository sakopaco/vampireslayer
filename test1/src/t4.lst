      17:  00:8CEE                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:8CEE                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:8CEE  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
