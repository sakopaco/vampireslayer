     150.  00:55B0                      
     151.  00:55B0  E6 01                    and    #01    		;save L
     152.  00:55B2  CB 27                    sla    a
     153.  00:55B4  CB 27                    sla    a    		;now 00000L00
     154.  00:55B6  4F                       ld    c,a    		;save for later
     155.  00:55B7                      
     156.  00:55B7  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:55B8  CB 3F                    srl    a
     158.  00:55BA  E6 03                    and    00000011b
     159.  00:55BC                      						;we've got 000000DU on a
     160.  00:55BC  B1                       or    c    		;now we've got 00000LDU
     161.  00:55BD  4F                       ld    c,a    		;save for later
     162.  00:55BE                      
     163.  00:55BE  78                       ld    a,b
     164.  00:55BF  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:55C1  B1                       or    c
     166.  00:55C2                           ;finally    000TRLDU
     167.  00:55C2                      
     168.  00:55C2                           ;and    00000100   ;preserve just
     169.  00:55C2  47                       ld    b,a    		;save for later
     170.  00:55C3  CD FD 55                 call  check_4th_keyboard_row      ;check M & P
     171.  00:55C6  F5                       push  af
     172.  00:55C7  E6 04                    and    00000100b	;filter just M
     173.  00:55C9  87                       add    a
     174.  00:55CA  87                       add    a
     175.  00:55CB  87                       add    a           ;shift 3th to 6th bit
     176.  00:55CC  B0                       or    b           	;merge with previous
     177.  00:55CD  47                       ld    b,a
     178.  00:55CE                      
     179.  00:55CE  CD DB 55                 call  read_joy_0
     180.  00:55D1  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:55D3  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:55D4  47                       ld    b,a      	;save for later
     183.  00:55D5  F1                       pop    af      	; let's check on P
     184.  00:55D6                      
     185.  00:55D6  E6 20                    and    00100000b
     186.  00:55D8  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:55D9  B0                       or    b
