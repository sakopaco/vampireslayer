     111:  00:9733  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:9735                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:9735  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:9738  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:9739  23                  		inc	hl				; Increment pointer
     116:  00:973A                      		; --- Check if there's new tone on stream ---
