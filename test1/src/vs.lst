     172.  00:56CC  E6 04                    and    00000100b	;filter just M
     173.  00:56CE  87                       add    a
     174.  00:56CF  87                       add    a
     175.  00:56D0  87                       add    a           ;shift 3th to 6th bit
     176.  00:56D1  B0                       or    b           	;merge with previous
     177.  00:56D2  47                       ld    b,a
     178.  00:56D3                      
     179.  00:56D3  CD E0 56                 call  read_joy_0
     180.  00:56D6  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:56D8  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:56D9  47                       ld    b,a      	;save for later
     183.  00:56DA  F1                       pop    af      	; let's check on P
     184.  00:56DB                      
     185.  00:56DB  E6 20                    and    00100000b
     186.  00:56DD  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
