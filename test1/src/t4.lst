     138:  00:9BB3  E6 0F               		and	$0F				; lower nibble
     139:  00:9BB5                      
     140:  00:9BB5                      		IF ( AYFXRELATIVE == 1 )
     141:  00:9BB5                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:9BB5                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:9BB5                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:9BB5                    ~ 			ld	d,0				; d:=0
     145:  00:9BB5                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:9BB5                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:9BB5                    ~ 			or	a				; If relative volume is zero...
     148:  00:9BB5                    ~ 		ENDIF
     149:  00:9BB5                      
     150:  00:9BB5  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
