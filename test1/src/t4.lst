     134:  00:9753                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:9753  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:9756                      		; --- Extract volume ---
     137:  00:9756  79                  		ld	a,c				; a:=Control byte
     138:  00:9757  E6 0F               		and	$0F				; lower nibble
     139:  00:9759                      
     140:  00:9759                      		IF ( AYFXRELATIVE == 1 )
     141:  00:9759                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:9759                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
