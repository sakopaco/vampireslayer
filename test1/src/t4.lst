     174.  00:9066  E6 04                    and    00000100b	;filter just M
     175.  00:9068  87                       add    a
     176.  00:9069  87                       add    a
     177.  00:906A  87                       add    a           ;shift 3th to 6th bit
     178.  00:906B  B0                       or    b           	;merge with previous
