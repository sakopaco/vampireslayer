      17:  00:9204                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:9204                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:9204  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
