     146.  00:514E                      update_controllers_status:
     147.  00:514E  CD 99 51                 call  check_8th_keyboard_row
     148.  00:5151                      [4]  rrc    a
     148.  00:5151  CB 0F CB 0F CB 0F CB 0F 
     149.  00:5159  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:515A                      
     151.  00:515A  E6 01                    and    #01    		;save L
     152.  00:515C  CB 27                    sla    a
     153.  00:515E  CB 27                    sla    a    		;now 00000L00
     154.  00:5160  4F                       ld    c,a    		;save for later
     155.  00:5161                      
     156.  00:5161  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:5162  CB 3F                    srl    a
     158.  00:5164  E6 03                    and    00000011b
     159.  00:5166                      						;we've got 000000DU on a
     160.  00:5166  B1                       or    c    		;now we've got 00000LDU
     161.  00:5167  4F                       ld    c,a    		;save for later
     162.  00:5168                      
     163.  00:5168  78                       ld    a,b
     164.  00:5169  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:516B  B1                       or    c
     166.  00:516C                           ;finally    000TRLDU
     167.  00:516C                      
     168.  00:516C                           ;and    00000100   ;preserve just
     169.  00:516C  47                       ld    b,a    		;save for later
     170.  00:516D  CD A7 51                 call  check_4th_keyboard_row      ;check M & P
     171.  00:5170  F5                       push  af
     172.  00:5171  E6 04                    and    00000100b	;filter just M
     173.  00:5173  87                       add    a
     174.  00:5174  87                       add    a
     175.  00:5175  87                       add    a           ;shift 3th to 6th bit
     176.  00:5176  B0                       or    b           	;merge with previous
     177.  00:5177  47                       ld    b,a
     178.  00:5178                      
     179.  00:5178  CD 85 51                 call  read_joy_0
     180.  00:517B  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:517D  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:517E  47                       ld    b,a      	;save for later
     183.  00:517F  F1                       pop    af      	; let's check on P
     184.  00:5180                      
     185.  00:5180  E6 20                    and    00100000b
     186.  00:5182  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:5183  B0                       or    b
     188.  00:5184                      
     189.  00:5184  C9                       ret
