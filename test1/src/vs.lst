      17:  00:7AC9                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7AC9                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7AC9  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7ACC  3E 01               		ld	a,1				; a:=0
      21:  00:7ACE  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7AD1  3C                  		inc	a				; Starting channel (=1)
      23:  00:7AD2  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7AD5                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7AD5  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7AD7  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7ADA  C9                  		ret					; Return
      28:  00:7ADB                      
      29:  00:7ADB                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7ADB                      		; --- INPUT: A -> sound to be played ---
      31:  00:7ADB                      		; ---        C -> sound priority     ---
      32:  00:7ADB  C5                  		push	bc				; Store bc in stack
      33:  00:7ADC  D5                  		push	de				; Store de in stack
      34:  00:7ADD  E5                  		push	hl				; Store hl in stack
      35:  00:7ADE                      		; --- Check if the index is in the bank ---
      36:  00:7ADE  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7ADF  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7AE2  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7AE3  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7AE4  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7AE6                      		; The bank has less than 256 samples
      42:  00:7AE6  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7AE7  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7AE8  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7AEA  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7AEC                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7AEC                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7AEC  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7AED  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7AF0  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7AF1  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7AF3  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7AF5                      		; --- Set new priority ---
      54:  00:7AF5  79                  		ld	a,c				; a:=New priority
      55:  00:7AF6  E6 0F               		and	$0F				; We mask the priority
      56:  00:7AF8  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7AFB                      
      58:  00:7AFB                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7AFB                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7AFB                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7AFB                    ~ 			ld	a,15				; a:=15
      62:  00:7AFB                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7AFB                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7AFB                    ~ 			add	a,a				; a:=a*2
      65:  00:7AFB                    ~ 			add	a,a				; a:=a*4
      66:  00:7AFB                    ~ 			add	a,a				; a:=a*8
      67:  00:7AFB                    ~ 			add	a,a				; a:=a*16
      68:  00:7AFB                    ~ 			ld	e,a				; e:=a
      69:  00:7AFB                    ~ 			ld	d,0				; de:=a
      70:  00:7AFB                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:7AFB                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:7AFB                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:7AFB                    ~ 		ENDIF
      74:  00:7AFB                      
      75:  00:7AFB                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:7AFB  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:7AFF  13                  		inc	de				; de points to the increments table of the bank
      78:  00:7B00  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:7B01  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:7B03  29                  		add	hl,hl				; hl:=hl*2
      81:  00:7B04  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:7B05  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:7B06  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:7B07  56                  		ld	d,[hl]				; de:=increment
      85:  00:7B08  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:7B09  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:7B0C  AF                  		xor	a				; a:=0 (no errors)
      88:  00:7B0D  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:7B0E  D1                  		pop	de				; Retrieve de from stack
      90:  00:7B0F  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:7B10  C9                  		ret					; Return
      92:  00:7B11                      
      93:  00:7B11                      		IF ( AYFXRELATIVE == 1 )
      94:  00:7B11                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:7B11                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:7B11                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:7B11                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:7B11                    ~ 		ENDIF
      99:  00:7B11                      
     100:  00:7B11                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:7B11  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:7B14  B7                  		or	a				; If priority has bit 7 on...
     103:  00:7B15  F8                  		ret	m				; ...return
     104:  00:7B16                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:7B16  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:7B19  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:7B1B  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:7B1D  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:7B20  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:7B21  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:7B23  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:7B25                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:7B25  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:7B28  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:7B29  23                  		inc	hl				; Increment pointer
     116:  00:7B2A                      		; --- Check if there's new tone on stream ---
     117:  00:7B2A  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:7B2C  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:7B2E                      		; --- Extract new tone from stream ---
     120:  00:7B2E  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:7B2F  23                  		inc	hl				; Increment pointer
     122:  00:7B30  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:7B31  23                  		inc	hl				; Increment pointer
     124:  00:7B32  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:7B36                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:7B36  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:7B38  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:7B3A                      		; --- Extract new noise from stream ---
     129:  00:7B3A  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:7B3B  23                  		inc	hl				; Increment pointer
     131:  00:7B3C  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:7B3E  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:7B40  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:7B43                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:7B43  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:7B46                      		; --- Extract volume ---
     137:  00:7B46  79                  		ld	a,c				; a:=Control byte
     138:  00:7B47  E6 0F               		and	$0F				; lower nibble
     139:  00:7B49                      
     140:  00:7B49                      		IF ( AYFXRELATIVE == 1 )
     141:  00:7B49                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:7B49                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:7B49                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:7B49                    ~ 			ld	d,0				; d:=0
     145:  00:7B49                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:7B49                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:7B49                    ~ 			or	a				; If relative volume is zero...
     148:  00:7B49                    ~ 		ENDIF
     149:  00:7B49                      
     150:  00:7B49  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:7B4C  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:7B4D                      		; -------------------------------------
     153:  00:7B4D                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:7B4D                      		; -------------------------------------
     155:  00:7B4D                      		; --- Set noise channel ---
     156:  00:7B4D  CB 79               		bit	7,c				; If noise is off...
     157:  00:7B4F  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:7B51  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:7B54  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:7B57                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:7B57  79                  		ld	a,c				; a:=Control byte
     162:  00:7B58  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:7B5A  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:7B5C  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:7B5D                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:7B5D  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:7B5E  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:7B5F  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:7B61                      		; --- Dump to correct channel ---
     170:  00:7B61  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:7B64  46                  		ld	b,[hl]				; Channel counter
     172:  00:7B65                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:7B65  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:7B67                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:7B67  CD 96 7B            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:7B6A  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:7B6D  CB 51               		bit	2,c				; If tone is off...
     178:  00:7B6F  C0                  		ret	nz				; ...return
     179:  00:7B70  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:7B73  C9                  		ret					; Return
     181:  00:7B74                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:7B74  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:7B76  0F                  		rrca					; Rotate right OR mask
     184:  00:7B77  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:7B79                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:7B79  CD 96 7B            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:7B7C  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:7B7F  CB 49               		bit	1,c				; If tone is off...
     189:  00:7B81  C0                  		ret	nz				; ...return
     190:  00:7B82  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:7B85  C9                  		ret					; Return
     192:  00:7B86                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:7B86  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:7B88  0F                  		rrca					; Rotate right OR mask
     195:  00:7B89                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:7B89  CD 96 7B            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:7B8C  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:7B8F  CB 41               		bit	0,c				; If tone is off...
     199:  00:7B91  C0                  		ret	nz				; ...return
     200:  00:7B92  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:7B95  C9                  		ret					; Return
     202:  00:7B96                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:7B96  4F                  		ld	c,a				; c:=OR mask
     204:  00:7B97  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:7B9A  A2                  		and	d				; AND mask
     206:  00:7B9B  B1                  		or	c				; OR mask
     207:  00:7B9C  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
     208:  00:7B9F  3A 49 F2            		ld	a,[ayFX_VOLUME]			; a:=ayFX volume value
     209:  00:7BA2  2A 46 F2            		ld	hl,[ayFX_TONE]			; ayFX tone value
     210:  00:7BA5  C9                  		ret					; Return
