     505.  00:4AA2                      	
     506.  00:4AA2                      		;ya tengo en A la coordenada X del centro del punto de mira					
     507.  00:4AA2  DD 96 01            		SUB		(IX + ESTRUCTURA_PUERTA.posx)	;le resto el punto x en la puerta
     508.  00:4AA5                      	
     509.  00:4AA5  D2 AA 4A            		JP		NC, .deteccioncolision_paso2	;si no es negativo comparo con el radio
     510.  00:4AA8                      
