     161.  00:526A  4F                       ld    c,a    		;save for later
     162.  00:526B                      
     163.  00:526B  78                       ld    a,b
     164.  00:526C  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:526E  B1                       or    c
