      64:  00:7C43                    ~ 			add	a,a				; a:=a*2
      65:  00:7C43                    ~ 			add	a,a				; a:=a*4
      66:  00:7C43                    ~ 			add	a,a				; a:=a*8
      67:  00:7C43                    ~ 			add	a,a				; a:=a*16
      68:  00:7C43                    ~ 			ld	e,a				; e:=a
      69:  00:7C43                    ~ 			ld	d,0				; de:=a
      70:  00:7C43                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
