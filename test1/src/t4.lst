      29:  00:8F58                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:8F58                      		; --- INPUT: A -> sound to be played ---
      31:  00:8F58                      		; ---        C -> sound priority     ---
      32:  00:8F58  C5                  		push	bc				; Store bc in stack
      33:  00:8F59  D5                  		push	de				; Store de in stack
      34:  00:8F5A  E5                  		push	hl				; Store hl in stack
      35:  00:8F5B                      		; --- Check if the index is in the bank ---
      36:  00:8F5B  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:8F5C  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:8F5F  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:8F60  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:8F61  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:8F63                      		; The bank has less than 256 samples
      42:  00:8F63  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:8F64  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:8F65  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:8F67  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:8F69                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:8F69                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:8F69  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:8F6A  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:8F6D  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:8F6E  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:8F70  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:8F72                      		; --- Set new priority ---
      54:  00:8F72  79                  		ld	a,c				; a:=New priority
      55:  00:8F73  E6 0F               		and	$0F				; We mask the priority
      56:  00:8F75  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:8F78                      
      58:  00:8F78                      		IF ( AYFXRELATIVE == 1 )
      59:  00:8F78                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:8F78                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:8F78                    ~ 			ld	a,15				; a:=15
      62:  00:8F78                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:8F78                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:8F78                    ~ 			add	a,a				; a:=a*2
      65:  00:8F78                    ~ 			add	a,a				; a:=a*4
      66:  00:8F78                    ~ 			add	a,a				; a:=a*8
      67:  00:8F78                    ~ 			add	a,a				; a:=a*16
