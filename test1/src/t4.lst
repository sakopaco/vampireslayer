      36:  00:9219  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:921A  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:921D  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:921E  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:921F  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
