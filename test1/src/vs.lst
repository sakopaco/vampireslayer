      75:  00:6CB2                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:6CB2  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:6CB6  13                  		inc	de				; de points to the increments table of the bank
      78:  00:6CB7  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:6CB8  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:6CBA  29                  		add	hl,hl				; hl:=hl*2
      81:  00:6CBB  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:6CBC  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:6CBD  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:6CBE  56                  		ld	d,[hl]				; de:=increment
      85:  00:6CBF  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:6CC0  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:6CC3  AF                  		xor	a				; a:=0 (no errors)
      88:  00:6CC4  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:6CC5  D1                  		pop	de				; Retrieve de from stack
      90:  00:6CC6  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:6CC7  C9                  		ret					; Return
      92:  00:6CC8                      
      93:  00:6CC8                      		IF ( AYFXRELATIVE == 1 )
      94:  00:6CC8                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:6CC8                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:6CC8                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:6CC8                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:6CC8                    ~ 		ENDIF
