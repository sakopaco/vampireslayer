      53:  00:9A29                      		; --- Set new priority ---
      54:  00:9A29  79                  		ld	a,c				; a:=New priority
      55:  00:9A2A  E6 0F               		and	$0F				; We mask the priority
      56:  00:9A2C  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:9A2F                      
      58:  00:9A2F                      		IF ( AYFXRELATIVE == 1 )
      59:  00:9A2F                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:9A2F                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:9A2F                    ~ 			ld	a,15				; a:=15
      62:  00:9A2F                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:9A2F                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:9A2F                    ~ 			add	a,a				; a:=a*2
      65:  00:9A2F                    ~ 			add	a,a				; a:=a*4
      66:  00:9A2F                    ~ 			add	a,a				; a:=a*8
      67:  00:9A2F                    ~ 			add	a,a				; a:=a*16
      68:  00:9A2F                    ~ 			ld	e,a				; e:=a
      69:  00:9A2F                    ~ 			ld	d,0				; de:=a
      70:  00:9A2F                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:9A2F                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:9A2F                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:9A2F                    ~ 		ENDIF
