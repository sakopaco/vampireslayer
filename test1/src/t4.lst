     125:  00:9746                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:9746  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:9748  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:974A                      		; --- Extract new noise from stream ---
     129:  00:974A  7E                  		ld	a,[hl]				; a:=New noise
