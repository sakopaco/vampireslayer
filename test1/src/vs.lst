      56:  00:7E76  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7E79                      
      58:  00:7E79                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7E79                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7E79                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7E79                    ~ 			ld	a,15				; a:=15
      62:  00:7E79                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7E79                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7E79                    ~ 			add	a,a				; a:=a*2
      65:  00:7E79                    ~ 			add	a,a				; a:=a*4
      66:  00:7E79                    ~ 			add	a,a				; a:=a*8
      67:  00:7E79                    ~ 			add	a,a				; a:=a*16
