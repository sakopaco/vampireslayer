     167.  00:56C7                      
     168.  00:56C7                           ;and    00000100   ;preserve just
     169.  00:56C7  47                       ld    b,a    		;save for later
     170.  00:56C8  CD 02 57                 call  check_4th_keyboard_row      ;check M & P
