      82:  00:76B4  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:76B5  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:76B6  56                  		ld	d,[hl]				; de:=increment
      85:  00:76B7  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:76B8  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:76BB  AF                  		xor	a				; a:=0 (no errors)
      88:  00:76BC  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:76BD  D1                  		pop	de				; Retrieve de from stack
      90:  00:76BE  C1                  		pop	bc				; Retrieve bc from stack
