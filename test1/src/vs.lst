     161.  00:541D  4F                       ld    c,a    		;save for later
     162.  00:541E                      
     163.  00:541E  78                       ld    a,b
     164.  00:541F  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5421  B1                       or    c
     166.  00:5422                           ;finally    000TRLDU
     167.  00:5422                      
     168.  00:5422                           ;and    00000100   ;preserve just
     169.  00:5422  47                       ld    b,a    		;save for later
     170.  00:5423  CD 5D 54                 call  check_4th_keyboard_row      ;check M & P
