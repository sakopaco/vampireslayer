     156:  00:8E80  CB 79               		bit	7,c				; If noise is off...
     157:  00:8E82  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:8E84  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:8E87  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:8E8A                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:8E8A  79                  		ld	a,c				; a:=Control byte
     162:  00:8E8B  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:8E8D  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:8E8F  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:8E90                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:8E90  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:8E91  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:8E92  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:8E94                      		; --- Dump to correct channel ---
     170:  00:8E94  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:8E97  46                  		ld	b,[hl]				; Channel counter
     172:  00:8E98                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:8E98  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:8E9A                      .PLAY_C:	; --- Play ayFX stream on channel C ---
