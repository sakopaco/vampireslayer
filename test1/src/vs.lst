     167.  00:55D7                      
     168.  00:55D7                           ;and    00000100   ;preserve just
     169.  00:55D7  47                       ld    b,a    		;save for later
     170.  00:55D8  CD 12 56                 call  check_4th_keyboard_row      ;check M & P
