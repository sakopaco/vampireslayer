     160:  00:8138                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:8138  79                  		ld	a,c				; a:=Control byte
     162:  00:8139  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:813B  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:813D  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:813E                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:813E  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:813F  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:8140  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:8142                      		; --- Dump to correct channel ---
