     147.  00:8B13                      update_controllers_status:
     148.  00:8B13  CD 5E 8B                 call  check_8th_keyboard_row
     149.  00:8B16                      [4]  rrc    a
     149.  00:8B16  CB 0F CB 0F CB 0F CB 0F 
     150.  00:8B1E  47                       ld    b,a    		;save for later  xxxTRDUL
     151.  00:8B1F                      
     152.  00:8B1F  E6 01                    and    #01    		;save L
     153.  00:8B21  CB 27                    sla    a
     154.  00:8B23  CB 27                    sla    a    		;now 00000L00
     155.  00:8B25  4F                       ld    c,a    		;save for later
     156.  00:8B26                      
     157.  00:8B26  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     158.  00:8B27  CB 3F                    srl    a
     159.  00:8B29  E6 03                    and    00000011b
     160.  00:8B2B                      						;we've got 000000DU on a
     161.  00:8B2B  B1                       or    c    		;now we've got 00000LDU
     162.  00:8B2C  4F                       ld    c,a    		;save for later
     163.  00:8B2D                      
     164.  00:8B2D  78                       ld    a,b
     165.  00:8B2E  E6 18                    and    00011000b 	;we've got 000TR000
     166.  00:8B30  B1                       or    c
     167.  00:8B31                           ;finally    000TRLDU
     168.  00:8B31                      
     169.  00:8B31                           ;and    00000100   ;preserve just
     170.  00:8B31  47                       ld    b,a    		;save for later
     171.  00:8B32  CD 6C 8B                 call  check_4th_keyboard_row      ;check M & P
     172.  00:8B35  F5                       push  af
     173.  00:8B36  E6 04                    and    00000100b	;filter just M
     174.  00:8B38  87                       add    a
     175.  00:8B39  87                       add    a
     176.  00:8B3A  87                       add    a           ;shift 3th to 6th bit
     177.  00:8B3B  B0                       or    b           	;merge with previous
     178.  00:8B3C  47                       ld    b,a
     179.  00:8B3D                      
     180.  00:8B3D  CD 4A 8B                 call  read_joy_0
     181.  00:8B40  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     182.  00:8B42  B0                       or    b      		;added 000TRLDU from keyboard
     183.  00:8B43  47                       ld    b,a      	;save for later
     184.  00:8B44  F1                       pop    af      	; let's check on P
     185.  00:8B45                      
     186.  00:8B45  E6 20                    and    00100000b
     187.  00:8B47  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     188.  00:8B48  B0                       or    b
     189.  00:8B49                      
     190.  00:8B49  C9                       ret
