     200.  00:858D  E6 04                    and    00000100b	;filter just M
     201.  00:858F  87                       add    a
     202.  00:8590  87                       add    a
     203.  00:8591  87                       add    a           ;shift 3th to 6th bit
     204.  00:8592  B0                       or    b           	;merge with previous
     205.  00:8593  47                       ld    b,a
     206.  00:8594                      
     207.  00:8594  CD A1 85                 call  read_joy_0
     208.  00:8597  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     209.  00:8599  B0                       or    b      		;added 000TRLDU from keyboard
     210.  00:859A  47                       ld    b,a      	;save for later
     211.  00:859B  F1                       pop    af      	; let's check on P
     212.  00:859C                      
     213.  00:859C  E6 20                    and    00100000b
     214.  00:859E  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
