     156:  00:7AC0  CB 79               		bit	7,c				; If noise is off...
     157:  00:7AC2  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:7AC4  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:7AC7  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:7ACA                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:7ACA  79                  		ld	a,c				; a:=Control byte
     162:  00:7ACB  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:7ACD  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:7ACF  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:7AD0                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:7AD0  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:7AD1  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:7AD2  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:7AD4                      		; --- Dump to correct channel ---
