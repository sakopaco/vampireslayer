     155:  00:9B60                      		; --- Set noise channel ---
     156:  00:9B60  CB 79               		bit	7,c				; If noise is off...
     157:  00:9B62  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:9B64  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:9B67  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:9B6A                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:9B6A  79                  		ld	a,c				; a:=Control byte
     162:  00:9B6B  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:9B6D  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:9B6F  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:9B70                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:9B70  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:9B71  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:9B72  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:9B74                      		; --- Dump to correct channel ---
     170:  00:9B74  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:9B77  46                  		ld	b,[hl]				; Channel counter
