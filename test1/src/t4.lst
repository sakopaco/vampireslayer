     212.  00:88D3  E6 04                    and    00000100b	;filter just M
     213.  00:88D5  87                       add    a
     214.  00:88D6  87                       add    a
     215.  00:88D7  87                       add    a           ;shift 3th to 6th bit
     216.  00:88D8  B0                       or    b           	;merge with previous
     217.  00:88D9  47                       ld    b,a
     218.  00:88DA                      
     219.  00:88DA  CD E7 88                 call  read_joy_0
