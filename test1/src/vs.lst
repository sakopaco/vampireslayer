      38:  00:7BA1  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7BA2  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7BA3  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
