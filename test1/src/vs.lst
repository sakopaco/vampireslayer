     182.  00:5281  47                       ld    b,a      	;save for later
     183.  00:5282  F1                       pop    af      	; let's check on P
     184.  00:5283                      
     185.  00:5283  E6 20                    and    00100000b
     186.  00:5285  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
