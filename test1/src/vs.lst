     146.  00:54CF                      update_controllers_status:
     147.  00:54CF  CD 1A 55                 call  check_8th_keyboard_row
     148.  00:54D2                      [4]  rrc    a
     148.  00:54D2  CB 0F CB 0F CB 0F CB 0F 
     149.  00:54DA  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:54DB                      
     151.  00:54DB  E6 01                    and    #01    		;save L
     152.  00:54DD  CB 27                    sla    a
     153.  00:54DF  CB 27                    sla    a    		;now 00000L00
     154.  00:54E1  4F                       ld    c,a    		;save for later
     155.  00:54E2                      
     156.  00:54E2  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:54E3  CB 3F                    srl    a
     158.  00:54E5  E6 03                    and    00000011b
     159.  00:54E7                      						;we've got 000000DU on a
     160.  00:54E7  B1                       or    c    		;now we've got 00000LDU
     161.  00:54E8  4F                       ld    c,a    		;save for later
     162.  00:54E9                      
     163.  00:54E9  78                       ld    a,b
     164.  00:54EA  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:54EC  B1                       or    c
     166.  00:54ED                           ;finally    000TRLDU
     167.  00:54ED                      
     168.  00:54ED                           ;and    00000100   ;preserve just
     169.  00:54ED  47                       ld    b,a    		;save for later
     170.  00:54EE  CD 28 55                 call  check_4th_keyboard_row      ;check M & P
     171.  00:54F1  F5                       push  af
     172.  00:54F2  E6 04                    and    00000100b	;filter just M
     173.  00:54F4  87                       add    a
     174.  00:54F5  87                       add    a
     175.  00:54F6  87                       add    a           ;shift 3th to 6th bit
     176.  00:54F7  B0                       or    b           	;merge with previous
     177.  00:54F8  47                       ld    b,a
     178.  00:54F9                      
     179.  00:54F9  CD 06 55                 call  read_joy_0
     180.  00:54FC  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:54FE  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:54FF  47                       ld    b,a      	;save for later
     183.  00:5500  F1                       pop    af      	; let's check on P
     184.  00:5501                      
     185.  00:5501  E6 20                    and    00100000b
     186.  00:5503  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:5504  B0                       or    b
     188.  00:5505                      
     189.  00:5505  C9                       ret
