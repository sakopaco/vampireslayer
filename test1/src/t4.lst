      46:  00:9227                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:9227                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:9227  78                  		ld	a,b				; a:=b (new ayFX stream index)
