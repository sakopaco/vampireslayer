      60:  00:9BF5                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:9BF5                    ~ 			ld	a,15				; a:=15
      62:  00:9BF5                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:9BF5                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:9BF5                    ~ 			add	a,a				; a:=a*2
      65:  00:9BF5                    ~ 			add	a,a				; a:=a*4
      66:  00:9BF5                    ~ 			add	a,a				; a:=a*8
      67:  00:9BF5                    ~ 			add	a,a				; a:=a*16
      68:  00:9BF5                    ~ 			ld	e,a				; e:=a
      69:  00:9BF5                    ~ 			ld	d,0				; de:=a
      70:  00:9BF5                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:9BF5                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
