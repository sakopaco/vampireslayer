      45:  00:8C21  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:8C23                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:8C23                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:8C23  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:8C24  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:8C27  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:8C28  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:8C2A  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:8C2C                      		; --- Set new priority ---
      54:  00:8C2C  79                  		ld	a,c				; a:=New priority
      55:  00:8C2D  E6 0F               		and	$0F				; We mask the priority
      56:  00:8C2F  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:8C32                      
      58:  00:8C32                      		IF ( AYFXRELATIVE == 1 )
      59:  00:8C32                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:8C32                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:8C32                    ~ 			ld	a,15				; a:=15
      62:  00:8C32                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:8C32                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:8C32                    ~ 			add	a,a				; a:=a*2
      65:  00:8C32                    ~ 			add	a,a				; a:=a*4
      66:  00:8C32                    ~ 			add	a,a				; a:=a*8
      67:  00:8C32                    ~ 			add	a,a				; a:=a*16
      68:  00:8C32                    ~ 			ld	e,a				; e:=a
      69:  00:8C32                    ~ 			ld	d,0				; de:=a
      70:  00:8C32                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
