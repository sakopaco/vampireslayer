     172.  00:56B4  E6 04                    and    00000100b	;filter just M
     173.  00:56B6  87                       add    a
     174.  00:56B7  87                       add    a
     175.  00:56B8  87                       add    a           ;shift 3th to 6th bit
     176.  00:56B9  B0                       or    b           	;merge with previous
     177.  00:56BA  47                       ld    b,a
     178.  00:56BB                      
     179.  00:56BB  CD C8 56                 call  read_joy_0
     180.  00:56BE  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:56C0  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:56C1  47                       ld    b,a      	;save for later
     183.  00:56C2  F1                       pop    af      	; let's check on P
     184.  00:56C3                      
     185.  00:56C3  E6 20                    and    00100000b
     186.  00:56C5  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
