      78:  00:9BFA  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:9BFB  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:9BFD  29                  		add	hl,hl				; hl:=hl*2
