     196.  00:5407  3E 0F                      ld    a,15
     197.  00:5409  D3 A0                      out    [PSG_A],a
     198.  00:540B  DB A2                      in    a,[PSG_R]
     199.  00:540D  E6 BF                      and    10111111b        ;joy0
     200.  00:540F                      
     201.  00:540F  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:5411                      
     203.  00:5411  3E 0E                      ld    a,14
     204.  00:5413  D3 A0                      out    [PSG_A],a
     205.  00:5415  DB A2                      in    a,[PSG_R]
