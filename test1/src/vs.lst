      85:  00:76B0  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:76B1  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:76B4  AF                  		xor	a				; a:=0 (no errors)
      88:  00:76B5  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:76B6  D1                  		pop	de				; Retrieve de from stack
      90:  00:76B7  C1                  		pop	bc				; Retrieve bc from stack
