     112.  00:80EB                      ;primer sumando	(en el ejemplo + (4x7)x16)
     113.  00:80EB  21 CD 88            		LD		HL, habitaciones_juego
     114.  00:80EE  3A A5 86            		LD		 A, (prota_nivel)	
     115.  00:80F1  B7                  		OR		 A
     116.  00:80F2  28 16               		JR		 Z, .no_suma_niveles_previos	;el primero nivel (0) no sumaría ni uno por lo que salto al segundo sumando
     117.  00:80F4                      	;sumo 7 veces el nº de nivel si no es cero
     118.  00:80F4  57                  		LD		 D, A
     119.  00:80F5  06 06               		LD		 B, 6
     120.  00:80F7                      .primer_producto
     121.  00:80F7  82                  		ADD		 A, D
     122.  00:80F8  10 FD               		DJNZ	 .primer_producto
     123.  00:80FA  47                  		LD		 B, A				;guardo el resultado en B
     124.  00:80FB                      
     125.  00:80FB                      ;el resultado previo está en B. Ahoro hay que multiplicar A x 16, lo guardo en DE y lo sumo en HL
     126.  00:80FB                      		;inicializo valores a 0
     127.  00:80FB  AF                  		XOR		 A
     128.  00:80FC  57                  		LD		 D, A
     129.  00:80FD  5F                  		LD		 E, A
     130.  00:80FE  67                  		LD		 H, A
     131.  00:80FF  6F                  		LD		 L, A
     132.  00:8100                      
     133.  00:8100  58                  		LD		 E, B				;pongo el (4x7) en HL (estaba en B)
     134.  00:8101                      		
     135.  00:8101  06 10               		LD		 B, 16
     136.  00:8103                      .segundo_producto
     137.  00:8103  19                  		ADD		HL, DE				;segundo producto x 16
     138.  00:8104  10 FD               		DJNZ	.segundo_producto	
     139.  00:8106                      		
     140.  00:8106                      .fin_seg_prod:
     141.  00:8106                      	;coloco puntero DE al inicio de las habitaciones y le sumo para poner el puntero en el nivel que me interesa
     142.  00:8106  11 CD 88            		LD		DE, habitaciones_juego
     143.  00:8109  19                  		ADD		HL, DE				;suma el nº de bytes de los niveles previos (4 x 7 x 16)
     144.  00:810A                      .no_suma_niveles_previos:
     145.  00:810A                      
     146.  00:810A                      ;ya tengo el puntero HL al inicio del nivel que me interesa
     147.  00:810A                      
     148.  00:810A                      ;segundo sumando (en el ejemplo + 3x16)
     149.  00:810A  3A A9 86            	LD		 A,(prota_pos_mapy)
     150.  00:810D                      [4]	SLA		 A				;equivalente a 3 x 16			
     150.  00:810D  CB 27 CB 27 CB 27 CB 27 
     151.  00:8115                      
     152.  00:8115  5F                  	LD		 E, A
     153.  00:8116  AF                  	XOR		 A
     154.  00:8117  57                  	LD		 D, A
     155.  00:8118  19                  	ADD		HL, DE			;equivale a 4x7x16 (HL) + 3x16 (DE)
     156.  00:8119                      
     157.  00:8119                      ;tercer sumando (en el ejemplo + 5)
     158.  00:8119  3A A8 86            	LD		 A, (prota_pos_mapx)
     159.  00:811C  47                  	LD		 B, A
     160.  00:811D                      .situa_columna:
     161.  00:811D  23 23               [2]	INC		HL
     162.  00:811F  10 FC               	DJNZ	.situa_columna
     163.  00:8121                      
     164.  00:8121                      ;ya tengo el puntero HL al inicio de la fila que me interesa
