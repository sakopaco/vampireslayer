     118:  00:76D4  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:76D6                      		; --- Extract new tone from stream ---
     120:  00:76D6  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:76D7  23                  		inc	hl				; Increment pointer
