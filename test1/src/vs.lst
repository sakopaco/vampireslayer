     146.  00:50DF                      update_controllers_status:
     147.  00:50DF  CD 2A 51                 call  check_8th_keyboard_row
     148.  00:50E2                      [4]  rrc    a
     148.  00:50E2  CB 0F CB 0F CB 0F CB 0F 
     149.  00:50EA  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:50EB                      
     151.  00:50EB  E6 01                    and    #01    		;save L
     152.  00:50ED  CB 27                    sla    a
     153.  00:50EF  CB 27                    sla    a    		;now 00000L00
     154.  00:50F1  4F                       ld    c,a    		;save for later
     155.  00:50F2                      
     156.  00:50F2  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:50F3  CB 3F                    srl    a
     158.  00:50F5  E6 03                    and    00000011b
     159.  00:50F7                      						;we've got 000000DU on a
     160.  00:50F7  B1                       or    c    		;now we've got 00000LDU
     161.  00:50F8  4F                       ld    c,a    		;save for later
     162.  00:50F9                      
     163.  00:50F9  78                       ld    a,b
     164.  00:50FA  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:50FC  B1                       or    c
     166.  00:50FD                           ;finally    000TRLDU
     167.  00:50FD                      
     168.  00:50FD                           ;and    00000100   ;preserve just
     169.  00:50FD  47                       ld    b,a    		;save for later
     170.  00:50FE  CD 38 51                 call  check_4th_keyboard_row      ;check M & P
     171.  00:5101  F5                       push  af
     172.  00:5102  E6 04                    and    00000100b	;filter just M
     173.  00:5104  87                       add    a
     174.  00:5105  87                       add    a
     175.  00:5106  87                       add    a           ;shift 3th to 6th bit
     176.  00:5107  B0                       or    b           	;merge with previous
     177.  00:5108  47                       ld    b,a
     178.  00:5109                      
     179.  00:5109  CD 16 51                 call  read_joy_0
     180.  00:510C  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:510E  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:510F  47                       ld    b,a      	;save for later
     183.  00:5110  F1                       pop    af      	; let's check on P
     184.  00:5111                      
     185.  00:5111  E6 20                    and    00100000b
