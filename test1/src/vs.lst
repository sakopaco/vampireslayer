     150.  00:569D                      
     151.  00:569D  E6 01                    and    #01    		;save L
     152.  00:569F  CB 27                    sla    a
     153.  00:56A1  CB 27                    sla    a    		;now 00000L00
     154.  00:56A3  4F                       ld    c,a    		;save for later
     155.  00:56A4                      
     156.  00:56A4  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:56A5  CB 3F                    srl    a
     158.  00:56A7  E6 03                    and    00000011b
     159.  00:56A9                      						;we've got 000000DU on a
