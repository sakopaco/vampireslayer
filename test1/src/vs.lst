      93:  00:826C                      		IF ( AYFXRELATIVE == 1 )
      94:  00:826C                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:826C                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:826C                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:826C                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:826C                    ~ 		ENDIF
