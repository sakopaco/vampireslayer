     195.  00:53D2  F3                         di
     196.  00:53D3  3E 0F                      ld    a,15
     197.  00:53D5  D3 A0                      out    [PSG_A],a
     198.  00:53D7  DB A2                      in    a,[PSG_R]
     199.  00:53D9  E6 BF                      and    10111111b        ;joy0
     200.  00:53DB                      
     201.  00:53DB  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:53DD                      
     203.  00:53DD  3E 0E                      ld    a,14
     204.  00:53DF  D3 A0                      out    [PSG_A],a
     205.  00:53E1  DB A2                      in    a,[PSG_R]
