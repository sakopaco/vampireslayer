     206:  00:7EAB  B1                  		or	c				; OR mask
     207:  00:7EAC  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
     208:  00:7EAF  3A 49 F2            		ld	a,[ayFX_VOLUME]			; a:=ayFX volume value
     209:  00:7EB2  2A 46 F2            		ld	hl,[ayFX_TONE]			; ayFX tone value
     210:  00:7EB5  C9                  		ret					; Return
