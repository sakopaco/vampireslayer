      46:  00:90B2                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:90B2                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:90B2  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:90B3  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:90B6  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:90B7  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:90B9  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:90BB                      		; --- Set new priority ---
      54:  00:90BB  79                  		ld	a,c				; a:=New priority
      55:  00:90BC  E6 0F               		and	$0F				; We mask the priority
      56:  00:90BE  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:90C1                      
      58:  00:90C1                      		IF ( AYFXRELATIVE == 1 )
      59:  00:90C1                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:90C1                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:90C1                    ~ 			ld	a,15				; a:=15
      62:  00:90C1                    ~ 			sub	c				; a:=15-New priority = relative volume
