     100:  00:9A45                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:9A45  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
