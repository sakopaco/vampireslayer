     145.  00:853A                      ;;update_controllers_status
     146.  00:853A                      ;;=====================================================	
     147.  00:853A                      ;depends on the next rountine
     148.  00:853A                      ;  returns
     149.  00:853A                      ;  a - 0P2TRLDU
     150.  00:853A                      
     151.  00:853A                      update_controllers_status:
     152.  00:853A  CD 85 85                 call  check_8th_keyboard_row
     153.  00:853D                      [4]  rrc    a
     153.  00:853D  CB 0F CB 0F CB 0F CB 0F 
     154.  00:8545  47                       ld    b,a    		;save for later  xxxTRDUL
     155.  00:8546                      
     156.  00:8546  E6 01                    and    #01    		;save L
     157.  00:8548  CB 27                    sla    a
     158.  00:854A  CB 27                    sla    a    		;now 00000L00
     159.  00:854C  4F                       ld    c,a    		;save for later
     160.  00:854D                      
     161.  00:854D  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     162.  00:854E  CB 3F                    srl    a
     163.  00:8550  E6 03                    and    00000011b
     164.  00:8552                      						;we've got 000000DU on a
     165.  00:8552  B1                       or    c    		;now we've got 00000LDU
     166.  00:8553  4F                       ld    c,a    		;save for later
     167.  00:8554                      
     168.  00:8554  78                       ld    a,b
     169.  00:8555  E6 18                    and    00011000b 	;we've got 000TR000
     170.  00:8557  B1                       or    c
     171.  00:8558                           ;finally    000TRLDU
     172.  00:8558                      
     173.  00:8558                           ;and    00000100   ;preserve just
     174.  00:8558  47                       ld    b,a    		;save for later
     175.  00:8559  CD 93 85                 call  check_4th_keyboard_row      ;check M & P
     176.  00:855C  F5                       push  af
     177.  00:855D  E6 04                    and    00000100b	;filter just M
     178.  00:855F  87                       add    a
     179.  00:8560  87                       add    a
     180.  00:8561  87                       add    a           ;shift 3th to 6th bit
     181.  00:8562  B0                       or    b           	;merge with previous
     182.  00:8563  47                       ld    b,a
     183.  00:8564                      
     184.  00:8564  CD 71 85                 call  read_joy_0
     185.  00:8567  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     186.  00:8569  B0                       or    b      		;added 000TRLDU from keyboard
     187.  00:856A  47                       ld    b,a      	;save for later
     188.  00:856B  F1                       pop    af      	; let's check on P
     189.  00:856C                      
     190.  00:856C  E6 20                    and    00100000b
     191.  00:856E  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     192.  00:856F  B0                       or    b
     193.  00:8570                      
     194.  00:8570  C9                       ret
