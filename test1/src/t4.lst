     148.  00:812B                      ;segundo sumando (en el ejemplo + 3x16)
     149.  00:812B  3A 28 86            	LD		 A,(prota_pos_mapy)
     150.  00:812E                      [4]	SLA		 A				;equivalente a 3 x 16			
     150.  00:812E  CB 27 CB 27 CB 27 CB 27 
     151.  00:8136                      
     152.  00:8136  5F                  	LD		 E, A
     153.  00:8137  AF                  	XOR		 A
     154.  00:8138  57                  	LD		 D, A
