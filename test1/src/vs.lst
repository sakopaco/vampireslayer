     135:  00:7E92  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:7E95                      		; --- Extract volume ---
     137:  00:7E95  79                  		ld	a,c				; a:=Control byte
