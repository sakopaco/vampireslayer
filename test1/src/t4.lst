     183.  00:8A11                      
     184.  00:8A11  E6 01                    and    #01    		;save L
     185.  00:8A13  CB 27                    sla    a
     186.  00:8A15  CB 27                    sla    a    		;now 00000L00
     187.  00:8A17  4F                       ld    c,a    		;save for later
     188.  00:8A18                      
     189.  00:8A18  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     190.  00:8A19  CB 3F                    srl    a
     191.  00:8A1B  E6 03                    and    00000011b
     192.  00:8A1D                      						;we've got 000000DU on a
     193.  00:8A1D  B1                       or    c    		;now we've got 00000LDU
     194.  00:8A1E  4F                       ld    c,a    		;save for later
     195.  00:8A1F                      
     196.  00:8A1F  78                       ld    a,b
     197.  00:8A20  E6 18                    and    00011000b 	;we've got 000TR000
     198.  00:8A22  B1                       or    c
     199.  00:8A23                           ;finally    000TRLDU
     200.  00:8A23                      
     201.  00:8A23                           ;and    00000100   ;preserve just
     202.  00:8A23  47                       ld    b,a    		;save for later
     203.  00:8A24  CD 5E 8A                 call  check_4th_keyboard_row      ;check M & P
     204.  00:8A27  F5                       push  af
     205.  00:8A28  E6 04                    and    00000100b	;filter just M
     206.  00:8A2A  87                       add    a
     207.  00:8A2B  87                       add    a
     208.  00:8A2C  87                       add    a           ;shift 3th to 6th bit
     209.  00:8A2D  B0                       or    b           	;merge with previous
     210.  00:8A2E  47                       ld    b,a
     211.  00:8A2F                      
     212.  00:8A2F  CD 3C 8A                 call  read_joy_0
     213.  00:8A32  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     214.  00:8A34  B0                       or    b      		;added 000TRLDU from keyboard
     215.  00:8A35  47                       ld    b,a      	;save for later
     216.  00:8A36  F1                       pop    af      	; let's check on P
     217.  00:8A37                      
     218.  00:8A37  E6 20                    and    00100000b
     219.  00:8A39  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
