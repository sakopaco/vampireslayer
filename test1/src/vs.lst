      29:  00:7E2A                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7E2A                      		; --- INPUT: A -> sound to be played ---
      31:  00:7E2A                      		; ---        C -> sound priority     ---
      32:  00:7E2A  C5                  		push	bc				; Store bc in stack
      33:  00:7E2B  D5                  		push	de				; Store de in stack
      34:  00:7E2C  E5                  		push	hl				; Store hl in stack
      35:  00:7E2D                      		; --- Check if the index is in the bank ---
      36:  00:7E2D  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7E2E  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7E31  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7E32  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7E33  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7E35                      		; The bank has less than 256 samples
      42:  00:7E35  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7E36  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7E37  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7E39  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7E3B                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7E3B                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7E3B  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7E3C  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7E3F  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7E40  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7E42  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7E44                      		; --- Set new priority ---
      54:  00:7E44  79                  		ld	a,c				; a:=New priority
      55:  00:7E45  E6 0F               		and	$0F				; We mask the priority
      56:  00:7E47  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7E4A                      
      58:  00:7E4A                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7E4A                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7E4A                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7E4A                    ~ 			ld	a,15				; a:=15
      62:  00:7E4A                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7E4A                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7E4A                    ~ 			add	a,a				; a:=a*2
      65:  00:7E4A                    ~ 			add	a,a				; a:=a*4
      66:  00:7E4A                    ~ 			add	a,a				; a:=a*8
      67:  00:7E4A                    ~ 			add	a,a				; a:=a*16
