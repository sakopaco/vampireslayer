     152.  00:50B8  CB 27                    sla    a
     153.  00:50BA  CB 27                    sla    a    		;now 00000L00
     154.  00:50BC  4F                       ld    c,a    		;save for later
     155.  00:50BD                      
     156.  00:50BD  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:50BE  CB 3F                    srl    a
     158.  00:50C0  E6 03                    and    00000011b
     159.  00:50C2                      						;we've got 000000DU on a
