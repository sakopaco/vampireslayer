       3:  00:7DDB                      		; --- v1.31	Fixed bug on previous version, only PSG channel C worked
       4:  00:7DDB                      		; --- v1.3	Fixed volume and Relative volume versions on the same file, conditional compilation
       5:  00:7DDB                      		; ---		Support for dynamic or fixed channel allocation
       6:  00:7DDB                      		; --- v1.2f/r	ayFX bank support
       7:  00:7DDB                      		; --- v1.11f/r	If a frame volume is zero then no AYREGS update
       8:  00:7DDB                      		; --- v1.1f/r	Fixed volume for all ayFX streams
       9:  00:7DDB                      		; --- v1.1	Explicit priority (as suggested by AR)
      10:  00:7DDB                      		; --- v1.0f	Bug fixed (error when using noise)
      11:  00:7DDB                      		; --- v1.0	Initial release
      12:  00:7DDB                      
      13:  00:7DDB                      		; --- DEFINE AYFXRELATIVE AS 0 FOR FIXED VOLUME VERSION ---
      14:  00:7DDB                      		; --- DEFINE AYFXRELATIVE AS 1 FOR RELATIVE VOLUME VERSION ---
