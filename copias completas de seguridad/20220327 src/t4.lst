      17:  00:9B90                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:9B90                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:9B90  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:9B93  3E 01               		ld	a,1				; a:=0
      21:  00:9B95  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:9B98  3C                  		inc	a				; Starting channel (=1)
      23:  00:9B99  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:9B9C                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:9B9C  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:9B9E  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:9BA1  C9                  		ret					; Return
      28:  00:9BA2                      
      29:  00:9BA2                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:9BA2                      		; --- INPUT: A -> sound to be played ---
      31:  00:9BA2                      		; ---        C -> sound priority     ---
      32:  00:9BA2  C5                  		push	bc				; Store bc in stack
      33:  00:9BA3  D5                  		push	de				; Store de in stack
      34:  00:9BA4  E5                  		push	hl				; Store hl in stack
      35:  00:9BA5                      		; --- Check if the index is in the bank ---
      36:  00:9BA5  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:9BA6  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:9BA9  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:9BAA  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:9BAB  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:9BAD                      		; The bank has less than 256 samples
      42:  00:9BAD  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:9BAE  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:9BAF  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:9BB1  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:9BB3                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:9BB3                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:9BB3  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:9BB4  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:9BB7  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:9BB8  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:9BBA  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:9BBC                      		; --- Set new priority ---
      54:  00:9BBC  79                  		ld	a,c				; a:=New priority
      55:  00:9BBD  E6 0F               		and	$0F				; We mask the priority
      56:  00:9BBF  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:9BC2                      
      58:  00:9BC2                      		IF ( AYFXRELATIVE == 1 )
      59:  00:9BC2                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:9BC2                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:9BC2                    ~ 			ld	a,15				; a:=15
      62:  00:9BC2                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:9BC2                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:9BC2                    ~ 			add	a,a				; a:=a*2
      65:  00:9BC2                    ~ 			add	a,a				; a:=a*4
      66:  00:9BC2                    ~ 			add	a,a				; a:=a*8
      67:  00:9BC2                    ~ 			add	a,a				; a:=a*16
      68:  00:9BC2                    ~ 			ld	e,a				; e:=a
      69:  00:9BC2                    ~ 			ld	d,0				; de:=a
      70:  00:9BC2                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:9BC2                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:9BC2                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:9BC2                    ~ 		ENDIF
      74:  00:9BC2                      
      75:  00:9BC2                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:9BC2  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:9BC6  13                  		inc	de				; de points to the increments table of the bank
      78:  00:9BC7  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:9BC8  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:9BCA  29                  		add	hl,hl				; hl:=hl*2
      81:  00:9BCB  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:9BCC  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:9BCD  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:9BCE  56                  		ld	d,[hl]				; de:=increment
      85:  00:9BCF  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:9BD0  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:9BD3  AF                  		xor	a				; a:=0 (no errors)
      88:  00:9BD4  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:9BD5  D1                  		pop	de				; Retrieve de from stack
      90:  00:9BD6  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:9BD7  C9                  		ret					; Return
      92:  00:9BD8                      
      93:  00:9BD8                      		IF ( AYFXRELATIVE == 1 )
      94:  00:9BD8                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:9BD8                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:9BD8                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:9BD8                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:9BD8                    ~ 		ENDIF
      99:  00:9BD8                      
     100:  00:9BD8                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:9BD8  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:9BDB  B7                  		or	a				; If priority has bit 7 on...
     103:  00:9BDC  F8                  		ret	m				; ...return
     104:  00:9BDD                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:9BDD  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:9BE0  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:9BE2  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:9BE4  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:9BE7  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:9BE8  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:9BEA  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:9BEC                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:9BEC  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:9BEF  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:9BF0  23                  		inc	hl				; Increment pointer
     116:  00:9BF1                      		; --- Check if there's new tone on stream ---
     117:  00:9BF1  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:9BF3  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:9BF5                      		; --- Extract new tone from stream ---
     120:  00:9BF5  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:9BF6  23                  		inc	hl				; Increment pointer
     122:  00:9BF7  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:9BF8  23                  		inc	hl				; Increment pointer
     124:  00:9BF9  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:9BFD                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:9BFD  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:9BFF  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:9C01                      		; --- Extract new noise from stream ---
     129:  00:9C01  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:9C02  23                  		inc	hl				; Increment pointer
     131:  00:9C03  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:9C05  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:9C07  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:9C0A                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:9C0A  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:9C0D                      		; --- Extract volume ---
     137:  00:9C0D  79                  		ld	a,c				; a:=Control byte
     138:  00:9C0E  E6 0F               		and	$0F				; lower nibble
     139:  00:9C10                      
     140:  00:9C10                      		IF ( AYFXRELATIVE == 1 )
     141:  00:9C10                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:9C10                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:9C10                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:9C10                    ~ 			ld	d,0				; d:=0
     145:  00:9C10                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:9C10                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:9C10                    ~ 			or	a				; If relative volume is zero...
