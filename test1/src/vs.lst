     199.  00:514A  E6 BF                      and    10111111b        ;joy0
     200.  00:514C                      
     201.  00:514C  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:514E                      
     203.  00:514E  3E 0E                      ld    a,14
     204.  00:5150  D3 A0                      out    [PSG_A],a
