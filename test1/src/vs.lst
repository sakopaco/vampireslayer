     195.  00:53DB  F3                         di
     196.  00:53DC  3E 0F                      ld    a,15
     197.  00:53DE  D3 A0                      out    [PSG_A],a
     198.  00:53E0  DB A2                      in    a,[PSG_R]
     199.  00:53E2  E6 BF                      and    10111111b        ;joy0
     200.  00:53E4                      
     201.  00:53E4  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:53E6                      
     203.  00:53E6  3E 0E                      ld    a,14
     204.  00:53E8  D3 A0                      out    [PSG_A],a
     205.  00:53EA  DB A2                      in    a,[PSG_R]
     206.  00:53EC                      
     207.  00:53EC  FB                         ei
     208.  00:53ED  2F                         cpl
