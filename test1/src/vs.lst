     183:  00:7B64  0F                  		rrca					; Rotate right OR mask
     184:  00:7B65  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:7B67                      .PLAY_B:	; --- Play ayFX stream on channel B ---
