     150.  00:91F7  CB 0F CB 0F CB 0F CB 0F 
     151.  00:91FF  47                       ld    b,a    		;save for later  xxxTRDUL
     152.  00:9200                      
     153.  00:9200  E6 01                    and    #01    		;save L
     154.  00:9202  CB 27                    sla    a
     155.  00:9204  CB 27                    sla    a    		;now 00000L00
     156.  00:9206  4F                       ld    c,a    		;save for later
     157.  00:9207                      
     158.  00:9207  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     159.  00:9208  CB 3F                    srl    a
     160.  00:920A  E6 03                    and    00000011b
     161.  00:920C                      						;we've got 000000DU on a
     162.  00:920C  B1                       or    c    		;now we've got 00000LDU
     163.  00:920D  4F                       ld    c,a    		;save for later
     164.  00:920E                      
     165.  00:920E  78                       ld    a,b
     166.  00:920F  E6 18                    and    00011000b 	;we've got 000TR000
     167.  00:9211  B1                       or    c
     168.  00:9212                           ;finally    000TRLDU
     169.  00:9212                      
     170.  00:9212                           ;and    00000100   ;preserve just
     171.  00:9212  47                       ld    b,a    		;save for later
     172.  00:9213  CD 4D 92                 call  check_4th_keyboard_row      ;check M & P
