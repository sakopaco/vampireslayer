     509.  00:4A78                      .deteccioncolision_paso1:
     510.  00:4A78  FD 21 83 C1         		LD		IY, puntomira	;IY punto de mira / IX puerta
     511.  00:4A7C  FD 7E 00            		LD		 A, (IY + ESTRUCTURA_PUNTOMIRA.posx)
     512.  00:4A7F  C6 08               		ADD		 8				;8-es fijo, offset del punto de mira ya que se mueve según la esquina superior izquierda y el centro del punto de mira está en el centro del sprite
     513.  00:4A81                      	
     514.  00:4A81                      		;ya tengo en A la coordenada X del centro del punto de mira					
     515.  00:4A81  DD 96 01            		SUB		(IX + ESTRUCTURA_PUERTA.posx)	;le resto el punto x en la puerta
     516.  00:4A84                      	
     517.  00:4A84  D2 89 4A            		JP		NC, .deteccioncolision_paso2	;si no es negativo comparo con el radio
     518.  00:4A87                      
     519.  00:4A87  ED 44               		NEG										;si es negativo lo niego (valor absoluto)
     520.  00:4A89                      	
     521.  00:4A89                      .deteccioncolision_paso2:
     522.  00:4A89  DD BE 03            		CP		(IX + ESTRUCTURA_PUERTA.radiox)	;comparo con el radio X de la puerta
     523.  00:4A8C                      	
     524.  00:4A8C  DA 91 4A            		JP		 C, .deteccioncolision_paso3	;SI NC la distancia es >= por lo que sale y no es necesario verificar nada más
     525.  00:4A8F                      	
     526.  00:4A8F  AF                  		XOR		 A								;el resultado es falso y se guarda en A y ya no hay que seguir coprobando
     527.  00:4A90  C9                  		RET
     528.  00:4A91                      	
     529.  00:4A91                      .deteccioncolision_paso3:					;la distancia X es válida, comprobamos la distancia Y
     530.  00:4A91  FD 7E 01            		LD		 A, (IY + ESTRUCTURA_PUNTOMIRA.posy)
     531.  00:4A94  C6 08               		ADD		 8								;le sumo el offset del punto de mira (8 es fijo)
     532.  00:4A96                      
     533.  00:4A96                      		;ya tengo en A la coordenada Y del centro del punto de mira					
     534.  00:4A96  DD 96 02            		SUB		(IX + ESTRUCTURA_PUERTA.posy)	;le resto el punto y en la puerta
     535.  00:4A99                      	
     536.  00:4A99  D2 9E 4A            		JP		NC, .deteccioncolision_paso4	;si no es negativo comparo con el radio
     537.  00:4A9C                      
     538.  00:4A9C  ED 44               		NEG										;si es negativo lo niego (valor absoluto)
     539.  00:4A9E                      
     540.  00:4A9E                      .deteccioncolision_paso4:
     541.  00:4A9E  DD BE 04            		CP		(IX + ESTRUCTURA_PUERTA.radioy)	;comparo con el radio Y de la puerta
     542.  00:4AA1                      
     543.  00:4AA1  DA A6 4A            		JP		 C, .deteccioncolision_paso5	;SI NC la distancia es >= por lo que sale y no es necesario verificar nada más
     544.  00:4AA4                      	
     545.  00:4AA4  AF                  		XOR		 A								;el resultado es falso y se guarda en A un 0 y al ser la 2º comprobación salimos
     546.  00:4AA5  C9                  		RET
