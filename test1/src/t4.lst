      17:  00:8F02                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:8F02                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:8F02  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
