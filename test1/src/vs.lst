     118:  00:8287  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:8289                      		; --- Extract new tone from stream ---
     120:  00:8289  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:828A  23                  		inc	hl				; Increment pointer
     122:  00:828B  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:828C  23                  		inc	hl				; Increment pointer
