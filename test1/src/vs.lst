     151.  00:569C  E6 01                    and    #01    		;save L
     152.  00:569E  CB 27                    sla    a
     153.  00:56A0  CB 27                    sla    a    		;now 00000L00
     154.  00:56A2  4F                       ld    c,a    		;save for later
     155.  00:56A3                      
     156.  00:56A3  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
