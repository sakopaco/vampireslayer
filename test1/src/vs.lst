     134:  00:76EB                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:76EB  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:76EE                      		; --- Extract volume ---
     137:  00:76EE  79                  		ld	a,c				; a:=Control byte
     138:  00:76EF  E6 0F               		and	$0F				; lower nibble
     139:  00:76F1                      
     140:  00:76F1                      		IF ( AYFXRELATIVE == 1 )
     141:  00:76F1                    ~ 			; --- Fix the volume using PT3 Volume Table ---
