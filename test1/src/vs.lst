     180:  00:7AB5  C9                  		ret					; Return
     181:  00:7AB6                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:7AB6  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:7AB8  0F                  		rrca					; Rotate right OR mask
     184:  00:7AB9  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:7ABB                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:7ABB  CD D8 7A            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:7ABE  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:7AC1  CB 49               		bit	1,c				; If tone is off...
     189:  00:7AC3  C0                  		ret	nz				; ...return
     190:  00:7AC4  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:7AC7  C9                  		ret					; Return
     192:  00:7AC8                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:7AC8  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:7ACA  0F                  		rrca					; Rotate right OR mask
     195:  00:7ACB                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:7ACB  CD D8 7A            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:7ACE  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:7AD1  CB 41               		bit	0,c				; If tone is off...
     199:  00:7AD3  C0                  		ret	nz				; ...return
     200:  00:7AD4  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:7AD7  C9                  		ret					; Return
