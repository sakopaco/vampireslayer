     101.  00:80EA                      ; función: 	busca las puertas que hay que pintar egún la posición del usuario y las pone en A
     102.  00:80EA                      ; entrada: 	prota.pos_mapy, prota.pos_mapx, prota_nivel habitaciones_juego
     103.  00:80EA                      ; salida: 	habitacion_actual
     104.  00:80EA                      ; toca:		A
     105.  00:80EA                      ;ejemplo: nivel 4, posx 5 y posy 3
     106.  00:80EA                      ;  (4 x 7) x 16 (se necesitan dos bytes y el prod de 16 es slr 4 veces)
     107.  00:80EA                      ; +(3 x 16) 	de la fila (se necesita 1 y byte el prod de 16 es slr 4 veces)
     108.  00:80EA                      ; +5        	de la columna 
     109.  00:80EA                      localiza_info_habitacion:
     110.  00:80EA  D9                  		EXX
