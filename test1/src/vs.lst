     140.  00:50F5                      ;;update_controllers_status
     141.  00:50F5                      ;;=====================================================	
     142.  00:50F5                      ;depends on the next rountine
     143.  00:50F5                      ;  returns
     144.  00:50F5                      ;  a - 0P2TRLDU
     145.  00:50F5                      
     146.  00:50F5                      update_controllers_status:
     147.  00:50F5  CD 40 51                 call  check_8th_keyboard_row
     148.  00:50F8                      [4]  rrc    a
     148.  00:50F8  CB 0F CB 0F CB 0F CB 0F 
     149.  00:5100  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:5101                      
     151.  00:5101  E6 01                    and    #01    		;save L
     152.  00:5103  CB 27                    sla    a
     153.  00:5105  CB 27                    sla    a    		;now 00000L00
     154.  00:5107  4F                       ld    c,a    		;save for later
     155.  00:5108                      
     156.  00:5108  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:5109  CB 3F                    srl    a
     158.  00:510B  E6 03                    and    00000011b
     159.  00:510D                      						;we've got 000000DU on a
     160.  00:510D  B1                       or    c    		;now we've got 00000LDU
     161.  00:510E  4F                       ld    c,a    		;save for later
     162.  00:510F                      
     163.  00:510F  78                       ld    a,b
     164.  00:5110  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5112  B1                       or    c
     166.  00:5113                           ;finally    000TRLDU
     167.  00:5113                      
     168.  00:5113                           ;and    00000100   ;preserve just
     169.  00:5113  47                       ld    b,a    		;save for later
     170.  00:5114  CD 4E 51                 call  check_4th_keyboard_row      ;check M & P
     171.  00:5117  F5                       push  af
     172.  00:5118  E6 04                    and    00000100b	;filter just M
     173.  00:511A  87                       add    a
     174.  00:511B  87                       add    a
     175.  00:511C  87                       add    a           ;shift 3th to 6th bit
     176.  00:511D  B0                       or    b           	;merge with previous
     177.  00:511E  47                       ld    b,a
     178.  00:511F                      
     179.  00:511F  CD 2C 51                 call  read_joy_0
     180.  00:5122  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:5124  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:5125  47                       ld    b,a      	;save for later
     183.  00:5126  F1                       pop    af      	; let's check on P
     184.  00:5127                      
     185.  00:5127  E6 20                    and    00100000b
     186.  00:5129  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:512A  B0                       or    b
     188.  00:512B                      
     189.  00:512B  C9                       ret
     190.  00:512C                      
     191.  00:512C                      
