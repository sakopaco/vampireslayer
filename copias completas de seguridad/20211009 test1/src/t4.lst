      23:  00:8D0D  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:8D10                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:8D10  3E FF               		ld	a,255				; Lowest ayFX priority
