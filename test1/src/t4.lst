     150:  00:974C  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:974F  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:9750                      		; -------------------------------------
     153:  00:9750                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:9750                      		; -------------------------------------
     155:  00:9750                      		; --- Set noise channel ---
