     195.  00:53EE  F3                         di
     196.  00:53EF  3E 0F                      ld    a,15
     197.  00:53F1  D3 A0                      out    [PSG_A],a
     198.  00:53F3  DB A2                      in    a,[PSG_R]
     199.  00:53F5  E6 BF                      and    10111111b        ;joy0
     200.  00:53F7                      
     201.  00:53F7  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:53F9                      
     203.  00:53F9  3E 0E                      ld    a,14
     204.  00:53FB  D3 A0                      out    [PSG_A],a
     205.  00:53FD  DB A2                      in    a,[PSG_R]
