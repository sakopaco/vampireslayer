     140.  00:53CF                      ;;update_controllers_status
     141.  00:53CF                      ;;=====================================================	
     142.  00:53CF                      ;depends on the next rountine
     143.  00:53CF                      ;  returns
     144.  00:53CF                      ;  a - 0P2TRLDU
     145.  00:53CF                      
     146.  00:53CF                      update_controllers_status:
     147.  00:53CF  CD 1A 54                 call  check_8th_keyboard_row
     148.  00:53D2                      [4]  rrc    a
     148.  00:53D2  CB 0F CB 0F CB 0F CB 0F 
     149.  00:53DA  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:53DB                      
     151.  00:53DB  E6 01                    and    #01    		;save L
     152.  00:53DD  CB 27                    sla    a
     153.  00:53DF  CB 27                    sla    a    		;now 00000L00
     154.  00:53E1  4F                       ld    c,a    		;save for later
     155.  00:53E2                      
     156.  00:53E2  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:53E3  CB 3F                    srl    a
     158.  00:53E5  E6 03                    and    00000011b
     159.  00:53E7                      						;we've got 000000DU on a
     160.  00:53E7  B1                       or    c    		;now we've got 00000LDU
     161.  00:53E8  4F                       ld    c,a    		;save for later
     162.  00:53E9                      
     163.  00:53E9  78                       ld    a,b
     164.  00:53EA  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:53EC  B1                       or    c
     166.  00:53ED                           ;finally    000TRLDU
     167.  00:53ED                      
     168.  00:53ED                           ;and    00000100   ;preserve just
     169.  00:53ED  47                       ld    b,a    		;save for later
     170.  00:53EE  CD 28 54                 call  check_4th_keyboard_row      ;check M & P
     171.  00:53F1  F5                       push  af
     172.  00:53F2  E6 04                    and    00000100b	;filter just M
     173.  00:53F4  87                       add    a
     174.  00:53F5  87                       add    a
     175.  00:53F6  87                       add    a           ;shift 3th to 6th bit
     176.  00:53F7  B0                       or    b           	;merge with previous
     177.  00:53F8  47                       ld    b,a
     178.  00:53F9                      
     179.  00:53F9  CD 06 54                 call  read_joy_0
     180.  00:53FC  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:53FE  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:53FF  47                       ld    b,a      	;save for later
     183.  00:5400  F1                       pop    af      	; let's check on P
     184.  00:5401                      
     185.  00:5401  E6 20                    and    00100000b
     186.  00:5403  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:5404  B0                       or    b
     188.  00:5405                      
     189.  00:5405  C9                       ret
     190.  00:5406                      
     191.  00:5406                      
