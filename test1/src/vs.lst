      31:  00:7BB8                      		; ---        C -> sound priority     ---
      32:  00:7BB8  C5                  		push	bc				; Store bc in stack
      33:  00:7BB9  D5                  		push	de				; Store de in stack
      34:  00:7BBA  E5                  		push	hl				; Store hl in stack
      35:  00:7BBB                      		; --- Check if the index is in the bank ---
      36:  00:7BBB  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7BBC  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7BBF  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7BC0  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7BC1  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7BC3                      		; The bank has less than 256 samples
