     146.  00:5125                      update_controllers_status:
     147.  00:5125  CD 70 51                 call  check_8th_keyboard_row
     148.  00:5128                      [4]  rrc    a
     148.  00:5128  CB 0F CB 0F CB 0F CB 0F 
     149.  00:5130  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:5131                      
     151.  00:5131  E6 01                    and    #01    		;save L
     152.  00:5133  CB 27                    sla    a
     153.  00:5135  CB 27                    sla    a    		;now 00000L00
     154.  00:5137  4F                       ld    c,a    		;save for later
     155.  00:5138                      
     156.  00:5138  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:5139  CB 3F                    srl    a
     158.  00:513B  E6 03                    and    00000011b
     159.  00:513D                      						;we've got 000000DU on a
     160.  00:513D  B1                       or    c    		;now we've got 00000LDU
     161.  00:513E  4F                       ld    c,a    		;save for later
     162.  00:513F                      
     163.  00:513F  78                       ld    a,b
     164.  00:5140  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5142  B1                       or    c
     166.  00:5143                           ;finally    000TRLDU
     167.  00:5143                      
     168.  00:5143                           ;and    00000100   ;preserve just
     169.  00:5143  47                       ld    b,a    		;save for later
     170.  00:5144  CD 7E 51                 call  check_4th_keyboard_row      ;check M & P
     171.  00:5147  F5                       push  af
     172.  00:5148  E6 04                    and    00000100b	;filter just M
     173.  00:514A  87                       add    a
     174.  00:514B  87                       add    a
     175.  00:514C  87                       add    a           ;shift 3th to 6th bit
     176.  00:514D  B0                       or    b           	;merge with previous
     177.  00:514E  47                       ld    b,a
     178.  00:514F                      
     179.  00:514F  CD 5C 51                 call  read_joy_0
     180.  00:5152  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:5154  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:5155  47                       ld    b,a      	;save for later
     183.  00:5156  F1                       pop    af      	; let's check on P
     184.  00:5157                      
     185.  00:5157  E6 20                    and    00100000b
     186.  00:5159  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:515A  B0                       or    b
