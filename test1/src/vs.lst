     149.  00:518E  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:518F                      
     151.  00:518F  E6 01                    and    #01    		;save L
     152.  00:5191  CB 27                    sla    a
     153.  00:5193  CB 27                    sla    a    		;now 00000L00
     154.  00:5195  4F                       ld    c,a    		;save for later
     155.  00:5196                      
     156.  00:5196  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:5197  CB 3F                    srl    a
     158.  00:5199  E6 03                    and    00000011b
     159.  00:519B                      						;we've got 000000DU on a
     160.  00:519B  B1                       or    c    		;now we've got 00000LDU
     161.  00:519C  4F                       ld    c,a    		;save for later
     162.  00:519D                      
     163.  00:519D  78                       ld    a,b
     164.  00:519E  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:51A0  B1                       or    c
