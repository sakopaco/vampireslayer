     174.  00:52EC  87                       add    a
     175.  00:52ED  87                       add    a           ;shift 3th to 6th bit
     176.  00:52EE  B0                       or    b           	;merge with previous
     177.  00:52EF  47                       ld    b,a
     178.  00:52F0                      
     179.  00:52F0  CD FD 52                 call  read_joy_0
