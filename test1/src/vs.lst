      36:  00:7BD1  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7BD2  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7BD5  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7BD6  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7BD7  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
