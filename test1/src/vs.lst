     149.  00:5561  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:5562                      
     151.  00:5562  E6 01                    and    #01    		;save L
     152.  00:5564  CB 27                    sla    a
     153.  00:5566  CB 27                    sla    a    		;now 00000L00
     154.  00:5568  4F                       ld    c,a    		;save for later
     155.  00:5569                      
     156.  00:5569  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:556A  CB 3F                    srl    a
     158.  00:556C  E6 03                    and    00000011b
     159.  00:556E                      						;we've got 000000DU on a
     160.  00:556E  B1                       or    c    		;now we've got 00000LDU
     161.  00:556F  4F                       ld    c,a    		;save for later
     162.  00:5570                      
     163.  00:5570  78                       ld    a,b
     164.  00:5571  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5573  B1                       or    c
     166.  00:5574                           ;finally    000TRLDU
     167.  00:5574                      
     168.  00:5574                           ;and    00000100   ;preserve just
     169.  00:5574  47                       ld    b,a    		;save for later
     170.  00:5575  CD AF 55                 call  check_4th_keyboard_row      ;check M & P
     171.  00:5578  F5                       push  af
     172.  00:5579  E6 04                    and    00000100b	;filter just M
     173.  00:557B  87                       add    a
     174.  00:557C  87                       add    a
     175.  00:557D  87                       add    a           ;shift 3th to 6th bit
     176.  00:557E  B0                       or    b           	;merge with previous
     177.  00:557F  47                       ld    b,a
     178.  00:5580                      
     179.  00:5580  CD 8D 55                 call  read_joy_0
     180.  00:5583  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:5585  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:5586  47                       ld    b,a      	;save for later
     183.  00:5587  F1                       pop    af      	; let's check on P
     184.  00:5588                      
     185.  00:5588  E6 20                    and    00100000b
     186.  00:558A  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
