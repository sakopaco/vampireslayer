     143.  00:8B6E                      ;;=====================================================	
     144.  00:8B6E                      ;depends on the next rountine
     145.  00:8B6E                      ;  returns
     146.  00:8B6E                      ;  a - 0P2TRLDU
     147.  00:8B6E                      
     148.  00:8B6E                      update_controllers_status:
     149.  00:8B6E  CD B9 8B                 call  check_8th_keyboard_row
     150.  00:8B71                      [4]  rrc    a
     150.  00:8B71  CB 0F CB 0F CB 0F CB 0F 
     151.  00:8B79  47                       ld    b,a    		;save for later  xxxTRDUL
     152.  00:8B7A                      
     153.  00:8B7A  E6 01                    and    #01    		;save L
     154.  00:8B7C  CB 27                    sla    a
     155.  00:8B7E  CB 27                    sla    a    		;now 00000L00
     156.  00:8B80  4F                       ld    c,a    		;save for later
     157.  00:8B81                      
     158.  00:8B81  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     159.  00:8B82  CB 3F                    srl    a
     160.  00:8B84  E6 03                    and    00000011b
     161.  00:8B86                      						;we've got 000000DU on a
     162.  00:8B86  B1                       or    c    		;now we've got 00000LDU
     163.  00:8B87  4F                       ld    c,a    		;save for later
     164.  00:8B88                      
     165.  00:8B88  78                       ld    a,b
     166.  00:8B89  E6 18                    and    00011000b 	;we've got 000TR000
     167.  00:8B8B  B1                       or    c
     168.  00:8B8C                           ;finally    000TRLDU
     169.  00:8B8C                      
     170.  00:8B8C                           ;and    00000100   ;preserve just
     171.  00:8B8C  47                       ld    b,a    		;save for later
     172.  00:8B8D  CD C7 8B                 call  check_4th_keyboard_row      ;check M & P
     173.  00:8B90  F5                       push  af
     174.  00:8B91  E6 04                    and    00000100b	;filter just M
     175.  00:8B93  87                       add    a
     176.  00:8B94  87                       add    a
     177.  00:8B95  87                       add    a           ;shift 3th to 6th bit
     178.  00:8B96  B0                       or    b           	;merge with previous
     179.  00:8B97  47                       ld    b,a
     180.  00:8B98                      
     181.  00:8B98  CD A5 8B                 call  read_joy_0
     182.  00:8B9B  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     183.  00:8B9D  B0                       or    b      		;added 000TRLDU from keyboard
     184.  00:8B9E  47                       ld    b,a      	;save for later
     185.  00:8B9F  F1                       pop    af      	; let's check on P
     186.  00:8BA0                      
     187.  00:8BA0  E6 20                    and    00100000b
     188.  00:8BA2  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     189.  00:8BA3  B0                       or    b
