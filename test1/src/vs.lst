     199.  00:518C  E6 BF                      and    10111111b        ;joy0
     200.  00:518E                      
     201.  00:518E  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:5190                      
     203.  00:5190  3E 0E                      ld    a,14
     204.  00:5192  D3 A0                      out    [PSG_A],a
     205.  00:5194  DB A2                      in    a,[PSG_R]
