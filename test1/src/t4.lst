     130.  00:80CA  82                  		ADD		 A, D
     131.  00:80CB  10 FD               		DJNZ	 .primer_producto
     132.  00:80CD  47                  		LD		 B, A				;guardo el resultado en B
     133.  00:80CE                      
     134.  00:80CE                      ;el resultado previo está en B. Ahoro hay que multiplicar A x 16, lo guardo en DE y lo sumo en HL
     135.  00:80CE                      		;inicializo valores a 0
     136.  00:80CE  AF                  		XOR		 A
     137.  00:80CF  57                  		LD		 D, A
     138.  00:80D0  5F                  		LD		 E, A
     139.  00:80D1  67                  		LD		 H, A
     140.  00:80D2  6F                  		LD		 L, A
     141.  00:80D3                      
     142.  00:80D3  58                  		LD		 E, B				;pongo el (4x7) en HL (estaba en B)
     143.  00:80D4                      		
     144.  00:80D4  06 10               		LD		 B, 16
     145.  00:80D6                      .segundo_producto
     146.  00:80D6  19                  		ADD		HL, DE				;segundo producto x 16
     147.  00:80D7  10 FD               		DJNZ	.segundo_producto	
     148.  00:80D9                      		
