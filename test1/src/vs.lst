     150.  00:53E5                      
     151.  00:53E5  E6 01                    and    #01    		;save L
     152.  00:53E7  CB 27                    sla    a
     153.  00:53E9  CB 27                    sla    a    		;now 00000L00
     154.  00:53EB  4F                       ld    c,a    		;save for later
     155.  00:53EC                      
     156.  00:53EC  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
