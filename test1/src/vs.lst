      17:  00:75B5                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:75B5                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:75B5  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
