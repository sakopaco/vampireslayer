     151.  00:55BE  E6 01                    and    #01    		;save L
     152.  00:55C0  CB 27                    sla    a
     153.  00:55C2  CB 27                    sla    a    		;now 00000L00
     154.  00:55C4  4F                       ld    c,a    		;save for later
     155.  00:55C5                      
     156.  00:55C5  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:55C6  CB 3F                    srl    a
     158.  00:55C8  E6 03                    and    00000011b
     159.  00:55CA                      						;we've got 000000DU on a
     160.  00:55CA  B1                       or    c    		;now we've got 00000LDU
     161.  00:55CB  4F                       ld    c,a    		;save for later
     162.  00:55CC                      
     163.  00:55CC  78                       ld    a,b
     164.  00:55CD  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:55CF  B1                       or    c
     166.  00:55D0                           ;finally    000TRLDU
     167.  00:55D0                      
     168.  00:55D0                           ;and    00000100   ;preserve just
     169.  00:55D0  47                       ld    b,a    		;save for later
     170.  00:55D1  CD 0B 56                 call  check_4th_keyboard_row      ;check M & P
     171.  00:55D4  F5                       push  af
     172.  00:55D5  E6 04                    and    00000100b	;filter just M
     173.  00:55D7  87                       add    a
     174.  00:55D8  87                       add    a
     175.  00:55D9  87                       add    a           ;shift 3th to 6th bit
     176.  00:55DA  B0                       or    b           	;merge with previous
     177.  00:55DB  47                       ld    b,a
     178.  00:55DC                      
     179.  00:55DC  CD E9 55                 call  read_joy_0
     180.  00:55DF  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:55E1  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:55E2  47                       ld    b,a      	;save for later
     183.  00:55E3  F1                       pop    af      	; let's check on P
     184.  00:55E4                      
     185.  00:55E4  E6 20                    and    00100000b
     186.  00:55E6  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:55E7  B0                       or    b
