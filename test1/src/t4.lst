      83:  00:963A  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:963B  56                  		ld	d,[hl]				; de:=increment
      85:  00:963C  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:963D  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:9640  AF                  		xor	a				; a:=0 (no errors)
      88:  00:9641  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:9642  D1                  		pop	de				; Retrieve de from stack
      90:  00:9643  C1                  		pop	bc				; Retrieve bc from stack
