     100:  00:9256                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:9256  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:9259  B7                  		or	a				; If priority has bit 7 on...
     103:  00:925A  F8                  		ret	m				; ...return
     104:  00:925B                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:925B  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:925E  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:9260  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:9262  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:9265  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:9266  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:9268  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:926A                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:926A  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:926D  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:926E  23                  		inc	hl				; Increment pointer
     116:  00:926F                      		; --- Check if there's new tone on stream ---
     117:  00:926F  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:9271  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:9273                      		; --- Extract new tone from stream ---
     120:  00:9273  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:9274  23                  		inc	hl				; Increment pointer
     122:  00:9275  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:9276  23                  		inc	hl				; Increment pointer
     124:  00:9277  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:927B                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:927B  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:927D  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:927F                      		; --- Extract new noise from stream ---
     129:  00:927F  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:9280  23                  		inc	hl				; Increment pointer
     131:  00:9281  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:9283  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:9285  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:9288                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:9288  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:928B                      		; --- Extract volume ---
     137:  00:928B  79                  		ld	a,c				; a:=Control byte
     138:  00:928C  E6 0F               		and	$0F				; lower nibble
     139:  00:928E                      
     140:  00:928E                      		IF ( AYFXRELATIVE == 1 )
     141:  00:928E                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:928E                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:928E                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:928E                    ~ 			ld	d,0				; d:=0
     145:  00:928E                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:928E                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:928E                    ~ 			or	a				; If relative volume is zero...
     148:  00:928E                    ~ 		ENDIF
     149:  00:928E                      
     150:  00:928E  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:9291  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:9292                      		; -------------------------------------
     153:  00:9292                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:9292                      		; -------------------------------------
     155:  00:9292                      		; --- Set noise channel ---
     156:  00:9292  CB 79               		bit	7,c				; If noise is off...
     157:  00:9294  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:9296  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
