       9.  00:61BA                      ; función: 	inicializa: replayer de música, player de efectos e interrupciones
      10.  00:61BA                      ; entrada: 	-
      11.  00:61BA                      ; salida: 	-
      12.  00:61BA                      ; toca: 	HL, A
      13.  00:61BA                      inicializa_replayer_efectos_interrupciones:
      14.  00:61BA  F3                  	DI
      15.  00:61BB                      
      16.  00:61BB                      	;inicializacion de replayer musical
      17.  00:61BB  21 63 68            	LD		HL, song-99			; hl <- initial address of module - 99
      18.  00:61BE  CD FA 61            	CALL	PT3_INIT			; Inits PT3 player
