      45:  00:7E1C  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7E1E                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7E1E                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7E1E  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7E1F  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7E22  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7E23  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7E25  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7E27                      		; --- Set new priority ---
      54:  00:7E27  79                  		ld	a,c				; a:=New priority
      55:  00:7E28  E6 0F               		and	$0F				; We mask the priority
      56:  00:7E2A  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7E2D                      
      58:  00:7E2D                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7E2D                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7E2D                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7E2D                    ~ 			ld	a,15				; a:=15
      62:  00:7E2D                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7E2D                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7E2D                    ~ 			add	a,a				; a:=a*2
      65:  00:7E2D                    ~ 			add	a,a				; a:=a*4
      66:  00:7E2D                    ~ 			add	a,a				; a:=a*8
      67:  00:7E2D                    ~ 			add	a,a				; a:=a*16
