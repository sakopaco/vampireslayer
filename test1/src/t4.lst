     168.  00:8146  7E                  	LD		 A, (HL)
     169.  00:8147  DD 77 00            	LD		(IX), A
     170.  00:814A  23                  	INC		HL
     171.  00:814B  7E                  	LD		 A, (HL)
     172.  00:814C  DD 77 01            	LD		(IX+1), A
