     100:  00:9209                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:9209  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:920C  B7                  		or	a				; If priority has bit 7 on...
     103:  00:920D  F8                  		ret	m				; ...return
     104:  00:920E                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:920E  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:9211  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:9213  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:9215  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:9218  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:9219  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:921B  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:921D                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:921D  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:9220  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:9221  23                  		inc	hl				; Increment pointer
     116:  00:9222                      		; --- Check if there's new tone on stream ---
     117:  00:9222  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:9224  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:9226                      		; --- Extract new tone from stream ---
     120:  00:9226  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:9227  23                  		inc	hl				; Increment pointer
     122:  00:9228  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:9229  23                  		inc	hl				; Increment pointer
     124:  00:922A  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:922E                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:922E  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:9230  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:9232                      		; --- Extract new noise from stream ---
     129:  00:9232  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:9233  23                  		inc	hl				; Increment pointer
     131:  00:9234  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:9236  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:9238  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:923B                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:923B  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:923E                      		; --- Extract volume ---
     137:  00:923E  79                  		ld	a,c				; a:=Control byte
     138:  00:923F  E6 0F               		and	$0F				; lower nibble
     139:  00:9241                      
     140:  00:9241                      		IF ( AYFXRELATIVE == 1 )
     141:  00:9241                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:9241                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:9241                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:9241                    ~ 			ld	d,0				; d:=0
     145:  00:9241                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:9241                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:9241                    ~ 			or	a				; If relative volume is zero...
     148:  00:9241                    ~ 		ENDIF
     149:  00:9241                      
     150:  00:9241  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:9244  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:9245                      		; -------------------------------------
     153:  00:9245                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:9245                      		; -------------------------------------
     155:  00:9245                      		; --- Set noise channel ---
     156:  00:9245  CB 79               		bit	7,c				; If noise is off...
     157:  00:9247  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:9249  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:924C  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:924F                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:924F  79                  		ld	a,c				; a:=Control byte
     162:  00:9250  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:9252  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:9254  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:9255                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:9255  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:9256  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:9257  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:9259                      		; --- Dump to correct channel ---
     170:  00:9259  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:925C  46                  		ld	b,[hl]				; Channel counter
     172:  00:925D                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:925D  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:925F                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:925F  CD 8E 92            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:9262  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:9265  CB 51               		bit	2,c				; If tone is off...
     178:  00:9267  C0                  		ret	nz				; ...return
     179:  00:9268  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:926B  C9                  		ret					; Return
     181:  00:926C                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:926C  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:926E  0F                  		rrca					; Rotate right OR mask
     184:  00:926F  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:9271                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:9271  CD 8E 92            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:9274  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:9277  CB 49               		bit	1,c				; If tone is off...
     189:  00:9279  C0                  		ret	nz				; ...return
     190:  00:927A  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:927D  C9                  		ret					; Return
     192:  00:927E                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:927E  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:9280  0F                  		rrca					; Rotate right OR mask
     195:  00:9281                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:9281  CD 8E 92            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:9284  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:9287  CB 41               		bit	0,c				; If tone is off...
     199:  00:9289  C0                  		ret	nz				; ...return
     200:  00:928A  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:928D  C9                  		ret					; Return
     202:  00:928E                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:928E  4F                  		ld	c,a				; c:=OR mask
     204:  00:928F  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:9292  A2                  		and	d				; AND mask
     206:  00:9293  B1                  		or	c				; OR mask
     207:  00:9294  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
     208:  00:9297  3A 49 F2            		ld	a,[ayFX_VOLUME]			; a:=ayFX volume value
     209:  00:929A  2A 46 F2            		ld	hl,[ayFX_TONE]			; ayFX tone value
     210:  00:929D  C9                  		ret					; Return
