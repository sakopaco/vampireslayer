      23:  00:90C0  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:90C3                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:90C3  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:90C5  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:90C8  C9                  		ret					; Return
