     160:  00:7A99                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:7A99  79                  		ld	a,c				; a:=Control byte
     162:  00:7A9A  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:7A9C  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:7A9E  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:7A9F                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:7A9F  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:7AA0  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:7AA1  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
