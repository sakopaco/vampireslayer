      17:  00:7DDB                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7DDB                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7DDB  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7DDE  3E 01               		ld	a,1				; a:=0
      21:  00:7DE0  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7DE3  3C                  		inc	a				; Starting channel (=1)
      23:  00:7DE4  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7DE7                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7DE7  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7DE9  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7DEC  C9                  		ret					; Return
      28:  00:7DED                      
      29:  00:7DED                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7DED                      		; --- INPUT: A -> sound to be played ---
      31:  00:7DED                      		; ---        C -> sound priority     ---
      32:  00:7DED  C5                  		push	bc				; Store bc in stack
      33:  00:7DEE  D5                  		push	de				; Store de in stack
      34:  00:7DEF  E5                  		push	hl				; Store hl in stack
      35:  00:7DF0                      		; --- Check if the index is in the bank ---
      36:  00:7DF0  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7DF1  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7DF4  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7DF5  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7DF6  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7DF8                      		; The bank has less than 256 samples
      42:  00:7DF8  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7DF9  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7DFA  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7DFC  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7DFE                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7DFE                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7DFE  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7DFF  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7E02  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7E03  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7E05  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7E07                      		; --- Set new priority ---
      54:  00:7E07  79                  		ld	a,c				; a:=New priority
      55:  00:7E08  E6 0F               		and	$0F				; We mask the priority
      56:  00:7E0A  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7E0D                      
      58:  00:7E0D                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7E0D                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7E0D                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7E0D                    ~ 			ld	a,15				; a:=15
      62:  00:7E0D                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7E0D                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7E0D                    ~ 			add	a,a				; a:=a*2
      65:  00:7E0D                    ~ 			add	a,a				; a:=a*4
      66:  00:7E0D                    ~ 			add	a,a				; a:=a*8
      67:  00:7E0D                    ~ 			add	a,a				; a:=a*16
      68:  00:7E0D                    ~ 			ld	e,a				; e:=a
      69:  00:7E0D                    ~ 			ld	d,0				; de:=a
      70:  00:7E0D                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:7E0D                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:7E0D                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:7E0D                    ~ 		ENDIF
      74:  00:7E0D                      
      75:  00:7E0D                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:7E0D  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:7E11  13                  		inc	de				; de points to the increments table of the bank
      78:  00:7E12  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:7E13  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:7E15  29                  		add	hl,hl				; hl:=hl*2
      81:  00:7E16  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:7E17  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:7E18  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:7E19  56                  		ld	d,[hl]				; de:=increment
      85:  00:7E1A  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:7E1B  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:7E1E  AF                  		xor	a				; a:=0 (no errors)
      88:  00:7E1F  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:7E20  D1                  		pop	de				; Retrieve de from stack
      90:  00:7E21  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:7E22  C9                  		ret					; Return
      92:  00:7E23                      
      93:  00:7E23                      		IF ( AYFXRELATIVE == 1 )
      94:  00:7E23                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:7E23                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:7E23                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:7E23                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:7E23                    ~ 		ENDIF
      99:  00:7E23                      
     100:  00:7E23                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:7E23  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:7E26  B7                  		or	a				; If priority has bit 7 on...
     103:  00:7E27  F8                  		ret	m				; ...return
     104:  00:7E28                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:7E28  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:7E2B  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:7E2D  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:7E2F  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:7E32  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:7E33  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:7E35  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:7E37                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:7E37  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:7E3A  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:7E3B  23                  		inc	hl				; Increment pointer
     116:  00:7E3C                      		; --- Check if there's new tone on stream ---
     117:  00:7E3C  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:7E3E  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:7E40                      		; --- Extract new tone from stream ---
     120:  00:7E40  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:7E41  23                  		inc	hl				; Increment pointer
     122:  00:7E42  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:7E43  23                  		inc	hl				; Increment pointer
     124:  00:7E44  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:7E48                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:7E48  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:7E4A  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:7E4C                      		; --- Extract new noise from stream ---
     129:  00:7E4C  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:7E4D  23                  		inc	hl				; Increment pointer
     131:  00:7E4E  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:7E50  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:7E52  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:7E55                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:7E55  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:7E58                      		; --- Extract volume ---
     137:  00:7E58  79                  		ld	a,c				; a:=Control byte
     138:  00:7E59  E6 0F               		and	$0F				; lower nibble
     139:  00:7E5B                      
     140:  00:7E5B                      		IF ( AYFXRELATIVE == 1 )
     141:  00:7E5B                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:7E5B                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:7E5B                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:7E5B                    ~ 			ld	d,0				; d:=0
     145:  00:7E5B                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:7E5B                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:7E5B                    ~ 			or	a				; If relative volume is zero...
     148:  00:7E5B                    ~ 		ENDIF
     149:  00:7E5B                      
     150:  00:7E5B  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:7E5E  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:7E5F                      		; -------------------------------------
     153:  00:7E5F                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:7E5F                      		; -------------------------------------
     155:  00:7E5F                      		; --- Set noise channel ---
     156:  00:7E5F  CB 79               		bit	7,c				; If noise is off...
     157:  00:7E61  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:7E63  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:7E66  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:7E69                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:7E69  79                  		ld	a,c				; a:=Control byte
     162:  00:7E6A  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:7E6C  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:7E6E  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:7E6F                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:7E6F  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:7E70  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:7E71  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:7E73                      		; --- Dump to correct channel ---
     170:  00:7E73  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:7E76  46                  		ld	b,[hl]				; Channel counter
     172:  00:7E77                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:7E77  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:7E79                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:7E79  CD A8 7E            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:7E7C  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:7E7F  CB 51               		bit	2,c				; If tone is off...
     178:  00:7E81  C0                  		ret	nz				; ...return
     179:  00:7E82  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:7E85  C9                  		ret					; Return
     181:  00:7E86                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:7E86  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:7E88  0F                  		rrca					; Rotate right OR mask
     184:  00:7E89  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:7E8B                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:7E8B  CD A8 7E            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:7E8E  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:7E91  CB 49               		bit	1,c				; If tone is off...
     189:  00:7E93  C0                  		ret	nz				; ...return
     190:  00:7E94  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:7E97  C9                  		ret					; Return
     192:  00:7E98                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:7E98  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:7E9A  0F                  		rrca					; Rotate right OR mask
     195:  00:7E9B                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:7E9B  CD A8 7E            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:7E9E  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:7EA1  CB 41               		bit	0,c				; If tone is off...
     199:  00:7EA3  C0                  		ret	nz				; ...return
     200:  00:7EA4  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:7EA7  C9                  		ret					; Return
     202:  00:7EA8                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:7EA8  4F                  		ld	c,a				; c:=OR mask
     204:  00:7EA9  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:7EAC  A2                  		and	d				; AND mask
     206:  00:7EAD  B1                  		or	c				; OR mask
     207:  00:7EAE  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
     208:  00:7EB1  3A 49 F2            		ld	a,[ayFX_VOLUME]			; a:=ayFX volume value
     209:  00:7EB4  2A 46 F2            		ld	hl,[ayFX_TONE]			; ayFX tone value
     210:  00:7EB7  C9                  		ret					; Return
