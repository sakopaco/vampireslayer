     112:  00:7E57                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:7E57  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:7E5A  4E                  		ld	c,[hl]				; c:=Control byte
