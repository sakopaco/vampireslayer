     181.  00:50BD  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:50BE  47                       ld    b,a      	;save for later
     183.  00:50BF  F1                       pop    af      	; let's check on P
     184.  00:50C0                      
     185.  00:50C0  E6 20                    and    00100000b
     186.  00:50C2  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
