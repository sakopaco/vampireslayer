     151.  00:55C2  E6 01                    and    #01    		;save L
     152.  00:55C4  CB 27                    sla    a
     153.  00:55C6  CB 27                    sla    a    		;now 00000L00
     154.  00:55C8  4F                       ld    c,a    		;save for later
     155.  00:55C9                      
     156.  00:55C9  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:55CA  CB 3F                    srl    a
     158.  00:55CC  E6 03                    and    00000011b
