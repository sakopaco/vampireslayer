      29:  00:90C9                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:90C9                      		; --- INPUT: A -> sound to be played ---
      31:  00:90C9                      		; ---        C -> sound priority     ---
      32:  00:90C9  C5                  		push	bc				; Store bc in stack
      33:  00:90CA  D5                  		push	de				; Store de in stack
