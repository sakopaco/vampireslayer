     140.  00:53BA                      ;;update_controllers_status
     141.  00:53BA                      ;;=====================================================	
     142.  00:53BA                      ;depends on the next rountine
     143.  00:53BA                      ;  returns
     144.  00:53BA                      ;  a - 0P2TRLDU
     145.  00:53BA                      
     146.  00:53BA                      update_controllers_status:
     147.  00:53BA  CD 05 54                 call  check_8th_keyboard_row
     148.  00:53BD                      [4]  rrc    a
     148.  00:53BD  CB 0F CB 0F CB 0F CB 0F 
     149.  00:53C5  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:53C6                      
     151.  00:53C6  E6 01                    and    #01    		;save L
     152.  00:53C8  CB 27                    sla    a
     153.  00:53CA  CB 27                    sla    a    		;now 00000L00
     154.  00:53CC  4F                       ld    c,a    		;save for later
     155.  00:53CD                      
     156.  00:53CD  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:53CE  CB 3F                    srl    a
     158.  00:53D0  E6 03                    and    00000011b
     159.  00:53D2                      						;we've got 000000DU on a
     160.  00:53D2  B1                       or    c    		;now we've got 00000LDU
     161.  00:53D3  4F                       ld    c,a    		;save for later
     162.  00:53D4                      
     163.  00:53D4  78                       ld    a,b
     164.  00:53D5  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:53D7  B1                       or    c
     166.  00:53D8                           ;finally    000TRLDU
     167.  00:53D8                      
     168.  00:53D8                           ;and    00000100   ;preserve just
     169.  00:53D8  47                       ld    b,a    		;save for later
     170.  00:53D9  CD 13 54                 call  check_4th_keyboard_row      ;check M & P
     171.  00:53DC  F5                       push  af
     172.  00:53DD  E6 04                    and    00000100b	;filter just M
     173.  00:53DF  87                       add    a
     174.  00:53E0  87                       add    a
     175.  00:53E1  87                       add    a           ;shift 3th to 6th bit
     176.  00:53E2  B0                       or    b           	;merge with previous
     177.  00:53E3  47                       ld    b,a
     178.  00:53E4                      
     179.  00:53E4  CD F1 53                 call  read_joy_0
     180.  00:53E7  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:53E9  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:53EA  47                       ld    b,a      	;save for later
     183.  00:53EB  F1                       pop    af      	; let's check on P
     184.  00:53EC                      
     185.  00:53EC  E6 20                    and    00100000b
     186.  00:53EE  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:53EF  B0                       or    b
     188.  00:53F0                      
     189.  00:53F0  C9                       ret
     190.  00:53F1                      
     191.  00:53F1                      
     192.  00:53F1                      ;ret
     193.  00:53F1                      ;  a -            xx2TRLDU
     194.  00:53F1                      read_joy_0:
     195.  00:53F1  F3                         di
     196.  00:53F2  3E 0F                      ld    a,15
     197.  00:53F4  D3 A0                      out    [PSG_A],a
     198.  00:53F6  DB A2                      in    a,[PSG_R]
     199.  00:53F8  E6 BF                      and    10111111b        ;joy0
     200.  00:53FA                      
     201.  00:53FA  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:53FC                      
     203.  00:53FC  3E 0E                      ld    a,14
     204.  00:53FE  D3 A0                      out    [PSG_A],a
     205.  00:5400  DB A2                      in    a,[PSG_R]
