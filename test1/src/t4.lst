     118:  00:973C  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:973E                      		; --- Extract new tone from stream ---
     120:  00:973E  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:973F  23                  		inc	hl				; Increment pointer
     122:  00:9740  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:9741  23                  		inc	hl				; Increment pointer
