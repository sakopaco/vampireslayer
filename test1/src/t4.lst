     159:  00:9756  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:9759                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:9759  79                  		ld	a,c				; a:=Control byte
     162:  00:975A  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:975C  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:975E  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:975F                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:975F  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:9760  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:9761  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:9763                      		; --- Dump to correct channel ---
