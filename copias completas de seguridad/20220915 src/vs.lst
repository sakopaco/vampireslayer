      45:  00:76D9  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:76DB                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:76DB                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:76DB  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:76DC  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
