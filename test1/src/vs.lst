      32:  00:7C23  C5                  		push	bc				; Store bc in stack
      33:  00:7C24  D5                  		push	de				; Store de in stack
      34:  00:7C25  E5                  		push	hl				; Store hl in stack
      35:  00:7C26                      		; --- Check if the index is in the bank ---
      36:  00:7C26  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7C27  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7C2A  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7C2B  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7C2C  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7C2E                      		; The bank has less than 256 samples
