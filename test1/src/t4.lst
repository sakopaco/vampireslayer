     166.  00:8121                      ;colocamos resultado en habitación actual
     167.  00:8121  DD 21 CB 88         	LD		IX, habitacion_actual
     168.  00:8125  7E                  	LD		 A, (HL)
     169.  00:8126  DD 77 00            	LD		(IX), A
     170.  00:8129  23                  	INC		HL
     171.  00:812A  7E                  	LD		 A, (HL)
     172.  00:812B  DD 77 01            	LD		(IX+1), A
     173.  00:812E                      	
     174.  00:812E  D9                  	EXX
