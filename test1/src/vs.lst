     118:  00:76DB  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:76DD                      		; --- Extract new tone from stream ---
     120:  00:76DD  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:76DE  23                  		inc	hl				; Increment pointer
     122:  00:76DF  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:76E0  23                  		inc	hl				; Increment pointer
     124:  00:76E1  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:76E5                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:76E5  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:76E7  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:76E9                      		; --- Extract new noise from stream ---
