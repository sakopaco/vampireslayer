      36:  00:7C86  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7C87  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7C8A  7E                  		ld	a,[hl]				; Number of samples in the bank
