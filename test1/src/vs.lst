      23.  00:61C7                      	
      24.  00:61C7                      	;Engancha nuestra rutina de servicio al gancha que deja preparado la BIOS cuando se termina de pintar la pantalla (50 o 60 veces por segundo)
      25.  00:61C7  3E C3               	LD		A, #C3
      26.  00:61C9  32 9F FD            	LD		[H.TIMI], A
      27.  00:61CC  21 D4 61            	LD		HL, subrutina_isr
      28.  00:61CF  22 A0 FD            	LD		[H.TIMI+1], HL
      29.  00:61D2                      	
      30.  00:61D2  FB                  	EI		;optimizacion:
      31.  00:61D3                      			;piensa que las dos ultimas lineas podrian haber sido: 
      32.  00:61D3                      			;ei						;primero ei
      33.  00:61D3                      			;ld		[H.TIMI+1],hl	;luego este ld
      34.  00:61D3                      									;PENSAR (y consultar ) PORQUE!!!!
