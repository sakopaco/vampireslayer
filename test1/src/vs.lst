      45:  00:75D6  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:75D8                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:75D8                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:75D8  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:75D9  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:75DC  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:75DD  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:75DF  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:75E1                      		; --- Set new priority ---
      54:  00:75E1  79                  		ld	a,c				; a:=New priority
