     188:  00:82E0  CB 49               		bit	1,c				; If tone is off...
     189:  00:82E2  C0                  		ret	nz				; ...return
     190:  00:82E3  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:82E6  C9                  		ret					; Return
     192:  00:82E7                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:82E7  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:82E9  0F                  		rrca					; Rotate right OR mask
     195:  00:82EA                      .PLAY_A:	; --- Play ayFX stream on channel A ---
