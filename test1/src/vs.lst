     195.  00:5506  F3                         di
     196.  00:5507  3E 0F                      ld    a,15
     197.  00:5509  D3 A0                      out    [PSG_A],a
     198.  00:550B  DB A2                      in    a,[PSG_R]
     199.  00:550D  E6 BF                      and    10111111b        ;joy0
     200.  00:550F                      
     201.  00:550F  D3 A1                      out    [PSG_W],a        ;set joy0
     202.  00:5511                      
     203.  00:5511  3E 0E                      ld    a,14
     204.  00:5513  D3 A0                      out    [PSG_A],a
     205.  00:5515  DB A2                      in    a,[PSG_R]
