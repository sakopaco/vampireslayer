     140.  00:53CA                      ;;update_controllers_status
     141.  00:53CA                      ;;=====================================================	
     142.  00:53CA                      ;depends on the next rountine
     143.  00:53CA                      ;  returns
     144.  00:53CA                      ;  a - 0P2TRLDU
     145.  00:53CA                      
     146.  00:53CA                      update_controllers_status:
     147.  00:53CA  CD 15 54                 call  check_8th_keyboard_row
     148.  00:53CD                      [4]  rrc    a
     148.  00:53CD  CB 0F CB 0F CB 0F CB 0F 
     149.  00:53D5  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:53D6                      
     151.  00:53D6  E6 01                    and    #01    		;save L
     152.  00:53D8  CB 27                    sla    a
     153.  00:53DA  CB 27                    sla    a    		;now 00000L00
     154.  00:53DC  4F                       ld    c,a    		;save for later
     155.  00:53DD                      
     156.  00:53DD  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:53DE  CB 3F                    srl    a
     158.  00:53E0  E6 03                    and    00000011b
     159.  00:53E2                      						;we've got 000000DU on a
     160.  00:53E2  B1                       or    c    		;now we've got 00000LDU
     161.  00:53E3  4F                       ld    c,a    		;save for later
     162.  00:53E4                      
     163.  00:53E4  78                       ld    a,b
     164.  00:53E5  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:53E7  B1                       or    c
     166.  00:53E8                           ;finally    000TRLDU
     167.  00:53E8                      
     168.  00:53E8                           ;and    00000100   ;preserve just
     169.  00:53E8  47                       ld    b,a    		;save for later
     170.  00:53E9  CD 23 54                 call  check_4th_keyboard_row      ;check M & P
     171.  00:53EC  F5                       push  af
     172.  00:53ED  E6 04                    and    00000100b	;filter just M
     173.  00:53EF  87                       add    a
     174.  00:53F0  87                       add    a
     175.  00:53F1  87                       add    a           ;shift 3th to 6th bit
     176.  00:53F2  B0                       or    b           	;merge with previous
     177.  00:53F3  47                       ld    b,a
     178.  00:53F4                      
     179.  00:53F4  CD 01 54                 call  read_joy_0
     180.  00:53F7  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:53F9  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:53FA  47                       ld    b,a      	;save for later
     183.  00:53FB  F1                       pop    af      	; let's check on P
     184.  00:53FC                      
     185.  00:53FC  E6 20                    and    00100000b
     186.  00:53FE  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:53FF  B0                       or    b
     188.  00:5400                      
     189.  00:5400  C9                       ret
     190.  00:5401                      
     191.  00:5401                      
     192.  00:5401                      ;ret
     193.  00:5401                      ;  a -            xx2TRLDU
