     178.  00:542E                      
     179.  00:542E  CD 3B 54                 call  read_joy_0
     180.  00:5431  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:5433  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:5434  47                       ld    b,a      	;save for later
     183.  00:5435  F1                       pop    af      	; let's check on P
     184.  00:5436                      
     185.  00:5436  E6 20                    and    00100000b
     186.  00:5438  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:5439  B0                       or    b
