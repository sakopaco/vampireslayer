      56:  00:7BD5  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7BD8                      
      58:  00:7BD8                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7BD8                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7BD8                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7BD8                    ~ 			ld	a,15				; a:=15
