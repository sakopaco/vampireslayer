      17:  00:7DD5                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7DD5                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7DD5  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7DD8  3E 01               		ld	a,1				; a:=0
      21:  00:7DDA  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7DDD  3C                  		inc	a				; Starting channel (=1)
      23:  00:7DDE  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7DE1                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7DE1  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7DE3  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7DE6  C9                  		ret					; Return
      28:  00:7DE7                      
      29:  00:7DE7                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7DE7                      		; --- INPUT: A -> sound to be played ---
      31:  00:7DE7                      		; ---        C -> sound priority     ---
      32:  00:7DE7  C5                  		push	bc				; Store bc in stack
      33:  00:7DE8  D5                  		push	de				; Store de in stack
      34:  00:7DE9  E5                  		push	hl				; Store hl in stack
      35:  00:7DEA                      		; --- Check if the index is in the bank ---
      36:  00:7DEA  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7DEB  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7DEE  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7DEF  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7DF0  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7DF2                      		; The bank has less than 256 samples
      42:  00:7DF2  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7DF3  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7DF4  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7DF6  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7DF8                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7DF8                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7DF8  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7DF9  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7DFC  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7DFD  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7DFF  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7E01                      		; --- Set new priority ---
      54:  00:7E01  79                  		ld	a,c				; a:=New priority
      55:  00:7E02  E6 0F               		and	$0F				; We mask the priority
      56:  00:7E04  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7E07                      
      58:  00:7E07                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7E07                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7E07                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7E07                    ~ 			ld	a,15				; a:=15
      62:  00:7E07                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7E07                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7E07                    ~ 			add	a,a				; a:=a*2
      65:  00:7E07                    ~ 			add	a,a				; a:=a*4
      66:  00:7E07                    ~ 			add	a,a				; a:=a*8
      67:  00:7E07                    ~ 			add	a,a				; a:=a*16
      68:  00:7E07                    ~ 			ld	e,a				; e:=a
      69:  00:7E07                    ~ 			ld	d,0				; de:=a
      70:  00:7E07                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:7E07                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:7E07                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:7E07                    ~ 		ENDIF
      74:  00:7E07                      
      75:  00:7E07                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:7E07  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:7E0B  13                  		inc	de				; de points to the increments table of the bank
      78:  00:7E0C  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:7E0D  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:7E0F  29                  		add	hl,hl				; hl:=hl*2
      81:  00:7E10  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:7E11  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:7E12  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:7E13  56                  		ld	d,[hl]				; de:=increment
      85:  00:7E14  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:7E15  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:7E18  AF                  		xor	a				; a:=0 (no errors)
      88:  00:7E19  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:7E1A  D1                  		pop	de				; Retrieve de from stack
      90:  00:7E1B  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:7E1C  C9                  		ret					; Return
      92:  00:7E1D                      
      93:  00:7E1D                      		IF ( AYFXRELATIVE == 1 )
      94:  00:7E1D                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:7E1D                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:7E1D                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:7E1D                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:7E1D                    ~ 		ENDIF
      99:  00:7E1D                      
     100:  00:7E1D                      ayFX_PLAY:	; --- PLAY A FRAME OF AN ayFX STREAM ---
     101:  00:7E1D  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
     102:  00:7E20  B7                  		or	a				; If priority has bit 7 on...
     103:  00:7E21  F8                  		ret	m				; ...return
     104:  00:7E22                      		; --- Calculate next ayFX channel (if needed) ---
     105:  00:7E22  3A 40 F2            		ld	a,[ayFX_MODE]			; ayFX mode
     106:  00:7E25  E6 01               		and	1				; If bit0=0 (fixed channel)...
     107:  00:7E27  28 08               		jr	z,.TAKECB			; ...skip channel changing
     108:  00:7E29  21 4A F2            		ld	hl,ayFX_CHANNEL			; Old ayFX playing channel
     109:  00:7E2C  35                  		dec	[hl]				; New ayFX playing channel
     110:  00:7E2D  20 02               		jr	nz,.TAKECB			; If not zero jump to .TAKECB
     111:  00:7E2F  36 03               		ld	[hl],3				; If zero -> set channel 3
     112:  00:7E31                      .TAKECB:	; --- Extract control byte from stream ---
     113:  00:7E31  2A 44 F2            		ld	hl,[ayFX_POINTER]		; Pointer to the current ayFX stream
     114:  00:7E34  4E                  		ld	c,[hl]				; c:=Control byte
     115:  00:7E35  23                  		inc	hl				; Increment pointer
     116:  00:7E36                      		; --- Check if there's new tone on stream ---
     117:  00:7E36  CB 69               		bit	5,c				; If bit 5 c is off...
     118:  00:7E38  28 08               		jr	z,.CHECK_NN			; ...jump to .CHECK_NN (no new tone)
     119:  00:7E3A                      		; --- Extract new tone from stream ---
     120:  00:7E3A  5E                  		ld	e,[hl]				; e:=lower byte of new tone
     121:  00:7E3B  23                  		inc	hl				; Increment pointer
     122:  00:7E3C  56                  		ld	d,[hl]				; d:=higher byte of new tone
     123:  00:7E3D  23                  		inc	hl				; Increment pointer
     124:  00:7E3E  ED 53 46 F2         		ld	[ayFX_TONE],de			; ayFX tone updated
     125:  00:7E42                      .CHECK_NN:	; --- Check if there's new noise on stream ---
     126:  00:7E42  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:7E44  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:7E46                      		; --- Extract new noise from stream ---
     129:  00:7E46  7E                  		ld	a,[hl]				; a:=New noise
     130:  00:7E47  23                  		inc	hl				; Increment pointer
     131:  00:7E48  FE 20               		cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     132:  00:7E4A  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:7E4C  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:7E4F                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:7E4F  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:7E52                      		; --- Extract volume ---
     137:  00:7E52  79                  		ld	a,c				; a:=Control byte
     138:  00:7E53  E6 0F               		and	$0F				; lower nibble
     139:  00:7E55                      
     140:  00:7E55                      		IF ( AYFXRELATIVE == 1 )
     141:  00:7E55                    ~ 			; --- Fix the volume using PT3 Volume Table ---
     142:  00:7E55                    ~ 			ld	hl,[ayFX_VT]			; hl:=Pointer to relative volume table
     143:  00:7E55                    ~ 			ld	e,a				; e:=a (ayFX volume)
     144:  00:7E55                    ~ 			ld	d,0				; d:=0
     145:  00:7E55                    ~ 			add	hl,de				; hl:=hl+de (hl points to the relative volume of this frame
     146:  00:7E55                    ~ 			ld	a,[hl]				; a:=ayFX relative volume
     147:  00:7E55                    ~ 			or	a				; If relative volume is zero...
     148:  00:7E55                    ~ 		ENDIF
     149:  00:7E55                      
     150:  00:7E55  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:7E58  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:7E59                      		; -------------------------------------
     153:  00:7E59                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:7E59                      		; -------------------------------------
     155:  00:7E59                      		; --- Set noise channel ---
     156:  00:7E59  CB 79               		bit	7,c				; If noise is off...
     157:  00:7E5B  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:7E5D  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:7E60  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:7E63                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:7E63  79                  		ld	a,c				; a:=Control byte
     162:  00:7E64  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:7E66  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:7E68  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:7E69                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:7E69  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:7E6A  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:7E6B  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:7E6D                      		; --- Dump to correct channel ---
     170:  00:7E6D  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:7E70  46                  		ld	b,[hl]				; Channel counter
     172:  00:7E71                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:7E71  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:7E73                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:7E73  CD A2 7E            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:7E76  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:7E79  CB 51               		bit	2,c				; If tone is off...
     178:  00:7E7B  C0                  		ret	nz				; ...return
     179:  00:7E7C  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:7E7F  C9                  		ret					; Return
     181:  00:7E80                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:7E80  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:7E82  0F                  		rrca					; Rotate right OR mask
     184:  00:7E83  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:7E85                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:7E85  CD A2 7E            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:7E88  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:7E8B  CB 49               		bit	1,c				; If tone is off...
     189:  00:7E8D  C0                  		ret	nz				; ...return
     190:  00:7E8E  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:7E91  C9                  		ret					; Return
     192:  00:7E92                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:7E92  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:7E94  0F                  		rrca					; Rotate right OR mask
     195:  00:7E95                      .PLAY_A:	; --- Play ayFX stream on channel A ---
