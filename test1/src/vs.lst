     160:  00:82B8                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:82B8  79                  		ld	a,c				; a:=Control byte
     162:  00:82B9  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:82BB  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:82BD  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:82BE                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:82BE  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:82BF  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:82C0  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:82C2                      		; --- Dump to correct channel ---
