     594.  00:5082                      .deteccioncolision_paso1:
     595.  00:5082  FD 21 00 C2         		LD		IY, puntomira	;IY punto de mira / IX puerta
     596.  00:5086  FD 7E 00            		LD		 A, (IY + ESTRUCTURA_PUNTOMIRA.posx)
     597.  00:5089  C6 08               		ADD		 8				;8-es fijo, offset del punto de mira ya que se mueve según la esquina superior izquierda y el centro del punto de mira está en el centro del sprite
     598.  00:508B                      	
     599.  00:508B                      		;ya tengo en A la coordenada X del centro del punto de mira					
     600.  00:508B  DD 96 01            		SUB		(IX + ESTRUCTURA_PUERTA.posx)	;le resto el punto x en la puerta
     601.  00:508E                      	
     602.  00:508E  30 02               		JR		NC, .deteccioncolision_paso2	;si no es negativo comparo con el radio
     603.  00:5090                      
     604.  00:5090  ED 44               		NEG										;si es negativo lo niego (valor absoluto)
     605.  00:5092                      	
     606.  00:5092                      .deteccioncolision_paso2:
     607.  00:5092  DD BE 03            		CP		(IX + ESTRUCTURA_PUERTA.radiox)	;comparo con el radio X de la puerta
     608.  00:5095                      	
     609.  00:5095  38 02               		JR		 C, .deteccioncolision_paso3	;SI NC la distancia es >= por lo que sale y no es necesario verificar nada más
     610.  00:5097                      	
     611.  00:5097  AF                  		XOR		 A								;el resultado es falso y se guarda en A y ya no hay que seguir coprobando
     612.  00:5098  C9                  		RET
     613.  00:5099                      	
     614.  00:5099                      .deteccioncolision_paso3:					;la distancia X es válida, comprobamos la distancia Y
     615.  00:5099  FD 7E 01            		LD		 A, (IY + ESTRUCTURA_PUNTOMIRA.posy)
     616.  00:509C  C6 08               		ADD		 8								;le sumo el offset del punto de mira (8 es fijo)
     617.  00:509E                      
     618.  00:509E                      		;ya tengo en A la coordenada Y del centro del punto de mira					
     619.  00:509E  DD 96 02            		SUB		(IX + ESTRUCTURA_PUERTA.posy)	;le resto el punto y en la puerta
     620.  00:50A1                      	
     621.  00:50A1  30 02               		JR		NC, .deteccioncolision_paso4	;si no es negativo comparo con el radio
     622.  00:50A3                      
     623.  00:50A3  ED 44               		NEG										;si es negativo lo niego (valor absoluto)
     624.  00:50A5                      
     625.  00:50A5                      .deteccioncolision_paso4:
     626.  00:50A5  DD BE 04            		CP		(IX + ESTRUCTURA_PUERTA.radioy)	;comparo con el radio Y de la puerta
     627.  00:50A8                      
     628.  00:50A8  38 02               		JR		 C, .deteccioncolision_paso5	;SI NC la distancia es >= por lo que sale y no es necesario verificar nada más
     629.  00:50AA                      	
     630.  00:50AA  AF                  		XOR		 A								;el resultado es falso y se guarda en A un 0 y al ser la 2º comprobación salimos
     631.  00:50AB  C9                  		RET
     632.  00:50AC                      
     633.  00:50AC                      .deteccioncolision_paso5:
     634.  00:50AC  3E 01               		LD		 A, SI
