      45:  00:7E68  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7E6A                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7E6A                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7E6A  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7E6B  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7E6E  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
