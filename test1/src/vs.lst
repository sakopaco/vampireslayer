     161.  00:541F  4F                       ld    c,a    		;save for later
     162.  00:5420                      
     163.  00:5420  78                       ld    a,b
     164.  00:5421  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:5423  B1                       or    c
