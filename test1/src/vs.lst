      23:  00:75CC  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:75CF                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:75CF  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:75D1  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:75D4  C9                  		ret					; Return
