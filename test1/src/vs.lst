     183:  00:774E  0F                  		rrca					; Rotate right OR mask
     184:  00:774F  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:7751                      .PLAY_B:	; --- Play ayFX stream on channel B ---
