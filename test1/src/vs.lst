     174.  00:5419  87                       add    a
     175.  00:541A  87                       add    a           ;shift 3th to 6th bit
     176.  00:541B  B0                       or    b           	;merge with previous
     177.  00:541C  47                       ld    b,a
     178.  00:541D                      
     179.  00:541D  CD 2A 54                 call  read_joy_0
     180.  00:5420  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:5422  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:5423  47                       ld    b,a      	;save for later
     183.  00:5424  F1                       pop    af      	; let's check on P
     184.  00:5425                      
     185.  00:5425  E6 20                    and    00100000b
     186.  00:5427  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:5428  B0                       or    b
