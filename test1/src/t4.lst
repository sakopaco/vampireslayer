     201.  00:88C9  4F                       ld    c,a    		;save for later
     202.  00:88CA                      
     203.  00:88CA  78                       ld    a,b
     204.  00:88CB  E6 18                    and    00011000b 	;we've got 000TR000
     205.  00:88CD  B1                       or    c
