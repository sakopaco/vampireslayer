      46:  00:7A2E                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7A2E                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7A2E  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7A2F  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7A32  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
