      82:  00:9715  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:9716  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:9717  56                  		ld	d,[hl]				; de:=increment
      85:  00:9718  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:9719  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:971C  AF                  		xor	a				; a:=0 (no errors)
      88:  00:971D  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:971E  D1                  		pop	de				; Retrieve de from stack
      90:  00:971F  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:9720  C9                  		ret					; Return
