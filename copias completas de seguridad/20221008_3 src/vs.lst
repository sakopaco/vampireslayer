      29:  00:7A17                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7A17                      		; --- INPUT: A -> sound to be played ---
      31:  00:7A17                      		; ---        C -> sound priority     ---
      32:  00:7A17  C5                  		push	bc				; Store bc in stack
      33:  00:7A18  D5                  		push	de				; Store de in stack
      34:  00:7A19  E5                  		push	hl				; Store hl in stack
      35:  00:7A1A                      		; --- Check if the index is in the bank ---
      36:  00:7A1A  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7A1B  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7A1E  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7A1F  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7A20  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7A22                      		; The bank has less than 256 samples
      42:  00:7A22  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7A23  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7A24  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7A26  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7A28                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7A28                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7A28  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7A29  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7A2C  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7A2D  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7A2F  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7A31                      		; --- Set new priority ---
      54:  00:7A31  79                  		ld	a,c				; a:=New priority
      55:  00:7A32  E6 0F               		and	$0F				; We mask the priority
      56:  00:7A34  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
