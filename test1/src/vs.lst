     140.  00:53BC                      ;;update_controllers_status
     141.  00:53BC                      ;;=====================================================	
     142.  00:53BC                      ;depends on the next rountine
     143.  00:53BC                      ;  returns
     144.  00:53BC                      ;  a - 0P2TRLDU
     145.  00:53BC                      
     146.  00:53BC                      update_controllers_status:
     147.  00:53BC  CD 07 54                 call  check_8th_keyboard_row
     148.  00:53BF                      [4]  rrc    a
     148.  00:53BF  CB 0F CB 0F CB 0F CB 0F 
     149.  00:53C7  47                       ld    b,a    		;save for later  xxxTRDUL
     150.  00:53C8                      
     151.  00:53C8  E6 01                    and    #01    		;save L
     152.  00:53CA  CB 27                    sla    a
     153.  00:53CC  CB 27                    sla    a    		;now 00000L00
     154.  00:53CE  4F                       ld    c,a    		;save for later
     155.  00:53CF                      
     156.  00:53CF  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     157.  00:53D0  CB 3F                    srl    a
     158.  00:53D2  E6 03                    and    00000011b
     159.  00:53D4                      						;we've got 000000DU on a
     160.  00:53D4  B1                       or    c    		;now we've got 00000LDU
     161.  00:53D5  4F                       ld    c,a    		;save for later
     162.  00:53D6                      
     163.  00:53D6  78                       ld    a,b
     164.  00:53D7  E6 18                    and    00011000b 	;we've got 000TR000
     165.  00:53D9  B1                       or    c
     166.  00:53DA                           ;finally    000TRLDU
     167.  00:53DA                      
     168.  00:53DA                           ;and    00000100   ;preserve just
     169.  00:53DA  47                       ld    b,a    		;save for later
     170.  00:53DB  CD 15 54                 call  check_4th_keyboard_row      ;check M & P
     171.  00:53DE  F5                       push  af
     172.  00:53DF  E6 04                    and    00000100b	;filter just M
     173.  00:53E1  87                       add    a
     174.  00:53E2  87                       add    a
     175.  00:53E3  87                       add    a           ;shift 3th to 6th bit
     176.  00:53E4  B0                       or    b           	;merge with previous
     177.  00:53E5  47                       ld    b,a
     178.  00:53E6                      
     179.  00:53E6  CD F3 53                 call  read_joy_0
     180.  00:53E9  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:53EB  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:53EC  47                       ld    b,a      	;save for later
     183.  00:53ED  F1                       pop    af      	; let's check on P
     184.  00:53EE                      
     185.  00:53EE  E6 20                    and    00100000b
     186.  00:53F0  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     187.  00:53F1  B0                       or    b
     188.  00:53F2                      
     189.  00:53F2  C9                       ret
     190.  00:53F3                      
     191.  00:53F3                      
