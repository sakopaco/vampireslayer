     182:  00:92AF  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:92B1  0F                  		rrca					; Rotate right OR mask
     184:  00:92B2  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:92B4                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:92B4  CD D1 92            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:92B7  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:92BA  CB 49               		bit	1,c				; If tone is off...
     189:  00:92BC  C0                  		ret	nz				; ...return
     190:  00:92BD  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:92C0  C9                  		ret					; Return
     192:  00:92C1                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:92C1  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:92C3  0F                  		rrca					; Rotate right OR mask
     195:  00:92C4                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:92C4  CD D1 92            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:92C7  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:92CA  CB 41               		bit	0,c				; If tone is off...
     199:  00:92CC  C0                  		ret	nz				; ...return
     200:  00:92CD  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:92D0  C9                  		ret					; Return
     202:  00:92D1                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:92D1  4F                  		ld	c,a				; c:=OR mask
     204:  00:92D2  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:92D5  A2                  		and	d				; AND mask
     206:  00:92D6  B1                  		or	c				; OR mask
     207:  00:92D7  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
     208:  00:92DA  3A 49 F2            		ld	a,[ayFX_VOLUME]			; a:=ayFX volume value
     209:  00:92DD  2A 46 F2            		ld	hl,[ayFX_TONE]			; ayFX tone value
     210:  00:92E0  C9                  		ret					; Return
