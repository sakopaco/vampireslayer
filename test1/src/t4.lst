      29:  00:8F1B                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:8F1B                      		; --- INPUT: A -> sound to be played ---
      31:  00:8F1B                      		; ---        C -> sound priority     ---
      32:  00:8F1B  C5                  		push	bc				; Store bc in stack
      33:  00:8F1C  D5                  		push	de				; Store de in stack
      34:  00:8F1D  E5                  		push	hl				; Store hl in stack
      35:  00:8F1E                      		; --- Check if the index is in the bank ---
      36:  00:8F1E  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:8F1F  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:8F22  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:8F23  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:8F24  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:8F26                      		; The bank has less than 256 samples
      42:  00:8F26  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:8F27  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:8F28  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:8F2A  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:8F2C                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:8F2C                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:8F2C  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:8F2D  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:8F30  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:8F31  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:8F33  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:8F35                      		; --- Set new priority ---
      54:  00:8F35  79                  		ld	a,c				; a:=New priority
      55:  00:8F36  E6 0F               		and	$0F				; We mask the priority
      56:  00:8F38  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:8F3B                      
      58:  00:8F3B                      		IF ( AYFXRELATIVE == 1 )
      59:  00:8F3B                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:8F3B                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:8F3B                    ~ 			ld	a,15				; a:=15
      62:  00:8F3B                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:8F3B                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:8F3B                    ~ 			add	a,a				; a:=a*2
      65:  00:8F3B                    ~ 			add	a,a				; a:=a*4
      66:  00:8F3B                    ~ 			add	a,a				; a:=a*8
      67:  00:8F3B                    ~ 			add	a,a				; a:=a*16
