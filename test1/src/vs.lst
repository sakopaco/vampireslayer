     126:  00:8292  CB 71               		bit	6,c				; if bit 6 c is off...
     127:  00:8294  28 09               		jr	z,.SETPOINTER			; ...jump to .SETPOINTER (no new noise)
     128:  00:8296                      		; --- Extract new noise from stream ---
