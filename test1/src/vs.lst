     150:  00:7CB1  32 49 F2            		ld	[ayFX_VOLUME],a			; ayFX volume updated
     151:  00:7CB4  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     152:  00:7CB5                      		; -------------------------------------
     153:  00:7CB5                      		; --- COPY ayFX VALUES IN TO AYREGS ---
     154:  00:7CB5                      		; -------------------------------------
     155:  00:7CB5                      		; --- Set noise channel ---
