     171:  00:7B52  46                  		ld	b,[hl]				; Channel counter
     172:  00:7B53                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:7B53  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:7B55                      .PLAY_C:	; --- Play ayFX stream on channel C ---
