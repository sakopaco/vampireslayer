      17:  00:6F18                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:6F18                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:6F18  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
