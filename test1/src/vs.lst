      37:  00:7A52  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7A55  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7A56  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7A57  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7A59                      		; The bank has less than 256 samples
      42:  00:7A59  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7A5A  BE                  		cp	[hl]				; If new index is not in the bank...
