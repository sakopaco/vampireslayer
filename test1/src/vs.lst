     174.  00:55DF  87                       add    a
     175.  00:55E0  87                       add    a           ;shift 3th to 6th bit
     176.  00:55E1  B0                       or    b           	;merge with previous
     177.  00:55E2  47                       ld    b,a
     178.  00:55E3                      
     179.  00:55E3  CD F0 55                 call  read_joy_0
     180.  00:55E6  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     181.  00:55E8  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:55E9  47                       ld    b,a      	;save for later
     183.  00:55EA  F1                       pop    af      	; let's check on P
