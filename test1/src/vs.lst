     197:  00:7E98  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:7E9B  CB 41               		bit	0,c				; If tone is off...
     199:  00:7E9D  C0                  		ret	nz				; ...return
