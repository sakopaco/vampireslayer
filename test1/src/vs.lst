     132:  00:8299  28 95               		jr.	z,ayFX_END			; ...jump to ayFX_END
     133:  00:829B  32 48 F2            		ld	[ayFX_NOISE],a			; ayFX noise updated
     134:  00:829E                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:829E  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:82A1                      		; --- Extract volume ---
     137:  00:82A1  79                  		ld	a,c				; a:=Control byte
     138:  00:82A2  E6 0F               		and	$0F				; lower nibble
