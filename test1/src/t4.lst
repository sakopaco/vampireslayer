     157:  00:9D0D  20 06               		jr	nz,.SETMASKS			; ...jump to .SETMASKS
     158:  00:9D0F  3A 48 F2            		ld	a,[ayFX_NOISE]			; ayFX noise value
     159:  00:9D12  32 46 F1            		ld	[AYREGS+6],a			; copied in to AYREGS (noise channel)
     160:  00:9D15                      .SETMASKS:	; --- Set mixer masks ---
     161:  00:9D15  79                  		ld	a,c				; a:=Control byte
     162:  00:9D16  E6 90               		and	$90				; Only bits 7 and 4 (noise and tone mask for psg reg 7)
     163:  00:9D18  FE 90               		cp	$90				; If no noise and no tone...
     164:  00:9D1A  C8                  		ret	z				; ...return (don't copy ayFX values in to AYREGS)
     165:  00:9D1B                      		; --- Copy ayFX values in to ARYREGS ---
     166:  00:9D1B  0F                  		rrca					; Rotate a to the right (1 TIME)
     167:  00:9D1C  0F                  		rrca					; Rotate a to the right (2 TIMES) (OR mask)
     168:  00:9D1D  16 DB               		ld	d,$DB				; d:=Mask for psg mixer (AND mask)
     169:  00:9D1F                      		; --- Dump to correct channel ---
     170:  00:9D1F  21 4A F2            		ld	hl,ayFX_CHANNEL			; Next ayFX playing channel
     171:  00:9D22  46                  		ld	b,[hl]				; Channel counter
     172:  00:9D23                      .CHK1:		; --- Check if playing channel was 1 ---
     173:  00:9D23  10 0D               		djnz	.CHK2				; Decrement and jump if channel was not 1
     174:  00:9D25                      .PLAY_C:	; --- Play ayFX stream on channel C ---
     175:  00:9D25  CD 54 9D            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     176:  00:9D28  32 4A F1            		ld	[AYREGS+10],a			; Volume copied in to AYREGS (channel C volume)
     177:  00:9D2B  CB 51               		bit	2,c				; If tone is off...
     178:  00:9D2D  C0                  		ret	nz				; ...return
     179:  00:9D2E  22 44 F1            		ld	[AYREGS+4],hl			; copied in to AYREGS (channel C tone)
     180:  00:9D31  C9                  		ret					; Return
     181:  00:9D32                      .CHK2:		; --- Check if playing channel was 2 ---
     182:  00:9D32  CB 0A               		rrc	d				; Rotate right AND mask
     183:  00:9D34  0F                  		rrca					; Rotate right OR mask
     184:  00:9D35  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:9D37                      .PLAY_B:	; --- Play ayFX stream on channel B ---
     186:  00:9D37  CD 54 9D            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     187:  00:9D3A  32 49 F1            		ld	[AYREGS+9],a			; Volume copied in to AYREGS (channel B volume)
     188:  00:9D3D  CB 49               		bit	1,c				; If tone is off...
     189:  00:9D3F  C0                  		ret	nz				; ...return
     190:  00:9D40  22 42 F1            		ld	[AYREGS+2],hl			; copied in to AYREGS (channel B tone)
     191:  00:9D43  C9                  		ret					; Return
     192:  00:9D44                      .CHK3:		; --- Check if playing channel was 3 ---
     193:  00:9D44  CB 0A               		rrc	d				; Rotate right AND mask
     194:  00:9D46  0F                  		rrca					; Rotate right OR mask
     195:  00:9D47                      .PLAY_A:	; --- Play ayFX stream on channel A ---
     196:  00:9D47  CD 54 9D            		call	.SETMIXER			; Set PSG mixer value (returning a=ayFX volume and hl=ayFX tone)
     197:  00:9D4A  32 48 F1            		ld	[AYREGS+8],a			; Volume copied in to AYREGS (channel A volume)
     198:  00:9D4D  CB 41               		bit	0,c				; If tone is off...
     199:  00:9D4F  C0                  		ret	nz				; ...return
     200:  00:9D50  22 40 F1            		ld	[AYREGS+0],hl			; copied in to AYREGS (channel A tone)
     201:  00:9D53  C9                  		ret					; Return
     202:  00:9D54                      .SETMIXER:	; --- Set PSG mixer value ---
     203:  00:9D54  4F                  		ld	c,a				; c:=OR mask
     204:  00:9D55  3A 47 F1            		ld	a,[AYREGS+7]			; a:=PSG mixer value
     205:  00:9D58  A2                  		and	d				; AND mask
     206:  00:9D59  B1                  		or	c				; OR mask
     207:  00:9D5A  32 47 F1            		ld	[AYREGS+7],a			; PSG mixer value updated
     208:  00:9D5D  3A 49 F2            		ld	a,[ayFX_VOLUME]			; a:=ayFX volume value
     209:  00:9D60  2A 46 F2            		ld	hl,[ayFX_TONE]			; ayFX tone value
     210:  00:9D63  C9                  		ret					; Return
