     134:  00:829F                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:829F  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:82A2                      		; --- Extract volume ---
