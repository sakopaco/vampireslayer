     183:  00:9779  0F                  		rrca					; Rotate right OR mask
     184:  00:977A  10 0D               		djnz	.CHK3				; Decrement and jump if channel was not 2
     185:  00:977C                      .PLAY_B:	; --- Play ayFX stream on channel B ---
