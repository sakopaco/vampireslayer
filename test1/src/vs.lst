     203.  00:5338                      pinta_obj_ayuda:
     204.  00:5338                      	;se recibe objeto desde fuera, por ejmplo LD IX, ayuda_oracion
     205.  00:5338                      
     206.  00:5338                      	;si A=1 dibujo on (si 0 off)
     207.  00:5338  B7                  	OR		 A
     208.  00:5339  C2 48 53            	JP		 NZ,.dibujooff
     209.  00:533C                      .dibujoon:	
     210.  00:533C  DD 6E 09            		LD		 L, (IX + ESTRUCTURA_AYUDA.tiles_ayudaoff)
