      22:  00:822C  3C                  		inc	a				; Starting channel (=1)
      23:  00:822D  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:8230                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:8230  3E FF               		ld	a,255				; Lowest ayFX priority
