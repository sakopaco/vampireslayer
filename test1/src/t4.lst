      82:  00:98C6  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:98C7  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:98C8  56                  		ld	d,[hl]				; de:=increment
      85:  00:98C9  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:98CA  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:98CD  AF                  		xor	a				; a:=0 (no errors)
      88:  00:98CE  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:98CF  D1                  		pop	de				; Retrieve de from stack
      90:  00:98D0  C1                  		pop	bc				; Retrieve bc from stack
