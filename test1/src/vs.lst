      17:  00:8229                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:8229                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:8229  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:822C  3E 01               		ld	a,1				; a:=0
      21:  00:822E  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:8231  3C                  		inc	a				; Starting channel (=1)
      23:  00:8232  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:8235                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:8235  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:8237  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
