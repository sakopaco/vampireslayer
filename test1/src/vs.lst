     134:  00:76F2                      .SETPOINTER:	; --- Update ayFX pointer ---
     135:  00:76F2  22 44 F2            		ld	[ayFX_POINTER],hl		; Update ayFX stream pointer
     136:  00:76F5                      		; --- Extract volume ---
     137:  00:76F5  79                  		ld	a,c				; a:=Control byte
     138:  00:76F6  E6 0F               		and	$0F				; lower nibble
     139:  00:76F8                      
     140:  00:76F8                      		IF ( AYFXRELATIVE == 1 )
     141:  00:76F8                    ~ 			; --- Fix the volume using PT3 Volume Table ---
