      23:  00:76C1  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:76C4                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:76C4  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:76C6  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
