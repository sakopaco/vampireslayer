      17:  00:8B15                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:8B15                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:8B15  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:8B18  3E 01               		ld	a,1				; a:=0
