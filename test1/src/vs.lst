      45:  00:7BDD  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7BDF                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7BDF                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7BDF  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7BE0  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
