      17:  00:7DBA                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7DBA                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7DBA  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7DBD  3E 01               		ld	a,1				; a:=0
      21:  00:7DBF  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
