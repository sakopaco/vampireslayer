      29:  00:76CA                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:76CA                      		; --- INPUT: A -> sound to be played ---
      31:  00:76CA                      		; ---        C -> sound priority     ---
      32:  00:76CA  C5                  		push	bc				; Store bc in stack
      33:  00:76CB  D5                  		push	de				; Store de in stack
      34:  00:76CC  E5                  		push	hl				; Store hl in stack
      35:  00:76CD                      		; --- Check if the index is in the bank ---
      36:  00:76CD  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:76CE  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:76D1  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:76D2  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:76D3  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
