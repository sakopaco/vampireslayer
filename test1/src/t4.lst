     142.  00:8F67                      ;;update_controllers_status
     143.  00:8F67                      ;;=====================================================	
     144.  00:8F67                      ;depends on the next rountine
     145.  00:8F67                      ;  returns
     146.  00:8F67                      ;  a - 0P2TRLDU
     147.  00:8F67                      
     148.  00:8F67                      update_controllers_status:
     149.  00:8F67  CD B2 8F                 call  check_8th_keyboard_row
     150.  00:8F6A                      [4]  rrc    a
     150.  00:8F6A  CB 0F CB 0F CB 0F CB 0F 
     151.  00:8F72  47                       ld    b,a    		;save for later  xxxTRDUL
     152.  00:8F73                      
     153.  00:8F73  E6 01                    and    #01    		;save L
     154.  00:8F75  CB 27                    sla    a
     155.  00:8F77  CB 27                    sla    a    		;now 00000L00
     156.  00:8F79  4F                       ld    c,a    		;save for later
     157.  00:8F7A                      
     158.  00:8F7A  78                       ld    a,b    		;again we've got xxxTRDUL. I am interested on leaving 000000DU on a
     159.  00:8F7B  CB 3F                    srl    a
     160.  00:8F7D  E6 03                    and    00000011b
     161.  00:8F7F                      						;we've got 000000DU on a
     162.  00:8F7F  B1                       or    c    		;now we've got 00000LDU
     163.  00:8F80  4F                       ld    c,a    		;save for later
     164.  00:8F81                      
     165.  00:8F81  78                       ld    a,b
     166.  00:8F82  E6 18                    and    00011000b 	;we've got 000TR000
     167.  00:8F84  B1                       or    c
     168.  00:8F85                           ;finally    000TRLDU
     169.  00:8F85                      
     170.  00:8F85                           ;and    00000100   ;preserve just
     171.  00:8F85  47                       ld    b,a    		;save for later
     172.  00:8F86  CD C0 8F                 call  check_4th_keyboard_row      ;check M & P
     173.  00:8F89  F5                       push  af
     174.  00:8F8A  E6 04                    and    00000100b	;filter just M
     175.  00:8F8C  87                       add    a
     176.  00:8F8D  87                       add    a
     177.  00:8F8E  87                       add    a           ;shift 3th to 6th bit
     178.  00:8F8F  B0                       or    b           	;merge with previous
     179.  00:8F90  47                       ld    b,a
     180.  00:8F91                      
     181.  00:8F91  CD 9E 8F                 call  read_joy_0
     182.  00:8F94  E6 3F                    and    00111111b  	;we want just 002TRLDU  from joystick    (2 is second trigger)
     183.  00:8F96  B0                       or    b      		;added 000TRLDU from keyboard
     184.  00:8F97  47                       ld    b,a      	;save for later
     185.  00:8F98  F1                       pop    af      	; let's check on P
     186.  00:8F99                      
     187.  00:8F99  E6 20                    and    00100000b
     188.  00:8F9B  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
     189.  00:8F9C  B0                       or    b
     190.  00:8F9D                      
     191.  00:8F9D  C9                       ret
     192.  00:8F9E                      
     193.  00:8F9E                      
     194.  00:8F9E                      ;ret
     195.  00:8F9E                      ;  a -            xx2TRLDU
     196.  00:8F9E                      read_joy_0:
     197.  00:8F9E  F3                         di
     198.  00:8F9F  3E 0F                      ld    a,15
     199.  00:8FA1  D3 A0                      out    [PSG_A],a
