     181.  00:52F5  B0                       or    b      		;added 000TRLDU from keyboard
     182.  00:52F6  47                       ld    b,a      	;save for later
     183.  00:52F7  F1                       pop    af      	; let's check on P
     184.  00:52F8                      
     185.  00:52F8  E6 20                    and    00100000b
     186.  00:52FA  87                       add    a      		;P status on 6th bit  ... so 0P2TRLDU
