      17:  00:7DD9                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:7DD9                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:7DD9  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:7DDC  3E 01               		ld	a,1				; a:=0
      21:  00:7DDE  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
      22:  00:7DE1  3C                  		inc	a				; Starting channel (=1)
      23:  00:7DE2  32 4A F2            		ld	[ayFX_CHANNEL],a		; Updated
      24:  00:7DE5                      ayFX_END:	; --- End of an ayFX stream ---
      25:  00:7DE5  3E FF               		ld	a,255				; Lowest ayFX priority
      26:  00:7DE7  32 43 F2            		ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      27:  00:7DEA  C9                  		ret					; Return
      28:  00:7DEB                      
      29:  00:7DEB                      ayFX_INIT:	; ---     INIT A NEW ayFX STREAM     ---
      30:  00:7DEB                      		; --- INPUT: A -> sound to be played ---
      31:  00:7DEB                      		; ---        C -> sound priority     ---
      32:  00:7DEB  C5                  		push	bc				; Store bc in stack
      33:  00:7DEC  D5                  		push	de				; Store de in stack
      34:  00:7DED  E5                  		push	hl				; Store hl in stack
      35:  00:7DEE                      		; --- Check if the index is in the bank ---
      36:  00:7DEE  47                  		ld	b,a				; b:=a (new ayFX stream index)
      37:  00:7DEF  2A 41 F2            		ld	hl,[ayFX_BANK]			; Current ayFX BANK
      38:  00:7DF2  7E                  		ld	a,[hl]				; Number of samples in the bank
      39:  00:7DF3  B7                  		or	a				; If zero (means 256 samples)...
      40:  00:7DF4  28 06               		jr	z,.CHECK_PRI			; ...goto .CHECK_PRI
      41:  00:7DF6                      		; The bank has less than 256 samples
      42:  00:7DF6  78                  		ld	a,b				; a:=b (new ayFX stream index)
      43:  00:7DF7  BE                  		cp	[hl]				; If new index is not in the bank...
      44:  00:7DF8  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      45:  00:7DFA  30 21               		jr	nc,.INIT_END			; ...we can't init it
      46:  00:7DFC                      .CHECK_PRI:	; --- Check if the new priority is lower than the current one ---
      47:  00:7DFC                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      48:  00:7DFC  78                  		ld	a,b				; a:=b (new ayFX stream index)
      49:  00:7DFD  3A 43 F2            		ld	a,[ayFX_PRIORITY]		; a:=Current ayFX stream priority
      50:  00:7E00  B9                  		cp	c				; If new ayFX stream priority is lower than current one...
      51:  00:7E01  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      52:  00:7E03  38 18               		jr	c,.INIT_END			; ...we don't start the new ayFX stream
      53:  00:7E05                      		; --- Set new priority ---
      54:  00:7E05  79                  		ld	a,c				; a:=New priority
      55:  00:7E06  E6 0F               		and	$0F				; We mask the priority
      56:  00:7E08  32 43 F2            		ld	[ayFX_PRIORITY],a		; new ayFX stream priority saved in RAM
      57:  00:7E0B                      
      58:  00:7E0B                      		IF ( AYFXRELATIVE == 1 )
      59:  00:7E0B                    ~ 			; --- Volume adjust using PT3 volume table ---
      60:  00:7E0B                    ~ 			ld	c,a				; c:=New priority (fixed)
      61:  00:7E0B                    ~ 			ld	a,15				; a:=15
      62:  00:7E0B                    ~ 			sub	c				; a:=15-New priority = relative volume
      63:  00:7E0B                    ~ 			jr	z,.INIT_NOSOUND		; If priority is 15 -> no sound output (volume is zero)
      64:  00:7E0B                    ~ 			add	a,a				; a:=a*2
      65:  00:7E0B                    ~ 			add	a,a				; a:=a*4
      66:  00:7E0B                    ~ 			add	a,a				; a:=a*8
      67:  00:7E0B                    ~ 			add	a,a				; a:=a*16
      68:  00:7E0B                    ~ 			ld	e,a				; e:=a
      69:  00:7E0B                    ~ 			ld	d,0				; de:=a
      70:  00:7E0B                    ~ 			ld	hl,VT_				; hl:=PT3 volume table
      71:  00:7E0B                    ~ 			add	hl,de				; hl is a pointer to the relative volume table
      72:  00:7E0B                    ~ 			ld	[ayFX_VT],hl			; Save pointer
      73:  00:7E0B                    ~ 		ENDIF
      74:  00:7E0B                      
      75:  00:7E0B                      		; --- Calculate the pointer to the new ayFX stream ---
      76:  00:7E0B  ED 5B 41 F2         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      77:  00:7E0F  13                  		inc	de				; de points to the increments table of the bank
      78:  00:7E10  68                  		ld	l,b				; l:=b (new ayFX stream index)
      79:  00:7E11  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      80:  00:7E13  29                  		add	hl,hl				; hl:=hl*2
      81:  00:7E14  19                  		add	hl,de				; hl:=hl+de (hl points to the correct increment)
      82:  00:7E15  5E                  		ld	e,[hl]				; e:=lower byte of the increment
      83:  00:7E16  23                  		inc	hl				; hl points to the higher byte of the correct increment
      84:  00:7E17  56                  		ld	d,[hl]				; de:=increment
      85:  00:7E18  19                  		add	hl,de				; hl:=hl+de (hl points to the new ayFX stream)
      86:  00:7E19  22 44 F2            		ld	[ayFX_POINTER],hl		; Pointer saved in RAM
      87:  00:7E1C  AF                  		xor	a				; a:=0 (no errors)
      88:  00:7E1D  E1                  .INIT_END:	pop	hl				; Retrieve hl from stack
      89:  00:7E1E  D1                  		pop	de				; Retrieve de from stack
      90:  00:7E1F  C1                  		pop	bc				; Retrieve bc from stack
      91:  00:7E20  C9                  		ret					; Return
      92:  00:7E21                      
      93:  00:7E21                      		IF ( AYFXRELATIVE == 1 )
      94:  00:7E21                    ~ .INIT_NOSOUND:	; --- Init a sample with relative volume zero -> no sound output ---
      95:  00:7E21                    ~ 			ld	a,255				; Lowest ayFX priority
      96:  00:7E21                    ~ 			ld	[ayFX_PRIORITY],a		; Priority saved (not playing ayFX stream)
      97:  00:7E21                    ~ 			jr	.INIT_END			; Jumps to .INIT_END
      98:  00:7E21                    ~ 		ENDIF
