      17:  00:76B8                      ayFX_SETUP:	; ---          ayFX replayer setup          ---
      18:  00:76B8                      		; --- INPUT: HL -> pointer to the ayFX bank ---
      19:  00:76B8  22 41 F2            		ld	[ayFX_BANK],hl			; Current ayFX bank
      20:  00:76BB  3E 01               		ld	a,1				; a:=0
      21:  00:76BD  32 40 F2            		ld	[ayFX_MODE],a			; Initial mode: fixed channel
